@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process that occurs."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event, consistent with a description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:Event
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a situation to the event it observes or interprets."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:HMISystem a owl:Class ;
    rdfs:label "HMI System"@en ;
    rdfs:comment "A system that provides human-machine interfaces and interactions."@en .

ahmi:DialogDesign a owl:Class ;
    rdfs:label "Dialog Design"@en ;
    rdfs:comment "The design of the dialog and interaction between the system and the user."@en ;
    rdfs:subClassOf ahmi:HMIComponent .

ahmi:HMIComponent a owl:Class ;
    rdfs:label "HMI Component"@en ;
    rdfs:comment "A component of an HMI system."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:isPartOf ;
        owl:someValuesFrom ahmi:HMISystem
    ] .

ahmi:isPartOf a owl:ObjectProperty ;
    rdfs:label "is part of"@en ;
    rdfs:comment "Indicates that an entity is a part of another entity."@en ;
    rdfs:domain ahmi:Entity ;
    rdfs:range ahmi:Entity .

ahmi:Entity a owl:Class ;
    rdfs:label "Entity"@en ;
    rdfs:comment "Any physical, social, or mental object, event, or quality."@en .

ahmi:MessageStructure a owl:Class ;
    rdfs:label "Message Structure"@en ;
    rdfs:comment "The structure and organization of messages presented to the user."@en ;
    rdfs:subClassOf ahmi:DialogDesign .

ahmi:communicatesStatus a owl:ObjectProperty ;
    rdfs:label "communicates status"@en ;
    rdfs:comment "Indicates that a dialog design element communicates the status of an intervention or event."@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:InterventionStatus .

ahmi:InterventionStatus a owl:Class ;
    rdfs:label "Intervention Status"@en ;
    rdfs:comment "The status or state of an intervention or system action."@en .

ahmi:communicatesRationale a owl:ObjectProperty ;
    rdfs:label "communicates rationale"@en ;
    rdfs:comment "Indicates that a dialog design element communicates the rationale or reason for an intervention or event."@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:InterventionRationale .

ahmi:InterventionRationale a owl:Class ;
    rdfs:label "Intervention Rationale"@en ;
    rdfs:comment "The rationale, reason, or justification for an intervention or system action."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The state or condition of the driver, such as emotional, cognitive, or behavioral states."@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A specific emotional state experienced by the driver."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:Confusion a owl:Class ;
    rdfs:label "Confusion"@en ;
    rdfs:comment "The state of being confused or perplexed."@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:SpeedViolationTendency a owl:Class ;
    rdfs:label "Speed Violation Tendency"@en ;
    rdfs:comment "A tendency or inclination to violate speed limits or drive at excessive speeds."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:adaptsFor a owl:ObjectProperty ;
    rdfs:label "adapts for"@en ;
    rdfs:comment "Indicates that a dialog design element adapts or adjusts for a specific driver state."@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:DriverState .