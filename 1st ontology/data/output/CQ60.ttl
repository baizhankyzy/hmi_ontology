@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A state of the driver, such as cognitive, emotional, or physiological state."@en .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The cognitive state of the driver, such as mental workload, distraction, or attention."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DistractionState a owl:Class ;
    rdfs:label "Distraction State"@en ;
    rdfs:comment "A state of the driver being distracted from the driving task."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A factor related to the personality of the driver, such as Big Five traits or aggressive driving tendency."@en .

ahmi:DetectionMethodology a owl:Class ;
    rdfs:label "Detection Methodology"@en ;
    rdfs:comment "A methodology used to detect or measure the driver's state."@en .

ahmi:ObjectiveDetectionMethodology a owl:Class ;
    rdfs:label "Objective Detection Methodology"@en ;
    rdfs:comment "A detection methodology that uses objective measures, such as physiological sensors."@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:SubjectiveDetectionMethodology a owl:Class ;
    rdfs:label "Subjective Detection Methodology"@en ;
    rdfs:comment "A detection methodology that uses subjective measures, such as self-report questionnaires."@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:Questionnaire a owl:Class ;
    rdfs:label "Questionnaire"@en ;
    rdfs:comment "A self-report questionnaire used to measure subjective aspects of the driver's state."@en ;
    rdfs:subClassOf ahmi:SubjectiveDetectionMethodology .

ahmi:PhysiologicalMeasurement a owl:Class ;
    rdfs:label "Physiological Measurement"@en ;
    rdfs:comment "A measurement of the driver's physiological state, such as electrodermal activity, heart rate, or brain activity."@en ;
    rdfs:subClassOf ahmi:ObjectiveDetectionMethodology .

ahmi:ElectrodermalActivity a owl:Class ;
    rdfs:label "Electrodermal Activity"@en ;
    rdfs:comment "A measurement of the driver's electrodermal activity, such as skin conductance."@en ;
    rdfs:subClassOf ahmi:PhysiologicalMeasurement .

ahmi:HeartRateMonitoring a owl:Class ;
    rdfs:label "Heart Rate Monitoring"@en ;
    rdfs:comment "A measurement of the driver's heart rate, such as through electrocardiography (ECG)."@en ;
    rdfs:subClassOf ahmi:PhysiologicalMeasurement .

ahmi:BrainActivity a owl:Class ;
    rdfs:label "Brain Activity"@en ;
    rdfs:comment "A measurement of the driver's brain activity, such as through electroencephalography (EEG)."@en ;
    rdfs:subClassOf ahmi:PhysiologicalMeasurement .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A modality through which feedback is provided to the driver, such as tactile, audio, or visual."@en .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Feedback provided through the sense of touch."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "Feedback provided through sound."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual displays."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AdaptationStrategy a owl:Class ;
    rdfs:label "Adaptation Strategy"@en ;
    rdfs:comment "A strategy for adapting the vehicle system to the driver's state."@en .

ahmi:TrustCalibration a owl:Class ;
    rdfs:label "Trust Calibration"@en ;
    rdfs:comment "A strategy for calibrating the driver's trust in the vehicle system."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:RegulationStrategy a owl:Class ;
    rdfs:label "Regulation Strategy"@en ;
    rdfs:comment "A strategy for regulating the vehicle system's behavior based on the driver's state."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:MultimodalIntegration a owl:Class ;
    rdfs:label "Multimodal Integration"@en ;
    rdfs:comment "The integration of multiple feedback modalities in the vehicle system."@en .

ahmi:CrossModalCongruence a owl:Class ;
    rdfs:label "Cross-Modal Congruence"@en ;
    rdfs:comment "The principle of ensuring congruence between different feedback modalities in a multimodal system."@en ;
    rdfs:subClassOf ahmi:MultimodalIntegration .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a situation to the driver state observed in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DriverState .

ahmi:hasPersonalityFactor a owl:ObjectProperty ;
    rdfs:label "has personality factor"@en ;
    rdfs:comment "Relates a driver to their personality factors."@en ;
    rdfs:domain dul:Entity ;
    rdfs:range ahmi:PersonalityFactor .

ahmi:usesDetectionMethodology a owl:ObjectProperty ;
    rdfs:label "uses detection methodology"@en ;
    rdfs:comment "Relates a situation to the detection methodology used to observe the driver's state in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DetectionMethodology .

ahmi:hasQuestionnaire a owl:ObjectProperty ;
    rdfs:label "has questionnaire"@en ;
    rdfs:comment "Relates a subjective detection methodology to the questionnaire used."@en ;
    rdfs:domain ahmi:SubjectiveDetectionMethodology ;
    rdfs:range ahmi:Questionnaire .

ahmi:hasPhysiologicalMeasurement a owl:ObjectProperty ;
    rdfs:label "has physiological measurement"@en ;
    rdfs:comment "Relates an objective detection methodology to the physiological measurement used."@en ;
    rdfs:domain ahmi:ObjectiveDetectionMethodology ;
    rdfs:range ahmi:PhysiologicalMeasurement .

ahmi:providesFeedback a owl:ObjectProperty ;
    rdfs:label "provides feedback"@en ;
    rdfs:comment "Relates a situation to the feedback modality used in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:FeedbackModality .

ahmi:employsAdaptationStrategy a owl:ObjectProperty ;
    rdfs:label "employs adaptation strategy"@en ;
    rdfs:comment "Relates a situation to the adaptation strategy employed in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:AdaptationStrategy .

ahmi:hasCongruence a owl:ObjectProperty ;
    rdfs:label "has congruence"@en ;
    rdfs:comment "Relates a multimodal integration to the cross-modal congruence principle applied."@en ;
    rdfs:domain ahmi:MultimodalIntegration ;
    rdfs:range ahmi:CrossModalCongruence .

ahmi:hasTimestamp a owl:DatatypeProperty ;
    rdfs:label "has timestamp"@en ;
    rdfs:comment "The timestamp associated with a measurement or event."@en ;
    rdfs:range xsd:dateTime .

ahmi:hasValue a owl:DatatypeProperty ;
    rdfs:label "has value"@en ;
    rdfs:comment "The value associated with a measurement or rating."@en ;
    rdfs:range xsd:float .

ahmi:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description"@en ;
    rdfs:comment "A textual description associated with an entity."@en ;
    rdfs:range xsd:string .