@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A state or condition experienced by a driver." .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A distinct emotional state experienced by a driver." ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:Confusion a owl:Class ;
    rdfs:label "Confusion"@en ;
    rdfs:comment "The state of being confused or perplexed." ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:SpeedViolationTendency a owl:Class ;
    rdfs:label "Speed Violation Tendency"@en ;
    rdfs:comment "A tendency or inclination to exceed speed limits." ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:MessageStructure a owl:Class ;
    rdfs:label "Message Structure"@en ;
    rdfs:comment "The structure or organization of a message presented to the driver." .

ahmi:DialogDesign a owl:Class ;
    rdfs:label "Dialog Design"@en ;
    rdfs:comment "The design of the dialog or interaction between the system and the driver." .

ahmi:Adaptation a owl:Class ;
    rdfs:label "Adaptation"@en ;
    rdfs:comment "A modification or adjustment made to the system's behavior or presentation." .

ahmi:MessageStructureAdaptation a owl:Class ;
    rdfs:label "Message Structure Adaptation"@en ;
    rdfs:comment "An adaptation made to the structure or organization of a message presented to the driver." ;
    rdfs:subClassOf ahmi:Adaptation .

ahmi:DialogDesignAdaptation a owl:Class ;
    rdfs:label "Dialog Design Adaptation"@en ;
    rdfs:comment "An adaptation made to the design of the dialog or interaction between the system and the driver." ;
    rdfs:subClassOf ahmi:Adaptation .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process or occurrence." .

ahmi:LaneDepartureEvent a owl:Class ;
    rdfs:label "Lane Departure Event"@en ;
    rdfs:comment "An event where the vehicle departs from its intended lane." ;
    rdfs:subClassOf ahmi:Event .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event based on a particular context or description." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events." .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies." ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a situation to the driver state observed in that situation." ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:DriverState .

ahmi:hasMessageStructureAdaptation a owl:ObjectProperty ;
    rdfs:label "has message structure adaptation"@en ;
    rdfs:comment "Relates a situation to the message structure adaptation applied in that situation." ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:MessageStructureAdaptation .

ahmi:hasDialogDesignAdaptation a owl:ObjectProperty ;
    rdfs:label "has dialog design adaptation"@en ;
    rdfs:comment "Relates a situation to the dialog design adaptation applied in that situation." ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:DialogDesignAdaptation .

ahmi:observesEvent a owl:ObjectProperty ;
    rdfs:label "observes event"@en ;
    rdfs:comment "Relates a situation to the event it observes or interprets." ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:isAppropriateFor a owl:ObjectProperty ;
    rdfs:label "is appropriate for"@en ;
    rdfs:comment "Relates an adaptation to the driver state for which it is appropriate." ;
    rdfs:domain ahmi:Adaptation ;
    rdfs:range ahmi:DriverState .

ahmi:hasMessageStructure a owl:ObjectProperty ;
    rdfs:label "has message structure"@en ;
    rdfs:comment "Relates an adaptation to the message structure it applies." ;
    rdfs:domain ahmi:MessageStructureAdaptation ;
    rdfs:range ahmi:MessageStructure .

ahmi:hasDialogDesign a owl:ObjectProperty ;
    rdfs:label "has dialog design"@en ;
    rdfs:comment "Relates an adaptation to the dialog design it applies." ;
    rdfs:domain ahmi:DialogDesignAdaptation ;
    rdfs:range ahmi:DialogDesign .