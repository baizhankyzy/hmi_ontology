@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:PostureState a owl:Class ;
    rdfs:label "Posture State"@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:HeadPosition a owl:Class ;
    rdfs:label "Head Position"@en ;
    rdfs:subClassOf ahmi:PostureState .

ahmi:EyeTracking a owl:Class ;
    rdfs:label "Eye Tracking"@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:BlinkDuration a owl:Class ;
    rdfs:label "Blink Duration"@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:Drowsiness a owl:Class ;
    rdfs:label "Drowsiness"@en ;
    rdfs:subClassOf ahmi:InattentionState .

ahmi:Fatigue a owl:Class ;
    rdfs:label "Fatigue"@en ;
    rdfs:subClassOf ahmi:InattentionState .

ahmi:InattentionState a owl:Class ;
    rdfs:label "Inattention State"@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:DisplayType a owl:Class ;
    rdfs:label "Display Type"@en ;
    rdfs:subClassOf ahmi:VisualFeedback .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:subClassOf ahmi:VisualFeedback .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:HapticAlert a owl:Class ;
    rdfs:label "Haptic Alert"@en ;
    rdfs:subClassOf ahmi:TactileFeedback .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AuditoryDisplay a owl:Class ;
    rdfs:label "Auditory Display"@en ;
    rdfs:subClassOf ahmi:AudioFeedback .

ahmi:DialogDesign a owl:Class ;
    rdfs:label "Dialog Design"@en .

ahmi:MessageStructure a owl:Class ;
    rdfs:label "Message Structure"@en ;
    rdfs:subClassOf ahmi:DialogDesign .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en .

ahmi:ForceFeedback a owl:Class ;
    rdfs:label "Force Feedback"@en ;
    rdfs:subClassOf ahmi:TactileFeedback .

ahmi:AdaptivityParameter a owl:Class ;
    rdfs:label "Adaptivity Parameter"@en .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en .

ahmi:hasPostureState a owl:ObjectProperty ;
    rdfs:label "has Posture State"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:PostureState .

ahmi:hasHeadPosition a owl:ObjectProperty ;
    rdfs:label "has Head Position"@en ;
    rdfs:domain ahmi:PostureState ;
    rdfs:range ahmi:HeadPosition .

ahmi:hasEyeTracking a owl:ObjectProperty ;
    rdfs:label "has Eye Tracking"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:EyeTracking .

ahmi:hasBlinkDuration a owl:ObjectProperty ;
    rdfs:label "has Blink Duration"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:BlinkDuration .

ahmi:indicatesInattentionState a owl:ObjectProperty ;
    rdfs:label "indicates Inattention State"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:InattentionState .

ahmi:hasCognitiveState a owl:ObjectProperty ;
    rdfs:label "has Cognitive State"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:CognitiveState .

ahmi:triggersVisualFeedback a owl:ObjectProperty ;
    rdfs:label "triggers Visual Feedback"@en ;
    rdfs:domain ahmi:InattentionState ;
    rdfs:range ahmi:VisualFeedback .

ahmi:modifiesDisplayType a owl:ObjectProperty ;
    rdfs:label "modifies Display Type"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:DisplayType .

ahmi:modifiesVisualElement a owl:ObjectProperty ;
    rdfs:label "modifies Visual Element"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:VisualElement .

ahmi:triggersTactileFeedback a owl:ObjectProperty ;
    rdfs:label "triggers Tactile Feedback"@en ;
    rdfs:domain ahmi:InattentionState ;
    rdfs:range ahmi:TactileFeedback .

ahmi:activatesHapticAlert a owl:ObjectProperty ;
    rdfs:label "activates Haptic Alert"@en ;
    rdfs:domain ahmi:TactileFeedback ;
    rdfs:range ahmi:HapticAlert .

ahmi:triggersAudioFeedback a owl:ObjectProperty ;
    rdfs:label "triggers Audio Feedback"@en ;
    rdfs:domain ahmi:InattentionState ;
    rdfs:range ahmi:AudioFeedback .

ahmi:modifiesAuditoryDisplay a owl:ObjectProperty ;
    rdfs:label "modifies Auditory Display"@en ;
    rdfs:domain ahmi:AudioFeedback ;
    rdfs:range ahmi:AuditoryDisplay .

ahmi:hasDialogDesign a owl:ObjectProperty ;
    rdfs:label "has Dialog Design"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:DialogDesign .

ahmi:hasMessageStructure a owl:ObjectProperty ;
    rdfs:label "has Message Structure"@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:MessageStructure .

ahmi:deviatesFromDrivingBehavior a owl:ObjectProperty ;
    rdfs:label "deviates from Driving Behavior"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DrivingBehavior .

ahmi:triggersForceFeedback a owl:ObjectProperty ;
    rdfs:label "triggers Force Feedback"@en ;
    rdfs:domain ahmi:InattentionState ;
    rdfs:range ahmi:ForceFeedback .

ahmi:hasAdaptivityParameter a owl:ObjectProperty ;
    rdfs:label "has Adaptivity Parameter"@en ;
    rdfs:domain ahmi:InattentionState ;
    rdfs:range ahmi:AdaptivityParameter .

dul:Situation a owl:Class ;
    rdfs:label "Situation"@en .

dul:Description a owl:Class ;
    rdfs:label "Description"@en .

dul:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range dul:Description .

ahmi:InattentionSituation a owl:Class ;
    rdfs:label "Inattention Situation"@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:InattentionDescription
    ] .

ahmi:InattentionDescription a owl:Class ;
    rdfs:label "Inattention Description"@en ;
    rdfs:subClassOf dul:Description .

ahmi:PostureStateSituation a owl:Class ;
    rdfs:label "Posture State Situation"@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:PostureStateDescription
    ] .

ahmi:PostureStateDescription a owl:Class ;
    rdfs:label "Posture State Description"@en ;
    rdfs:subClassOf dul:Description .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:domain ahmi:InattentionSituation ;
    rdfs:range ahmi:PostureStateSituation .

ahmi:isParameterFor a owl:ObjectProperty ;
    rdfs:label "is parameter for"@en ;
    rdfs:domain ahmi:AdaptivityParameter ;
    rdfs:range ahmi:InattentionDescription .