###  http://example.org/ahmi#hasAdaptation
ns2:hasAdaptation rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf ns1:isParameterFor ;
                  rdfs:domain ns2:DriverStateAdaptation ;
                  rdfs:range ns2:Adaptation ;
                  rdfs:label "has adaptation"@en .


###  http://example.org/ahmi#hasAdaptationStrategy
ns2:hasAdaptationStrategy rdf:type owl:ObjectProperty ;
                          rdfs:domain ns2:ADASEvaluation ,
                                      ns2:Driver ,
                                      ns2:Situation ,
                                      ns2:SystemComponent ,
                                      ns2:TripDuration ,
                                      ns2:Vehicle ;
                          rdfs:range ns2:AdaptationStrategy ,
                                     ns2:HMIAdaptation ;
                          rdfs:comment "Relates a driver state adaptation situation to the adaptation strategy employed."@en ,
                                       "Relates a driving situation to the strategies used by the vehicle system to adapt to the driver's state."@en ,
                                       "Relates a situation to the adaptation strategy it describes."@en ,
                                       "Relates a situation to the adaptation strategy to be applied"@en ,
                                       "Relates a system component to the adaptation strategies it employs."@en ,
                                       "Relates a vehicle system or situation to the adaptations made."@en ,
                                       "Relates an ADAS evaluation to the adaptation strategies being assessed or employed."@en ;
                          rdfs:label "has adaptation"@en ,
                                     "has adaptation strategy"@en .


