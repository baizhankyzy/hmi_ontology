@prefix : <http://example.org/adaptive-hmi#> .
@prefix ns1: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix ns2: <http://example.org/ahmi#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/adaptive-hmi#> .

<http://example.org/adaptive-hmi> rdf:type owl:Ontology ;
                                   rdfs:comment "A human-machine interface that adapts its behavior based on driver state, vehicle context, and environmental factors."@en ,
                                                "A human-machine interface that adapts its feedback and interaction modalities based on driver state, vehicle context, and environmental factors."@en ,
                                                "A human-machine interface that adapts its feedback modalities based on driver state and context."@en ,
                                                "An ontology for describing adaptive human-machine interfaces with a focus on driver state detection and multimodal feedback"@en ;
                                   rdfs:label "Adaptive HMI"@en ,
                                              "Adaptive Human-Machine Interface Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://example.org/ahmi#hasAirQuality
ns2:hasAirQuality rdf:type owl:AnnotationProperty .


###  http://example.org/ahmi#hasArousalLevel
ns2:hasArousalLevel rdf:type owl:AnnotationProperty ;
                    rdfs:subPropertyOf ns1:hasParameter ;
                    rdfs:range ns2:ArousalLevel .


###  http://example.org/ahmi#hasClimateControlResponse
ns2:hasClimateControlResponse rdf:type owl:AnnotationProperty .


###  http://example.org/ahmi#triggersClimateControlResponse
ns2:triggersClimateControlResponse rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameter
ns1:hasParameter rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies
ns1:satisfies rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#hasValue
owl:hasValue rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/ahmi#ADASAdaptation
ns2:ADASAdaptation rdf:type owl:ObjectProperty ;
                   rdfs:domain ns2:HMIAdaptation ;
                   rdfs:range ns2:ADASAdaptation .


###  http://example.org/ahmi#AdaptationAction
ns2:AdaptationAction rdf:type owl:ObjectProperty ;
                     rdfs:domain ns2:AdaptationRule ;
                     rdfs:range ns2:AdaptationAction .


###  http://example.org/ahmi#AdaptationRule
ns2:AdaptationRule rdf:type owl:ObjectProperty ;
                   rdfs:domain ns2:AdaptiveHMI ;
                   rdfs:range ns2:AdaptationRule .


###  http://example.org/ahmi#AdaptivityParameter
ns2:AdaptivityParameter rdf:type owl:ObjectProperty ;
                        rdfs:domain ns2:HMISystem ,
                                    ns2:InattentionState ,
                                    ns2:Vehicle ,
                                    ns1:Situation ;
                        rdfs:range ns2:AdaptivityParameter .


###  http://example.org/ahmi#AirQualityClassification
ns2:AirQualityClassification rdf:type owl:ObjectProperty ;
                             rdfs:domain ns2:AirQualityClassification ;
                             rdfs:range ns2:AirQualityClassification .


###  http://example.org/ahmi#AlertTiming
ns2:AlertTiming rdf:type owl:ObjectProperty ;
                rdfs:domain ns2:Situation ;
                rdfs:range ns2:AlertTiming .


###  http://example.org/ahmi#AttentionLevel
ns2:AttentionLevel rdf:type owl:ObjectProperty ;
                   rdfs:domain ns2:CognitiveState ,
                               ns2:Driver ;
                   rdfs:range ns2:AttentionLevel ,
                              ns2:Driver .


###  http://example.org/ahmi#BodyPosture
ns2:BodyPosture rdf:type owl:ObjectProperty ;
                rdfs:domain ns2:Description ;
                rdfs:range ns2:BodyPosture .


###  http://example.org/ahmi#BrightnessParameter
ns2:BrightnessParameter rdf:type owl:ObjectProperty ;
                        rdfs:domain ns2:DaytimeCondition ;
                        rdfs:range ns2:DisplayBrightness .


###  http://example.org/ahmi#ClimateControl
ns2:ClimateControl rdf:type owl:ObjectProperty ;
                   rdfs:domain ns2:AirQualityClassification ,
                               ns2:Situation ;
                   rdfs:range ns2:ClimateControl .


###  http://example.org/ahmi#Concept
ns2:Concept rdf:type owl:ObjectProperty ;
            rdfs:range ns2:Concept ,
                       ns2:Description .


###  http://example.org/ahmi#ContextualAnalysis
ns2:ContextualAnalysis rdf:type owl:ObjectProperty ;
                       rdfs:domain ns2:Driver ;
                       rdfs:range ns2:ContextualAnalysis .


###  http://example.org/ahmi#ContextualFactor
ns2:ContextualFactor rdf:type owl:ObjectProperty ;
                     rdfs:domain ns2:ContextualAnalysis ,
                                 ns2:Situation ;
                     rdfs:range ns2:ContextualFactor ,
                                ns1:Entity .


###  http://example.org/ahmi#DetectionMethod
ns2:DetectionMethod rdf:type owl:ObjectProperty ;
                    rdfs:domain ns2:Driver ,
                                ns2:DrivingBehaviour ,
                                ns2:InattentionState ,
                                ns2:Situation ,
                                ns2:Vehicle ,
                                ns1:Situation ;
                    rdfs:range ns2:DetectionMethod ,
                               ns2:DetectionMethodology .


###  http://example.org/ahmi#DialogStrategy
ns2:DialogStrategy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf ns2:hasComponent ;
                   rdfs:domain ns2:DialogDesign ;
                   rdfs:range ns2:DialogStrategy .


###  http://example.org/ahmi#EEGSignal
ns2:EEGSignal rdf:type owl:ObjectProperty ;
              rdfs:domain ns2:CognitiveState ;
              rdfs:range ns2:EEGSignal .


###  http://example.org/ahmi#EnvironmentContext
ns2:EnvironmentContext rdf:type owl:ObjectProperty ;
                       rdfs:domain ns2:AdaptationRule ,
                                   ns2:AdaptiveHMI ;
                       rdfs:range ns2:EnvironmentContext .


###  http://example.org/ahmi#EnvironmentFactor
ns2:EnvironmentFactor rdf:type owl:ObjectProperty ;
                      rdfs:domain ns2:ContextualAnalysis ,
                                  ns2:Driver ,
                                  ns2:Situation ;
                      rdfs:range ns2:EnvironmentFactor .


###  http://example.org/ahmi#EnvironmentalFactor
ns2:EnvironmentalFactor rdf:type owl:ObjectProperty ;
                        rdfs:domain ns2:Situation ;
                        rdfs:range ns2:EnvironmentalFactor .


###  http://example.org/ahmi#Event
ns2:Event rdf:type owl:ObjectProperty ;
          rdfs:domain ns2:Situation ;
          rdfs:range ns2:Event .


###  http://example.org/ahmi#FeedbackModality
ns2:FeedbackModality rdf:type owl:ObjectProperty ;
                     rdfs:domain ns2:Driver ,
                                 ns2:HMI ,
                                 ns2:Situation ,
                                 ns2:SystemComponent ,
                                 ns2:TrustCalibrationMechanism ,
                                 ns2:Vehicle ,
                                 ns1:Situation ,
                                 owl:Thing ;
                     rdfs:range ns2:FeedbackModality .


###  http://example.org/ahmi#ForceParameter
ns2:ForceParameter rdf:type owl:ObjectProperty ;
                   rdfs:domain ns2:ForceFeedback ;
                   rdfs:range ns2:ForceParameter .


###  http://example.org/ahmi#HeadPosition
ns2:HeadPosition rdf:type owl:ObjectProperty ;
                 rdfs:domain ns2:Driver ,
                             ns2:DrivingBehaviour ,
                             ns2:PostureState ;
                 rdfs:range ns2:HeadPosition .


###  http://example.org/ahmi#InputModality
ns2:InputModality rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf ns2:hasComponent ;
                  rdfs:domain ns2:DialogDesign ;
                  rdfs:range ns2:InputModality .


###  http://example.org/ahmi#InteractionTiming
ns2:InteractionTiming rdf:type owl:ObjectProperty ;
                      rdfs:domain ns2:Situation ;
                      rdfs:range ns2:InteractionTiming .


###  http://example.org/ahmi#MeasurementMetric
ns2:MeasurementMetric rdf:type owl:ObjectProperty ;
                      rdfs:domain ns2:DetectionMethodology ;
                      rdfs:range ns2:MeasurementMetric .


###  http://example.org/ahmi#MetricChange
ns2:MetricChange rdf:type owl:ObjectProperty ;
                 rdfs:domain ns2:Situation ;
                 rdfs:range ns2:MetricChange .


###  http://example.org/ahmi#MultimodalStrategy
ns2:MultimodalStrategy rdf:type owl:ObjectProperty ;
                       rdfs:domain ns2:MultimodalIntegrationProtocol ;
                       rdfs:range ns2:MultimodalStrategy .


###  http://example.org/ahmi#OutputModality
ns2:OutputModality rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf ns2:hasComponent ;
                   rdfs:domain ns2:DialogDesign ;
                   rdfs:range ns2:OutputModality .


###  http://example.org/ahmi#PersonalityFactor
ns2:PersonalityFactor rdf:type owl:ObjectProperty ;
                      rdfs:domain ns2:Driver ,
                                  ns2:Questionnaire ,
                                  ns2:Situation ,
                                  ns1:Entity ;
                      rdfs:range ns2:PersonalityFactor .


###  http://example.org/ahmi#PhysiologicalMeasurement
ns2:PhysiologicalMeasurement rdf:type owl:ObjectProperty ;
                             rdfs:domain ns2:DetectionMethodology ;
                             rdfs:range ns2:PhysiologicalMeasurement .


###  http://example.org/ahmi#PostureTransition
ns2:PostureTransition rdf:type owl:ObjectProperty ;
                      rdfs:domain ns2:PostureState ;
                      rdfs:range ns2:PostureTransition .


###  http://example.org/ahmi#Rating
ns2:Rating rdf:type owl:ObjectProperty ;
           rdfs:domain ns2:FeedbackModality ;
           rdfs:range ns2:Rating .


###  http://example.org/ahmi#RoadCondition
ns2:RoadCondition rdf:type owl:ObjectProperty ;
                  rdfs:domain ns2:BehaviorAnalysis ,
                              ns2:Driver ,
                              ns2:Situation ;
                  rdfs:range ns2:RoadCondition .


###  http://example.org/ahmi#SensorData
ns2:SensorData rdf:type owl:ObjectProperty ;
               rdfs:domain ns2:Driver ;
               rdfs:range ns2:SensorData .


###  http://example.org/ahmi#SteeringBehavior
ns2:SteeringBehavior rdf:type owl:ObjectProperty ;
                     rdfs:domain ns2:Event ,
                                 ns2:Situation ;
                     rdfs:range ns2:SteeringBehavior .


###  http://example.org/ahmi#TactileCharacteristic
ns2:TactileCharacteristic rdf:type owl:ObjectProperty ;
                          rdfs:domain ns2:Situation ;
                          rdfs:range ns2:TactileCharacteristic .


###  http://example.org/ahmi#VisualComplexity
ns2:VisualComplexity rdf:type owl:ObjectProperty ;
                     rdfs:domain ns2:DialogDesign ;
                     rdfs:range ns2:VisualComplexity .


###  http://example.org/ahmi#WeatherTransition
ns2:WeatherTransition rdf:type owl:ObjectProperty ;
                      rdfs:domain ns2:EnvironmentalCondition ;
                      rdfs:range ns2:WeatherTransition .


###  http://example.org/ahmi#activatesHapticAlert
ns2:activatesHapticAlert rdf:type owl:ObjectProperty ;
                         rdfs:domain ns2:TactileFeedback ;
                         rdfs:range ns2:HapticAlert ;
                         rdfs:label "activates Haptic Alert"@en .


###  http://example.org/ahmi#adaptedFor
ns2:adaptedFor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf ns2:satisfies ;
               rdfs:domain ns2:DriverStateAdaptation ;
               rdfs:range ns2:Driver ;
               rdfs:label "adapted for"@en .


###  http://example.org/ahmi#adaptsDialogDesign
ns2:adaptsDialogDesign rdf:type owl:ObjectProperty ;
                       rdfs:domain ns2:AdaptationStrategy ;
                       rdfs:range ns2:DialogDesign ;
                       rdfs:comment "Relates an adaptation strategy to the dialog design elements being adjusted based on the driver's state or context."@en ;
                       rdfs:label "adapts dialog design"@en .


###  http://example.org/ahmi#adaptsFor
ns2:adaptsFor rdf:type owl:ObjectProperty ;
              rdfs:domain ns2:DialogDesign ;
              rdfs:range ns2:Driver ;
              rdfs:comment "Indicates that a dialog design element adapts or adjusts for a specific driver state."@en ;
              rdfs:label "adapts for"@en .


###  http://example.org/ahmi#adaptsMessageStructure
ns2:adaptsMessageStructure rdf:type owl:ObjectProperty ;
                           rdfs:domain ns2:AdaptationStrategy ,
                                       ns2:SlowerProcessingCapability ;
                           rdfs:range ns2:MessageStructure ;
                           rdfs:comment "Relates a slower processing capability to the adaptation of simplifying the message structure."@en ,
                                        "Relates an adaptation strategy to the message structure elements being adjusted based on the driver's state or context."@en ;
                           rdfs:label "adapts message structure"@en .


###  http://example.org/ahmi#adaptsTo
ns2:adaptsTo rdf:type owl:ObjectProperty ;
             rdfs:domain ns2:DialogDesign ,
                         ns2:FeedbackModality ,
                         ns2:HMISystem ,
                         ns2:MessageStructure ,
                         ns2:Situation ,
                         ns2:Vehicle ;
             rdfs:range ns2:Driver ,
                        ns2:EnvironmentalCondition ,
                        ns2:SteeringBehavior ,
                        ns2:UserExperienceFactor ;
             rdfs:comment "Relates a vehicle system or situation to the driver state, steering behavior, or environmental condition it adapts to."@en ,
                          "Relates an HMI system to the driver situation it adapts to."@en ,
                          "Relates an intervention or action to the driver state it adapts to."@en ,
                          "Relates a system or feedback modality to the driver state or user experience factor it adapts to." ;
             rdfs:label "adapts to"@en .


###  http://example.org/ahmi#adaptsVehicleSystem
ns2:adaptsVehicleSystem rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf ns2:hasAdaptation ;
                        rdfs:domain ns2:VehicleSystemAdaptation ;
                        rdfs:range ns2:Vehicle ;
                        rdfs:label "adapts vehicle system"@en .


###  http://example.org/ahmi#adaptsVisualElementSize
ns2:adaptsVisualElementSize rdf:type owl:ObjectProperty ;
                            rdfs:domain ns2:SlowerProcessingCapability ;
                            rdfs:range ns2:VisualElement ;
                            rdfs:comment "Relates a slower processing capability to the adaptation of increasing visual element size."@en ;
                            rdfs:label "adapts visual element size"@en .


###  http://example.org/ahmi#addressesDriverState
ns2:addressesDriverState rdf:type owl:ObjectProperty ;
                         rdfs:domain ns2:AdaptationStrategy ;
                         rdfs:range ns2:Driver ;
                         rdfs:comment "Relates an adaptation strategy to the driver state it addresses."@en ;
                         rdfs:label "addresses driver state"@en .


###  http://example.org/ahmi#adjustsFor
ns2:adjustsFor rdf:type owl:ObjectProperty ;
               rdfs:domain ns2:AudioParameters ;
               rdfs:range ns2:Driver ;
               rdfs:comment "Relates an audio parameter to the driver state it adjusts for."@en ;
               rdfs:label "adjusts for"@en .


###  http://example.org/ahmi#affectsPostureState
ns2:affectsPostureState rdf:type owl:ObjectProperty ;
                        rdfs:domain ns2:EnvironmentFactor ,
                                    ns2:RoadCondition ;
                        rdfs:range ns2:Driver ,
                                   ns2:PostureState ;
                        rdfs:comment "Relates a road condition to the posture state it influences."@en ,
                                     "Relates an environment factor to the driver state it can influence."@en ;
                        rdfs:label "affects"@en ,
                                   "affects posture state"@en .


###  http://example.org/ahmi#assessesDriverState
ns2:assessesDriverState rdf:type owl:ObjectProperty ;
                        rdfs:domain ns2:DetectionMethod ,
                                    ns2:Driver ;
                        rdfs:range ns2:Driver ;
                        rdfs:comment "Relates a detection method to the driver state it assesses."@en ,
                                     "Relates a driver readiness assessment to the driver state being assessed."@en ;
                        rdfs:label "assesses driver state"@en .


###  http://example.org/ahmi#assessesEnvironment
ns2:assessesEnvironment rdf:type owl:ObjectProperty ;
                        rdfs:domain ns2:DetectionMethod ;
                        rdfs:range ns2:EnvironmentalCondition ;
                        rdfs:comment "Relates a detection method to the environmental conditions it assesses."@en ;
                        rdfs:label "assesses environment"@en .


###  http://example.org/ahmi#assessesSatisfactionWith
ns2:assessesSatisfactionWith rdf:type owl:ObjectProperty ;
                             rdfs:domain ns2:DriverSatisfactionMeasure ;
                             rdfs:range ns2:TactileFeedbackType ;
                             rdfs:comment "Relates a driver satisfaction measure to the specific type of tactile feedback it is used to assess." ;
                             rdfs:label "assesses satisfaction with"@en .


###  http://example.org/ahmi#assessesVisibility
ns2:assessesVisibility rdf:type owl:ObjectProperty ;
                       rdfs:domain ns2:VisibilityAssessment ;
                       rdfs:range ns2:WeatherCondition ;
                       rdfs:comment "Relates a visibility assessment to the weather condition being evaluated for visibility."@en ;
                       rdfs:label "assesses visibility"@en .


###  http://example.org/ahmi#communicatesRationale
ns2:communicatesRationale rdf:type owl:ObjectProperty ;
                          rdfs:domain ns2:DialogDesign ;
                          rdfs:range ns2:InterventionRationale ;
                          rdfs:comment "Indicates that a dialog design element communicates the rationale or reason for an intervention or event."@en ;
                          rdfs:label "communicates rationale"@en .


###  http://example.org/ahmi#communicatesStatus
ns2:communicatesStatus rdf:type owl:ObjectProperty ;
                       rdfs:domain ns2:DialogDesign ;
                       rdfs:range ns2:InterventionStatus ;
                       rdfs:comment "Indicates that a dialog design element communicates the status of an intervention or event."@en ;
                       rdfs:label "communicates status"@en .


###  http://example.org/ahmi#compliesWith
ns2:compliesWith rdf:type owl:ObjectProperty ;
                 rdfs:domain ns2:DialogDesign ,
                             ns2:FeedbackModality ,
                             ns2:MessageStructure ;
                 rdfs:range ns2:RegulationStrategy ;
                 rdfs:comment "Relates a system or feedback modality to the regulation strategy it complies with." ;
                 rdfs:label "complies with"@en .


###  http://example.org/ahmi#conformsToRegulation
ns2:conformsToRegulation rdf:type owl:ObjectProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( ns2:AdaptationStrategy
                                                     ns2:SystemComponent
                                                   )
                                     ] ;
                         rdfs:range ns2:RegulationStrategy ;
                         rdfs:comment "Relates a system component or adaptation strategy to the regulation strategies it conforms to."@en ;
                         rdfs:label "conforms to regulation"@en .


###  http://example.org/ahmi#considersUserExperienceFactor
ns2:considersUserExperienceFactor rdf:type owl:ObjectProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( ns2:AdaptationStrategy
                                                              ns2:SystemComponent
                                                            )
                                              ] ;
                                  rdfs:range ns2:UserExperienceFactor ;
                                  rdfs:comment "Relates a system component or adaptation strategy to the user experience factors it considers."@en ;
                                  rdfs:label "considers user experience factor"@en .


###  http://example.org/ahmi#correlatedWith
ns2:correlatedWith rdf:type owl:ObjectProperty ;
                   rdfs:domain ns2:Description ;
                   rdfs:range ns2:Description ;
                   rdfs:comment "Indicates a correlation between two descriptions or situations."@en ;
                   rdfs:label "correlated with"@en .


###  http://example.org/ahmi#describedBy
ns2:describedBy rdf:type owl:ObjectProperty ;
                rdfs:domain ns2:Driver ;
                rdfs:range ns2:Driver ;
                rdfs:label "described by"@en .


###  http://example.org/ahmi#detectedBy
ns2:detectedBy rdf:type owl:ObjectProperty ;
               rdfs:domain ns2:Driver ;
               rdfs:range ns2:DetectionMethod ,
                          ns2:DetectionMethodology ,
                          ns2:Driver ,
                          ns2:Vehicle ;
               rdfs:comment "Relates a driver state to the detection method used to identify it."@en ,
                            "Relates a driver state to the detection methodology used to measure it."@en ,
                            "Relates a driver state to the method used to detect it"@en ,
                            "Relates a driver state to the system or method that detected it"@en ;
               rdfs:label "detected by"@en ,
                          "detected by method"@en ,
                          "detected by methodology"@en ,
                          "is detected by"@en .


###  http://example.org/ahmi#detectedDriverState
ns2:detectedDriverState rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf ns2:satisfies ;
                        rdfs:domain ns2:Driver ;
                        rdfs:range ns2:Driver ;
                        rdfs:label "detected driver state"@en .


###  http://example.org/ahmi#detects
ns2:detects rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ns2:detects ;
            rdfs:domain ns2:DetectionMethod ,
                        ns2:FacialExpressionDetection ,
                        ns2:HeartRateSensor ,
                        ns2:PhysiologicalMonitoring ,
                        ns2:PhysiologicalSensor ,
                        ns2:Situation ,
                        ns2:VoiceAnalysis ;
            rdfs:range ns2:DiscreteEmotiveState ,
                       ns2:Driver ,
                       ns2:EmotionalState ,
                       ns2:HighCognitiveLoad ,
                       ns2:PhysiologicalSignal ;
            rdfs:comment "Relates a detection method to the driver state it detects"@en ,
                         "Relates a detection method to the emotive state it can detect."@en ,
                         "Relates a detection situation to the driver reaction it detects."@en ,
                         "Relates a facial expression detection method to the emotive states it can detect."@en ,
                         "Relates a physiological monitoring method to the emotive states it can detect."@en ,
                         "Relates a sensor to the physiological signal or property it detects."@en ,
                         "Relates a voice analysis method to the emotive states it can detect."@en ;
            rdfs:label "detects"@en ,
                       "detects emotional state"@en ,
                       "detects facial expression"@en ,
                       "detects physiological signal"@en ,
                       "detects state"@en ,
                       "detects voice pattern"@en .


###  http://example.org/ahmi#deviatesFromDrivingBehavior
ns2:deviatesFromDrivingBehavior rdf:type owl:ObjectProperty ;
                                rdfs:domain ns2:Driver ;
