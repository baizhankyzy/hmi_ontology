@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:HapticAlert a owl:Class ;
    rdfs:label "Haptic Alert"@en ;
    rdfs:comment "A haptic feedback mechanism used to notify the driver."@en .

ahmi:HapticAlertCharacteristic a owl:Class ;
    rdfs:label "Haptic Alert Characteristic"@en ;
    rdfs:comment "A characteristic or property of a haptic alert."@en .

ahmi:hasCharacteristic a owl:ObjectProperty ;
    rdfs:label "has characteristic"@en ;
    rdfs:comment "Relates a haptic alert to its characteristics."@en ;
    rdfs:domain ahmi:HapticAlert ;
    rdfs:range ahmi:HapticAlertCharacteristic .

ahmi:Intensity a owl:Class ;
    rdfs:label "Intensity"@en ;
    rdfs:comment "The intensity or strength of a haptic alert."@en ;
    rdfs:subClassOf ahmi:HapticAlertCharacteristic .

ahmi:Frequency a owl:Class ;
    rdfs:label "Frequency"@en ;
    rdfs:comment "The frequency or rate of a haptic alert."@en ;
    rdfs:subClassOf ahmi:HapticAlertCharacteristic .

ahmi:Pattern a owl:Class ;
    rdfs:label "Pattern"@en ;
    rdfs:comment "The pattern or rhythm of a haptic alert."@en ;
    rdfs:subClassOf ahmi:HapticAlertCharacteristic .

ahmi:Location a owl:Class ;
    rdfs:label "Location"@en ;
    rdfs:comment "The location or position of a haptic alert."@en ;
    rdfs:subClassOf ahmi:HapticAlertCharacteristic .

ahmi:ElderlyDriver a owl:Class ;
    rdfs:label "Elderly Driver"@en ;
    rdfs:comment "A driver who is elderly or of advanced age."@en .

ahmi:hasPreference a owl:ObjectProperty ;
    rdfs:label "has preference"@en ;
    rdfs:comment "Relates an elderly driver to their preferred haptic alert characteristics."@en ;
    rdfs:domain ahmi:ElderlyDriver ;
    rdfs:range ahmi:HapticAlertCharacteristic .

ahmi:HapticAlertEvent a owl:Class ;
    rdfs:label "Haptic Alert Event"@en ;
    rdfs:comment "An event involving the activation of a haptic alert."@en .

ahmi:HapticAlertSituation a owl:Class ;
    rdfs:label "Haptic Alert Situation"@en ;
    rdfs:comment "A situation or view on a haptic alert event, interpreted based on a description."@en ;
    rdfs:subClassOf owl:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:HapticAlertEvent
    ] .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a situation to the event it observes or interprets."@en ;
    rdfs:domain ahmi:HapticAlertSituation ;
    rdfs:range ahmi:HapticAlertEvent .

ahmi:HapticAlertDescription a owl:Class ;
    rdfs:label "Haptic Alert Description"@en ;
    rdfs:comment "A description or conceptualization that provides context for interpreting haptic alert situations."@en ;
    rdfs:subClassOf owl:Description .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:HapticAlertSituation ;
    rdfs:range ahmi:HapticAlertDescription .

ahmi:DriverStateDescription a owl:Class ;
    rdfs:label "Driver State Description"@en ;
    rdfs:comment "A description or conceptualization of the driver's state or condition."@en ;
    rdfs:subClassOf ahmi:HapticAlertDescription .

ahmi:EnvironmentDescription a owl:Class ;
    rdfs:label "Environment Description"@en ;
    rdfs:comment "A description or conceptualization of the environmental conditions."@en ;
    rdfs:subClassOf ahmi:HapticAlertDescription .

ahmi:VehicleSystemDescription a owl:Class ;
    rdfs:label "Vehicle System Description"@en ;
    rdfs:comment "A description or conceptualization of the vehicle system or HMI."@en ;
    rdfs:subClassOf ahmi:HapticAlertDescription .

ahmi:hasIntensity a owl:DatatypeProperty ;
    rdfs:label "has intensity"@en ;
    rdfs:comment "The intensity or strength of a haptic alert."@en ;
    rdfs:domain ahmi:HapticAlert ;
    rdfs:range xsd:float .

ahmi:hasFrequency a owl:DatatypeProperty ;
    rdfs:label "has frequency"@en ;
    rdfs:comment "The frequency or rate of a haptic alert."@en ;
    rdfs:domain ahmi:HapticAlert ;
    rdfs:range xsd:float .