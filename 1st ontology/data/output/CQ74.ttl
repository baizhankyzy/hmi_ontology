@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:LaneDepartureEvent a owl:Class ;
    rdfs:label "Lane Departure Event"@en ;
    rdfs:comment "An event where the vehicle departs from its intended lane." .

ahmi:LaneKeepingAssistActivation a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystemEvent ;
    rdfs:label "Lane Keeping Assist Activation"@en ;
    rdfs:comment "An event where the lane keeping assist system is activated in response to a lane departure." .

ahmi:VehicleSystemEvent a owl:Class ;
    rdfs:label "Vehicle System Event"@en ;
    rdfs:comment "An event involving the operation of a vehicle system." .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The cognitive, emotional, or behavioral state of the driver." .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A distinct emotional state experienced by the driver." .

ahmi:Confusion a owl:Class ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState ;
    rdfs:label "Confusion"@en ;
    rdfs:comment "The state of being confused or perplexed." .

ahmi:SpeedViolationTendency a owl:Class ;
    rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Speed Violation Tendency"@en ;
    rdfs:comment "A tendency or inclination of the driver to exceed the speed limit." .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A sensory channel through which feedback is provided to the driver." .

ahmi:VisualFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual elements." .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "A visual component used to provide feedback to the driver." .

ahmi:AudioFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "Feedback provided through auditory signals or tones." .

ahmi:TactileFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Feedback provided through tactile or haptic sensations." .

ahmi:ForceFeedback a owl:Class ;
    rdfs:subClassOf ahmi:TactileFeedback ;
    rdfs:label "Force Feedback"@en ;
    rdfs:comment "Tactile feedback provided through force or resistance applied to a control interface." .

ahmi:SteeringWheelForceFeedback a owl:Class ;
    rdfs:subClassOf ahmi:ForceFeedback ;
    rdfs:label "Steering Wheel Force Feedback"@en ;
    rdfs:comment "Force feedback applied to the steering wheel." .

ahmi:DialogDesign a owl:Class ;
    rdfs:label "Dialog Design"@en ;
    rdfs:comment "The design and structure of the dialog between the system and the driver." .

ahmi:MessageStructure a owl:Class ;
    rdfs:label "Message Structure"@en ;
    rdfs:comment "The organization and presentation of messages or information to the driver." .

ahmi:TrustCalibrationMechanism a owl:Class ;
    rdfs:label "Trust Calibration Mechanism"@en ;
    rdfs:comment "A mechanism for adjusting the driver's trust or confidence in the system." .

ahmi:UserExperienceFactor a owl:Class ;
    rdfs:label "User Experience Factor"@en ;
    rdfs:comment "A factor or principle related to the user's overall experience with the system." .

ahmi:RegulationStrategy a owl:Class ;
    rdfs:label "Regulation Strategy"@en ;
    rdfs:comment "A strategy or guideline defined by regulations or standards." .

ahmi:WarningPrioritization a owl:Class ;
    rdfs:subClassOf ahmi:RegulationStrategy ;
    rdfs:label "Warning Prioritization"@en ;
    rdfs:comment "A strategy for prioritizing and presenting warnings based on their urgency or importance." .

ahmi:LaneDepartureWarning a owl:Class ;
    rdfs:subClassOf ahmi:VisualElement ;
    rdfs:label "Lane Departure Warning"@en ;
    rdfs:comment "A visual element used to warn the driver about a lane departure event." .

ahmi:hasVisualElement a owl:ObjectProperty ;
    rdfs:label "has visual element"@en ;
    rdfs:comment "Relates a feedback modality or event to the visual elements it includes or triggers." ;
    rdfs:domain ahmi:VisualFeedback , ahmi:LaneKeepingAssistActivation ;
    rdfs:range ahmi:VisualElement .

ahmi:providesFor a owl:ObjectProperty ;
    rdfs:label "provides for"@en ;
    rdfs:comment "Relates a feedback modality to the event or situation it provides feedback for." ;
    rdfs:domain ahmi:FeedbackModality ;
    rdfs:range ahmi:VehicleSystemEvent , ahmi:Situation .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a situation or event to the driver state associated with it." ;
    rdfs:domain ahmi:Situation , ahmi:VehicleSystemEvent ;
    rdfs:range ahmi:DriverState .

ahmi:adaptsTo a owl:ObjectProperty ;
    rdfs:label "adapts to"@en ;
    rdfs:comment "Relates a system or feedback modality to the driver state or user experience factor it adapts to." ;
    rdfs:domain ahmi:FeedbackModality , ahmi:DialogDesign , ahmi:MessageStructure ;
    rdfs:range ahmi:DriverState , ahmi:UserExperienceFactor .

ahmi:compliesWith a owl:ObjectProperty ;
    rdfs:label "complies with"@en ;
    rdfs:comment "Relates a system or feedback modality to the regulation strategy it complies with." ;
    rdfs:domain ahmi:FeedbackModality , ahmi:DialogDesign , ahmi:MessageStructure ;
    rdfs:range ahmi:RegulationStrategy .

ahmi:hasUrgency a owl:DatatypeProperty ;
    rdfs:label "has urgency"@en ;
    rdfs:comment "The urgency or priority level associated with a warning or feedback." ;
    rdfs:domain ahmi:VisualElement , ahmi:AudioFeedback ;
    rdfs:range xsd:float .

ahmi:LaneDepartureWarningEvent a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystemEvent ;
    rdfs:label "Lane Departure Warning Event"@en ;
    rdfs:comment "An event where a lane departure warning is triggered." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasVisualElement ;
        owl:someValuesFrom ahmi:LaneDepartureWarning
    ] .

ahmi:LaneDepartureWarningViewSituation a owl:Class ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:label "Lane Departure Warning View Situation"@en ;
    rdfs:comment "A situation representing a particular view or interpretation of a lane departure warning event." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:LaneDepartureWarningDescription
    ] .

ahmi:LaneDepartureWarningDescription a owl:Class ;
    rdfs:subClassOf ahmi:Description ;
    rdfs:label "Lane Departure Warning Description"@en ;
    rdfs:comment "A conceptualization or description that provides context for interpreting lane departure warning situations." .

ahmi:CrossModalCongruence a owl:Class ;
    rdfs:subClassOf ahmi:UserExperienceFactor ;
    rdfs:label "Cross-Modal Congruence"@en ;
    rdfs:comment "The consistency and alignment between different feedback modalities." .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies." ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .