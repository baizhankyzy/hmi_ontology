@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A trait or characteristic of an individual's personality."@en .

ahmi:AggressiveDrivingTendency a owl:Class ;
    rdfs:label "Aggressive Driving Tendency"@en ;
    rdfs:subClassOf ahmi:PersonalityFactor .

ahmi:LowConscientiousness a owl:Class ;
    rdfs:label "Low Conscientiousness"@en ;
    rdfs:subClassOf ahmi:PersonalityFactor .

ahmi:BigFivePersonalityTraits a owl:Class ;
    rdfs:label "Big Five Personality Traits"@en ;
    rdfs:comment "A model of personality traits consisting of five broad dimensions: Openness, Conscientiousness, Extraversion, Agreeableness, and Neuroticism."@en .

ahmi:hasPersonalityFactor a owl:ObjectProperty ;
    rdfs:label "has personality factor"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:PersonalityFactor .

ahmi:Driver a owl:Class ;
    rdfs:label "Driver"@en ;
    rdfs:comment "A person who operates a vehicle."@en .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en ;
    rdfs:comment "The actions and patterns exhibited by a driver while operating a vehicle."@en .

ahmi:hasTypicalDrivingBehavior a owl:ObjectProperty ;
    rdfs:label "has typical driving behavior"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:DrivingBehavior .

ahmi:DetectionMethodology a owl:Class ;
    rdfs:label "Detection Methodology"@en ;
    rdfs:comment "A method or technique used by a vehicle system to monitor and detect the driver's state."@en .

ahmi:SteeringBehaviorAnalysis a owl:Class ;
    rdfs:label "Steering Behavior Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:PedalControlAnalysis a owl:Class ;
    rdfs:label "Pedal Control Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:PressureSensors a owl:Class ;
    rdfs:label "Pressure Sensors"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:SpeechRecognition a owl:Class ;
    rdfs:label "Speech Recognition"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:ContextualAnalysis a owl:Class ;
    rdfs:label "Contextual Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:employsDetectionMethodology a owl:ObjectProperty ;
    rdfs:label "employs detection methodology"@en ;
    rdfs:domain ahmi:VehicleSystem ;
    rdfs:range ahmi:DetectionMethodology .

ahmi:VehicleSystem a owl:Class ;
    rdfs:label "Vehicle System"@en ;
    rdfs:comment "A system or component within a vehicle responsible for monitoring, analyzing, and responding to the driver's state and behavior."@en .

ahmi:HMI a owl:Class ;
    rdfs:label "Human-Machine Interface"@en ;
    rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:comment "The interface through which a human (driver) interacts with the vehicle's systems and receives feedback."@en .

ahmi:EmotionalState a owl:Class ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "The emotional condition or state of the driver."@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:comment "A distinct emotional state, such as anger, fear, or joy."@en .

ahmi:Frustration a owl:Class ;
    rdfs:label "Frustration"@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:ContinuousEmotiveState a owl:Class ;
    rdfs:label "Continuous Emotive State"@en ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:comment "An emotional state that varies along a continuous scale, such as arousal or valence."@en .

ahmi:ArousalLevel a owl:Class ;
    rdfs:label "Arousal Level"@en ;
    rdfs:subClassOf ahmi:ContinuousEmotiveState .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "An external factor or condition in the environment that may influence the driver's state or behavior."@en .

ahmi:TrafficCondition a owl:Class ;
    rdfs:label "Traffic Condition"@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:HighCongestion a owl:Class ;
    rdfs:label "High Congestion"@en ;
    rdfs:subClassOf ahmi:TrafficCondition .

ahmi:RoadType a owl:Class ;
    rdfs:label "Road Type"@en ;
    rdfs:comment "The type or classification of the road."@en .

ahmi:CityRoad a owl:Class ;
    rdfs:label "City Road"@en ;
    rdfs:subClassOf ahmi:RoadType .

ahmi:ConstructionEvent a owl:Class ;
    rdfs:label "Construction Event"@en ;
    rdfs:comment "An event or situation involving construction activities on the road."@en .

ahmi:JourneyContext a owl:Class ;
    rdfs:label "Journey Context"@en ;
    rdfs:comment "The context or circumstances surrounding the driver's journey."@en .

ahmi:TimeOnTask a owl:Class ;
    rdfs:label "Time on Task"@en ;
    rdfs:comment "The amount of time spent on a particular task or activity."@en .

ahmi:BrakingBehavior a owl:Class ;
    rdfs:label "Braking Behavior"@en ;
    rdfs:subClassOf ahmi:DrivingBehavior .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "The sensory channel or mode through which feedback is provided to the driver."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AuditoryFeedback a owl:Class ;
    rdfs:label "Auditory Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AuditoryDisplay a owl:Class ;
    rdfs:label "Auditory Display"@en ;
    rdfs:comment "A type of auditory feedback or interface used to convey information to the driver."@en .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "A visual component or element used in the visual feedback interface."@en .

ahmi:ColorScheme a owl:Class ;
    rdfs:label "Color Scheme"@en ;
    rdfs:subClassOf ahmi:VisualElement .

ahmi:BlueColorScheme a owl:Class ;
    rdfs:label "Blue Color Scheme"@en ;
    rdfs:subClassOf ahmi:ColorScheme .

ahmi:EnvironmentalSystem a owl:Class ;
    rdfs:label "Environmental System"@en ;
    rdfs:comment "A system within the vehicle that controls environmental factors, such as temperature or air quality."@en .

ahmi:CabinTemperature a owl:Class ;
    rdfs:label "Cabin Temperature"@en ;
    rdfs:comment "The temperature inside the vehicle cabin."@en .

ahmi:SpeedManagementAnalysis a owl:Class ;
    rdfs:label "Speed Management Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:AggressiveAcceleration a owl:Class ;
    rdfs:label "Aggressive Acceleration"@en ;
    rdfs:subClassOf ahmi:DrivingBehavior .

ahmi:ForceFeedback a owl:Class ;
    rdfs:label "Force Feedback"@en ;
    rdfs:subClassOf ahmi:TactileFeedback .

ahmi:AcceleratorPedal a owl:Class ;
    rdfs:label "Accelerator Pedal"@en ;
    rdfs:comment "The pedal in a vehicle used to control acceleration."@en .

ahmi:SmoothDrivingBehavior a owl:Class ;
    rdfs:label "Smooth Driving Behavior"@en ;
    rdfs:subClassOf ahmi:DrivingBehavior .

ahmi:providesVisualFeedback a owl:ObjectProperty ;
    rdfs:label "provides visual feedback"@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:VisualFeedback .

ahmi:providesAuditoryFeedback a owl:ObjectProperty ;
    rdfs:label "provides auditory feedback"@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:AuditoryFeedback .

ahmi:providesTactileFeedback a owl:ObjectProperty ;
    rdfs:label "provides tactile feedback"@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:TactileFeedback .

ahmi:hasVisualElement a owl:ObjectProperty ;
    rdfs:label "has visual element"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:VisualElement .

ahmi:hasColorScheme a owl:ObjectProperty ;
    rdfs:label "has color scheme"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:ColorScheme .

ahmi:controlsEnvironment a owl:ObjectProperty ;
    rdfs:label "controls environment"@en ;
    rdfs:domain ahmi:EnvironmentalSystem ;
    rdfs:range ahmi:CabinTemperature .

ahmi:hasPreferredAuditoryDisplay a owl:ObjectProperty ;
    rdfs:label "has preferred auditory display"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:AuditoryDisplay .

ahmi:providesForceFeedback a owl:ObjectProperty ;
    rdfs:label "provides force feedback"@en ;
    rdfs:domain ahmi:TactileFeedback ;
    rdfs:range ahmi:AcceleratorPedal .

dul: