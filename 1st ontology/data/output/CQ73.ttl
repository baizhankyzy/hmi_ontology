@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:LaneKeepingAssistIntervention a owl:Class ;
    rdfs:label "Lane keeping assist intervention"@en ;
    rdfs:subClassOf ahmi:VehicleSystemIntervention .

ahmi:VehicleSystemIntervention a owl:Class ;
    rdfs:label "Vehicle system intervention"@en ;
    rdfs:subClassOf ahmi:Event .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en .

ahmi:ForceFeedbackParameter a owl:Class ;
    rdfs:label "Force feedback parameter"@en ;
    rdfs:subClassOf ahmi:Parameter .

ahmi:Parameter a owl:Class ;
    rdfs:label "Parameter"@en .

ahmi:LaneKeepingAssistForceFeedbackSituation a owl:Class ;
    rdfs:label "Lane keeping assist force feedback situation"@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:LaneKeepingAssistIntervention
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasParameter ;
        owl:someValuesFrom ahmi:ForceFeedbackParameter
    ] .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Parameter .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .