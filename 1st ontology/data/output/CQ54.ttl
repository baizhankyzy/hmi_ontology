@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process that occurs over time."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event, consistent with a particular description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:Event
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en .

ahmi:Driver a owl:Class ;
    rdfs:label "Driver"@en ;
    rdfs:comment "A person operating a vehicle."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A condition or attribute of a driver during a driving event."@en ;
    rdfs:subClassOf ahmi:Description .

ahmi:Frustration a owl:Class ;
    rdfs:label "Frustration"@en ;
    rdfs:comment "A negative emotional state experienced by a driver."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:ElectrodermalActivity a owl:Class ;
    rdfs:label "Electrodermal Activity"@en ;
    rdfs:comment "Physiological measurement of skin conductance."@en ;
    rdfs:subClassOf ahmi:Description .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a situation to the event it observes or interprets."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a driver to a state they experience during an event."@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:DriverState .

ahmi:hasElectrodermalActivity a owl:ObjectProperty ;
    rdfs:label "has electrodermal activity"@en ;
    rdfs:comment "Relates a driver state to a measured electrodermal activity pattern."@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:ElectrodermalActivity .

ahmi:correlatedWith a owl:ObjectProperty ;
    rdfs:label "correlated with"@en ;
    rdfs:comment "Indicates a correlation between two descriptions or situations."@en ;
    rdfs:domain ahmi:Description ;
    rdfs:range ahmi:Description .

ahmi:FrustrationSituation a owl:Class ;
    rdfs:label "Frustration Situation"@en ;
    rdfs:comment "A situation where a driver experiences frustration."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:hasValue ahmi:Frustration
    ] .

ahmi:ElectrodermalActivitySituation a owl:Class ;
    rdfs:label "Electrodermal Activity Situation"@en ;
    rdfs:comment "A situation where a driver exhibits a particular electrodermal activity pattern."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:hasValue ahmi:ElectrodermalActivity
    ] .

ahmi:correlatedFrustrationAndEDA a owl:Class ;
    rdfs:label "Correlated Frustration and Electrodermal Activity"@en ;
    rdfs:comment "A situation where a driver's frustration is correlated with a specific electrodermal activity pattern."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:FrustrationSituation
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:ElectrodermalActivitySituation
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:hasValue [
            a owl:Restriction ;
            owl:onProperty ahmi:correlatedWith ;
            owl:someValuesFrom ahmi:Frustration ;
            owl:someValuesFrom ahmi:ElectrodermalActivity
        ]
    ] .