@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process that occurs over time."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event, consistent with a particular description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:Event
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A condition or attribute of the driver that influences the HMI interaction."@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A distinct emotional state experienced by the driver, such as confusion."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DriverTendency a owl:Class ;
    rdfs:label "Driver Tendency"@en ;
    rdfs:comment "A recurring behavior or inclination exhibited by the driver."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:SpeedViolationTendency a owl:Class ;
    rdfs:label "Speed Violation Tendency"@en ;
    rdfs:comment "A tendency of the driver to exceed speed limits."@en ;
    rdfs:subClassOf ahmi:DriverTendency .

ahmi:HMIFeedback a owl:Class ;
    rdfs:label "HMI Feedback"@en ;
    rdfs:comment "A feedback mechanism used by the HMI system to communicate with the driver."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Visual elements used by the HMI system to provide feedback to the driver."@en ;
    rdfs:subClassOf ahmi:HMIFeedback .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "Audio cues or tones used by the HMI system to provide feedback to the driver."@en ;
    rdfs:subClassOf ahmi:HMIFeedback .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Tactile or haptic feedback mechanisms used by the HMI system, such as force feedback."@en ;
    rdfs:subClassOf ahmi:HMIFeedback .

ahmi:ForceFeedback a owl:Class ;
    rdfs:label "Force Feedback"@en ;
    rdfs:comment "A type of tactile feedback that applies physical forces to the driver's controls."@en ;
    rdfs:subClassOf ahmi:TactileFeedback .

ahmi:LaneKeepingAssistActivation a owl:Class ;
    rdfs:label "Lane Keeping Assist Activation"@en ;
    rdfs:comment "An event where the lane keeping assist system is activated."@en ;
    rdfs:subClassOf ahmi:Event .

ahmi:LaneDeparture a owl:Class ;
    rdfs:label "Lane Departure"@en ;
    rdfs:comment "An event where the vehicle departs from its intended lane."@en ;
    rdfs:subClassOf ahmi:Event .

ahmi:AlertTiming a owl:Class ;
    rdfs:label "Alert Timing"@en ;
    rdfs:comment "The timing and urgency of alerts provided by the HMI system."@en .

ahmi:UrgencyBasedTiming a owl:Class ;
    rdfs:label "Urgency-Based Timing"@en ;
    rdfs:comment "Alert timing based on the urgency or criticality of the situation."@en ;
    rdfs:subClassOf ahmi:AlertTiming .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a situation to the event it observes or interprets."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies or is consistent with."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a situation to the driver state observed in that situation."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:DriverState .

ahmi:providesVisualFeedback a owl:ObjectProperty ;
    rdfs:label "provides visual feedback"@en ;
    rdfs:comment "Relates a situation to the visual feedback provided by the HMI system."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:VisualFeedback .

ahmi:providesAudioFeedback a owl:ObjectProperty ;
    rdfs:label "provides audio feedback"@en ;
    rdfs:comment "Relates a situation to the audio feedback provided by the HMI system."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:AudioFeedback .

ahmi:providesTouchFeedback a owl:ObjectProperty ;
    rdfs:label "provides touch feedback"@en ;
    rdfs:comment "Relates a situation to the tactile or haptic feedback provided by the HMI system."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:TactileFeedback .

ahmi:hasAlertTiming a owl:ObjectProperty ;
    rdfs:label "has alert timing"@en ;
    rdfs:comment "Relates a situation to the timing and urgency of alerts provided by the HMI system."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:AlertTiming .

ahmi:hasUrgencyBasedTiming a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasAlertTiming ;
    rdfs:label "has urgency-based timing"@en ;
    rdfs:comment "Relates a situation to the urgency-based timing of alerts provided by the HMI system."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:UrgencyBasedTiming .

ahmi:triggeredBy a owl:ObjectProperty ;
    rdfs:label "triggered by"@en ;
    rdfs:comment "Relates an event to the event that triggered or caused it."@en ;
    rdfs:domain ahmi:Event ;
    rdfs:range ahmi:Event .

ahmi:hasTimingThreshold a owl:DatatypeProperty ;
    rdfs:label "has timing threshold"@en ;
    rdfs:comment "The timing threshold or urgency level associated with an alert timing strategy."@en ;
    rdfs:domain ahmi:AlertTiming ;
    rdfs:range xsd:decimal .