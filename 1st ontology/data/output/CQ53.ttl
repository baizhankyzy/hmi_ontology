@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A state of the driver, such as cognitive state, distraction state, or personality factor."@en .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The cognitive state of the driver, such as mental workload, frustration, or effort level."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DistractionState a owl:Class ;
    rdfs:label "Distraction State"@en ;
    rdfs:comment "A state of distraction experienced by the driver."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A factor related to the driver's personality, such as Big Five traits or aggressive driving tendency."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DetectionMethodology a owl:Class ;
    rdfs:label "Detection Methodology"@en ;
    rdfs:comment "A methodology used to detect or measure the driver's state."@en .

ahmi:PhysiologicalDetection a owl:Class ;
    rdfs:label "Physiological Detection"@en ;
    rdfs:comment "A detection methodology that measures physiological signals, such as electrodermal activity, heart rate, or brain activity."@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:ElectrodermalActivity a owl:Class ;
    rdfs:label "Electrodermal Activity"@en ;
    rdfs:comment "A physiological detection method that measures skin conductance."@en ;
    rdfs:subClassOf ahmi:PhysiologicalDetection .

ahmi:HeartRateMonitoring a owl:Class ;
    rdfs:label "Heart Rate Monitoring"@en ;
    rdfs:comment "A physiological detection method that measures heart rate using electrocardiography (ECG)."@en ;
    rdfs:subClassOf ahmi:PhysiologicalDetection .

ahmi:Electroencephalogram a owl:Class ;
    rdfs:label "Electroencephalogram"@en ;
    rdfs:comment "A physiological detection method that measures brain activity using electroencephalography (EEG)."@en ;
    rdfs:subClassOf ahmi:PhysiologicalDetection .

ahmi:SelfMeasure a owl:Class ;
    rdfs:label "Self-Measure"@en ;
    rdfs:comment "A subjective measure reported by the driver, such as a questionnaire or rating."@en ;
    rdfs:subClassOf ahmi:DetectionMethodology .

ahmi:Questionnaire a owl:Class ;
    rdfs:label "Questionnaire"@en ;
    rdfs:comment "A self-measure in the form of a questionnaire, such as NASA-TLX or personality assessments."@en ;
    rdfs:subClassOf ahmi:SelfMeasure .

ahmi:Rating a owl:Class ;
    rdfs:label "Rating"@en ;
    rdfs:comment "A self-measure in the form of a rating, such as user satisfaction with feedback modalities."@en ;
    rdfs:subClassOf ahmi:SelfMeasure .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A modality used to provide feedback to the driver, such as tactile, audio, or visual feedback."@en .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "A feedback modality that provides tactile feedback to the driver."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "A feedback modality that provides audio feedback to the driver."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "A feedback modality that provides visual feedback to the driver."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:DriverEvent a owl:Class ;
    rdfs:label "Driver Event"@en ;
    rdfs:comment "An event involving the driver, such as a cognitive state change or a distraction event."@en ;
    rdfs:subClassOf dul:Event .

ahmi:DriverSituation a owl:Class ;
    rdfs:label "Driver Situation"@en ;
    rdfs:comment "A situation representing a view or interpretation of a driver event."@en ;
    rdfs:subClassOf dul:Situation .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a driver situation or event to the driver state involved."@en ;
    rdfs:domain dul:Situation , dul:Event ;
    rdfs:range ahmi:DriverState .

ahmi:detectedBy a owl:ObjectProperty ;
    rdfs:label "detected by"@en ;
    rdfs:comment "Relates a driver state to the detection methodology used to measure it."@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DetectionMethodology .

ahmi:involvesFeedback a owl:ObjectProperty ;
    rdfs:label "involves feedback"@en ;
    rdfs:comment "Relates a driver situation or event to the feedback modality involved."@en ;
    rdfs:domain dul:Situation , dul:Event ;
    rdfs:range ahmi:FeedbackModality .

ahmi:hasRating a owl:ObjectProperty ;
    rdfs:label "has rating"@en ;
    rdfs:comment "Relates a feedback modality to a rating provided by the driver."@en ;
    rdfs:domain ahmi:FeedbackModality ;
    rdfs:range ahmi:Rating .

ahmi:hasTimestamp a owl:DatatypeProperty ;
    rdfs:label "has timestamp"@en ;
    rdfs:comment "The timestamp associated with a self-measure or detection methodology."@en ;
    rdfs:domain ahmi:SelfMeasure , ahmi:DetectionMethodology ;
    rdfs:range xsd:dateTime .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:equivalentProperty dul:satisfies ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en .

ahmi:isPartOf a owl:ObjectProperty ;
    rdfs:equivalentProperty dul:isPartOf ;
    rdfs:label "is part of"@en ;
    rdfs:comment "A relation between any entities, e.g. 'a driver state is part of a driver situation'."@en .

ahmi:isParameterFor a owl:ObjectProperty ;
    rdfs:equivalentProperty dul:isParameterFor ;
    rdfs:label "is parameter for"@en ;
    rdfs:comment "A parameter that constrains the attributes of a driver state or situation."@en .