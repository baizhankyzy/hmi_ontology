@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A trait or characteristic of an individual's personality."@en .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en ;
    rdfs:comment "The manner in which an individual operates a vehicle."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The physical, mental, and emotional condition of a driver."@en .

ahmi:EmotionalState a owl:Class ;
    rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "The emotional condition of a driver."@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A distinct emotional state, such as frustration or anger."@en .

ahmi:ContinuousEmotiveState a owl:Class ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:label "Continuous Emotive State"@en ;
    rdfs:comment "An emotional state that varies along a continuum, such as arousal levels."@en .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "An external condition or circumstance that may influence a driver's state."@en .

ahmi:DetectionMethodology a owl:Class ;
    rdfs:label "Detection Methodology"@en ;
    rdfs:comment "A technique or approach used to monitor and detect a driver's state."@en .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A means of providing feedback or adaptation to a driver."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual displays or elements."@en .

ahmi:AudioFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "Feedback provided through auditory displays or sounds."@en .

ahmi:TactileFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Feedback provided through tactile or haptic sensations."@en .

ahmi:AdaptationStrategy a owl:Class ;
    rdfs:label "Adaptation Strategy"@en ;
    rdfs:comment "A plan or approach for adapting the vehicle's systems or feedback to a driver's state."@en .

ahmi:hasPersonalityFactor a owl:ObjectProperty ;
    rdfs:label "has personality factor"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:PersonalityFactor .

ahmi:exhibitsDrivingBehavior a owl:ObjectProperty ;
    rdfs:label "exhibits driving behavior"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DrivingBehavior .

ahmi:triggeredByEnvironmentFactor a owl:ObjectProperty ;
    rdfs:label "triggered by environment factor"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:detectedByMethodology a owl:ObjectProperty ;
    rdfs:label "detected by methodology"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DetectionMethodology .

ahmi:requiresFeedbackModality a owl:ObjectProperty ;
    rdfs:label "requires feedback modality"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:FeedbackModality .

ahmi:hasAdaptationStrategy a owl:ObjectProperty ;
    rdfs:label "has adaptation strategy"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:AdaptationStrategy .

ahmi:DriverSituation a owl:Class ;
    rdfs:subClassOf dul:Situation ;
    rdfs:label "Driver Situation"@en ;
    rdfs:comment "A view or interpretation of a driver's state and the context in which it occurs."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:DriverStateDescription
    ] .

ahmi:DriverStateDescription a owl:Class ;
    rdfs:subClassOf dul:Description ;
    rdfs:label "Driver State Description"@en ;
    rdfs:comment "A conceptualization that provides a context for interpreting a driver's state and situation."@en .

ahmi:hasDriverSituation a owl:ObjectProperty ;
    rdfs:label "has driver situation"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DriverSituation .

ahmi:describedBy a owl:ObjectProperty ;
    rdfs:label "described by"@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:DriverStateDescription .