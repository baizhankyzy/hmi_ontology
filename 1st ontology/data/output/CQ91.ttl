@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A trait or characteristic of an individual's personality that influences their behavior and interactions."@en .

ahmi:DrivingBehaviour a owl:Class ;
    rdfs:label "Driving Behaviour"@en ;
    rdfs:comment "A pattern of actions and responses exhibited by a driver while operating a vehicle."@en .

ahmi:influences a owl:ObjectProperty ;
    rdfs:label "influences"@en ;
    rdfs:comment "Relates a personality factor to the driving behaviour it affects."@en ;
    rdfs:domain ahmi:PersonalityFactor ;
    rdfs:range ahmi:DrivingBehaviour .

ahmi:DrivingEvent a owl:Class ;
    rdfs:label "Driving Event"@en ;
    rdfs:comment "A physical, social, or mental process that occurs during driving."@en ;
    rdfs:subClassOf owl:Thing .

ahmi:DrivingSituation a owl:Class ;
    rdfs:label "Driving Situation"@en ;
    rdfs:comment "A view or interpretation of a driving event, consistent with a driving context description."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:DrivingEvent
    ] .

ahmi:DrivingContextDescription a owl:Class ;
    rdfs:label "Driving Context Description"@en ;
    rdfs:comment "A conceptualization that provides a context for interpreting driving situations and events."@en ;
    rdfs:subClassOf ahmi:Description .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view, consistent with a Description, on a set of entities."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides a context for interpreting situations and events."@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a driving situation to the driving event it observes or interprets."@en ;
    rdfs:domain ahmi:DrivingSituation ;
    rdfs:range ahmi:DrivingEvent .

ahmi:DrivingContextParameter a owl:Class ;
    rdfs:label "Driving Context Parameter"@en ;
    rdfs:comment "A parameter that constrains the attributes of entities in a driving situation."@en ;
    rdfs:subClassOf ahmi:Parameter .

ahmi:Parameter a owl:Class ;
    rdfs:label "Parameter"@en ;
    rdfs:comment "A concept that constrains attributes of entities in a situation."@en .

ahmi:isParameterFor a owl:ObjectProperty ;
    rdfs:label "is parameter for"@en ;
    rdfs:comment "Relates a parameter to the driving context description it constrains."@en ;
    rdfs:domain ahmi:DrivingContextParameter ;
    rdfs:range ahmi:DrivingContextDescription ;
    rdfs:subPropertyOf ahmi:isRelatedToConcept .

ahmi:isRelatedToConcept a owl:ObjectProperty ;
    rdfs:label "is related to concept"@en ;
    rdfs:comment "A general relation between an entity and a concept."@en ;
    rdfs:range ahmi:Description .