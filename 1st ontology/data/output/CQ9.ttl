@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:Driver a owl:Class ;
    rdfs:label "Driver"@en ;
    rdfs:comment "A person operating a vehicle"@en .

ahmi:Vehicle a owl:Class ;
    rdfs:label "Vehicle"@en ;
    rdfs:comment "A means of transportation"@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A condition or state of the driver"@en .

ahmi:CognitiveState a owl:Class ;
    rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "A mental state of the driver"@en .

ahmi:InattentionState a owl:Class ;
    rdfs:subClassOf ahmi:CognitiveState ;
    rdfs:label "Inattention State"@en ;
    rdfs:comment "A state of reduced attention or focus"@en .

ahmi:Drowsiness a owl:Class ;
    rdfs:subClassOf ahmi:InattentionState ;
    rdfs:label "Drowsiness"@en ;
    rdfs:comment "A state of feeling sleepy or fatigued"@en .

ahmi:Fatigue a owl:Class ;
    rdfs:subClassOf ahmi:InattentionState ;
    rdfs:label "Fatigue"@en ;
    rdfs:comment "A state of physical or mental exhaustion"@en .

ahmi:VehicleSystem a owl:Class ;
    rdfs:label "Vehicle System"@en ;
    rdfs:comment "A system or component of the vehicle"@en .

ahmi:HMI a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:label "Human-Machine Interface"@en ;
    rdfs:comment "An interface for interaction between the driver and vehicle"@en .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A type of feedback provided to the driver"@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual elements"@en .

ahmi:DisplayType a owl:Class ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:label "Display Type"@en ;
    rdfs:comment "A type of visual display used for feedback"@en .

ahmi:VisualElement a owl:Class ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "A visual component or element used for feedback"@en .

ahmi:TactileFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Feedback provided through touch or haptic sensations"@en .

ahmi:HapticAlert a owl:Class ;
    rdfs:subClassOf ahmi:TactileFeedback ;
    rdfs:label "Haptic Alert"@en ;
    rdfs:comment "A tactile alert or notification"@en .

ahmi:AudioFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "Feedback provided through sound or audio"@en .

ahmi:AuditoryDisplay a owl:Class ;
    rdfs:subClassOf ahmi:AudioFeedback ;
    rdfs:label "Auditory Display"@en ;
    rdfs:comment "An audio display or sound used for feedback"@en .

ahmi:DialogDesign a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:label "Dialog Design"@en ;
    rdfs:comment "A system for designing and managing dialog interactions"@en .

ahmi:MessageStructure a owl:Class ;
    rdfs:label "Message Structure"@en ;
    rdfs:comment "The structure or format of a message or alert"@en .

ahmi:VehicleData a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:label "Vehicle Data"@en ;
    rdfs:comment "Data collected or generated by the vehicle"@en .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en ;
    rdfs:comment "The behavior or patterns exhibited while driving"@en .

ahmi:AdaptivityParameter a owl:Class ;
    rdfs:label "Adaptivity Parameter"@en ;
    rdfs:comment "A parameter used to adapt or adjust the system's behavior"@en .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process or occurrence"@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event based on a description"@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations"@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies"@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a driver to their current state"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:DriverState .

ahmi:detectedBy a owl:ObjectProperty ;
    rdfs:label "detected by"@en ;
    rdfs:comment "Relates a driver state to the system or method that detected it"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:VehicleSystem .

ahmi:triggersFeedback a owl:ObjectProperty ;
    rdfs:label "triggers feedback"@en ;
    rdfs:comment "Relates a driver state to the feedback modalities triggered in response"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:FeedbackModality .

ahmi:hasFeedbackModality a owl:ObjectProperty ;
    rdfs:label "has feedback modality"@en ;
    rdfs:comment "Relates an HMI system to the feedback modalities it provides"@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:FeedbackModality .

ahmi:hasMessageStructure a owl:ObjectProperty ;
    rdfs:label "has message structure"@en ;
    rdfs:comment "Relates a dialog design system to the message structures it uses"@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:MessageStructure .

ahmi:observesEvent a owl:ObjectProperty ;
    rdfs:label "observes event"@en ;
    rdfs:comment "Relates a situation to the event it is an interpretation of"@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:hasAdaptivityParameter a owl:ObjectProperty ;
    rdfs:label "has adaptivity parameter"@en ;
    rdfs:comment "Relates a system or component to the adaptivity parameters it uses"@en ;
    rdfs:domain ahmi:VehicleSystem ;
    rdfs:range ahmi:AdaptivityParameter .

ahmi:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:comment "Relates a concept to the parameters that constrain it"@en ;
    rdfs:domain owl:Class ;
    rdfs:range ahmi:AdaptivityParameter .

ahmi:isPartOf a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "is part of"@en ;
    rdfs:comment "Indicates that an entity is a part of another entity"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing .