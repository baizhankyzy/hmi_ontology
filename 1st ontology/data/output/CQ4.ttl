@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Visual elements presented to the driver through the vehicle's human-machine interface (HMI)."@en .

ahmi:DisplayType a owl:Class ;
    rdfs:label "Display Type"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "A type of visual display used for presenting information to the driver."@en .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "A visual component or graphic used in the HMI display."@en .

ahmi:Brightness a owl:Class ;
    rdfs:label "Brightness"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "The level of brightness or luminance of a visual display or element."@en .

ahmi:Contrast a owl:Class ;
    rdfs:label "Contrast"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "The difference in luminance or color between visual elements and their background."@en .

ahmi:InattentionState a owl:Class ;
    rdfs:label "Inattention State"@en ;
    rdfs:comment "A cognitive state of the driver characterized by reduced attention or awareness."@en .

ahmi:Drowsiness a owl:Class ;
    rdfs:label "Drowsiness"@en ;
    rdfs:subClassOf ahmi:InattentionState ;
    rdfs:comment "A state of feeling sleepy or fatigued."@en .

ahmi:Fatigue a owl:Class ;
    rdfs:label "Fatigue"@en ;
    rdfs:subClassOf ahmi:InattentionState ;
    rdfs:comment "A state of physical or mental exhaustion."@en .

ahmi:hasVisualFeedback a owl:ObjectProperty ;
    rdfs:label "has visual feedback"@en ;
    rdfs:domain ahmi:InattentionState ;
    rdfs:range ahmi:VisualFeedback ;
    rdfs:comment "Relates an inattention state to the visual feedback elements that should be modified in response."@en .

ahmi:modifiesBrightness a owl:ObjectProperty ;
    rdfs:label "modifies brightness"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:Brightness ;
    rdfs:subPropertyOf ahmi:hasVisualFeedback ;
    rdfs:comment "Indicates that a visual feedback element modifies the brightness of a display or visual element."@en .

ahmi:modifiesContrast a owl:ObjectProperty ;
    rdfs:label "modifies contrast"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:Contrast ;
    rdfs:subPropertyOf ahmi:hasVisualFeedback ;
    rdfs:comment "Indicates that a visual feedback element modifies the contrast of a display or visual element."@en .

ahmi:InattentionEvent a owl:Class ;
    rdfs:label "Inattention Event"@en ;
    rdfs:comment "An event or process involving the driver being in an inattentive state."@en .

ahmi:InattentionSituation a owl:Class ;
    rdfs:label "Inattention Situation"@en ;
    rdfs:subClassOf :Situation ;
    rdfs:comment "A situation or view on an inattention event, interpreted based on a conceptualization or description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :satisfies ;
        owl:someValuesFrom ahmi:InattentionDescription
    ] .

ahmi:InattentionDescription a owl:Class ;
    rdfs:label "Inattention Description"@en ;
    rdfs:subClassOf :Description ;
    rdfs:comment "A conceptualization or description that provides a context for interpreting inattention situations and events."@en .

ahmi:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:domain ahmi:InattentionDescription ;
    rdfs:range ahmi:InattentionParameter ;
    rdfs:comment "Relates an inattention description to the parameters that constrain it."@en .

ahmi:InattentionParameter a owl:Class ;
    rdfs:label "Inattention Parameter"@en ;
    rdfs:subClassOf :Parameter ;
    rdfs:comment "A parameter that constrains the attributes of entities in an inattention situation."@en .

ahmi:AdaptivityParameter a owl:Class ;
    rdfs:label "Adaptivity Parameter"@en ;
    rdfs:subClassOf ahmi:InattentionParameter ;
    rdfs:comment "A parameter that governs how the HMI should adapt its visual feedback in response to an inattention situation."@en .