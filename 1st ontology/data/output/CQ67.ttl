@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process that occurs over time."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event, consistent with a description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "A factor related to the environment that can influence driving conditions."@en .

ahmi:WeatherCondition a owl:Class ;
    rdfs:label "Weather Condition"@en ;
    rdfs:comment "A condition related to the weather."@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:DaytimeCondition a owl:Class ;
    rdfs:label "Daytime Condition"@en ;
    rdfs:comment "A condition related to the time of day."@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:Rain a owl:Class ;
    rdfs:label "Rain"@en ;
    rdfs:comment "A weather condition involving precipitation in the form of liquid water droplets."@en ;
    rdfs:subClassOf ahmi:WeatherCondition .

ahmi:Daytime a owl:Class ;
    rdfs:label "Daytime"@en ;
    rdfs:comment "The period of the day when the sun is above the horizon."@en ;
    rdfs:subClassOf ahmi:DaytimeCondition .

ahmi:Nighttime a owl:Class ;
    rdfs:label "Nighttime"@en ;
    rdfs:comment "The period of the day when the sun is below the horizon."@en ;
    rdfs:subClassOf ahmi:DaytimeCondition .

ahmi:RoadComplexityAnalysis a owl:Class ;
    rdfs:label "Road Complexity Analysis"@en ;
    rdfs:comment "An analysis of the complexity of road conditions based on various factors."@en .

ahmi:hasRoadComplexityScore a owl:DatatypeProperty ;
    rdfs:label "has road complexity score"@en ;
    rdfs:comment "A score indicating the level of complexity of road conditions."@en ;
    rdfs:domain ahmi:RoadComplexityAnalysis ;
    rdfs:range xsd:float .

ahmi:isInfluencedBy a owl:ObjectProperty ;
    rdfs:label "is influenced by"@en ;
    rdfs:comment "Relates a road complexity analysis to the factors that influence it."@en ;
    rdfs:domain ahmi:RoadComplexityAnalysis ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:WeatherAndDaytimeSituation a owl:Class ;
    rdfs:label "Weather and Daytime Situation"@en ;
    rdfs:comment "A situation involving a combination of weather and daytime conditions."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom [
            a owl:Restriction ;
            owl:onProperty ahmi:hasParameter ;
            owl:someValuesFrom ahmi:WeatherCondition
        ]
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom [
            a owl:Restriction ;
            owl:onProperty ahmi:hasParameter ;
            owl:someValuesFrom ahmi:DaytimeCondition
        ]
    ] .

ahmi:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:comment "Relates a description to the parameters that constrain it."@en ;
    rdfs:domain ahmi:Description ;
    rdfs:range ahmi:EnvironmentFactor .