@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "A mental state related to cognitive processes and functions."@en .

ahmi:EEGSignal a owl:Class ;
    rdfs:label "EEG Signal"@en ;
    rdfs:comment "A signal recorded from an electroencephalogram (EEG) device."@en .

ahmi:indicatesState a owl:ObjectProperty ;
    rdfs:label "indicates state"@en ;
    rdfs:comment "Relates an EEG signal to the cognitive state it indicates."@en ;
    rdfs:domain ahmi:EEGSignal ;
    rdfs:range ahmi:CognitiveState .

ahmi:DistractionState a owl:Class ;
    rdfs:label "Distraction State"@en ;
    rdfs:comment "A cognitive state characterized by a lack of attention or focus."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:HighWorkloadState a owl:Class ;
    rdfs:label "High Workload State"@en ;
    rdfs:comment "A cognitive state characterized by high mental workload or cognitive strain."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:LowWorkloadState a owl:Class ;
    rdfs:label "Low Workload State"@en ;
    rdfs:comment "A cognitive state characterized by low mental workload or cognitive strain."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:FrustrationState a owl:Class ;
    rdfs:label "Frustration State"@en ;
    rdfs:comment "A cognitive state characterized by feelings of frustration or annoyance."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process or occurrence."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event, consistent with a description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:Event
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a situation to the event it observes or interprets."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:CognitiveStateDetection a owl:Class ;
    rdfs:label "Cognitive State Detection"@en ;
    rdfs:comment "A situation that involves detecting or interpreting a cognitive state based on EEG signals and other data."@en ;
    rdfs:subClassOf ahmi:Situation .

ahmi:hasEEGSignal a owl:ObjectProperty ;
    rdfs:label "has EEG signal"@en ;
    rdfs:comment "Relates a cognitive state detection situation to the EEG signals observed."@en ;
    rdfs:domain ahmi:CognitiveStateDetection ;
    rdfs:range ahmi:EEGSignal .

ahmi:hasDetectedState a owl:ObjectProperty ;
    rdfs:label "has detected state"@en ;
    rdfs:comment "Relates a cognitive state detection situation to the cognitive state detected."@en ;
    rdfs:domain ahmi:CognitiveStateDetection ;
    rdfs:range ahmi:CognitiveState .

ahmi:DriverStateAdaptation a owl:Class ;
    rdfs:label "Driver State Adaptation"@en ;
    rdfs:comment "A situation that involves adapting a vehicle system or interface based on the detected cognitive state of the driver."@en ;
    rdfs:subClassOf ahmi:Situation .

ahmi:hasAdaptationStrategy a owl:ObjectProperty ;
    rdfs:label "has adaptation strategy"@en ;
    rdfs:comment "Relates a driver state adaptation situation to the adaptation strategy employed."@en ;
    rdfs:domain ahmi:DriverStateAdaptation ;
    rdfs:range ahmi:AdaptationStrategy .

ahmi:AdaptationStrategy a owl:Class ;
    rdfs:label "Adaptation Strategy"@en ;
    rdfs:comment "A strategy for adapting a vehicle system or interface based on the detected cognitive state of the driver."@en .

ahmi:TactileFeedbackAdaptation a owl:Class ;
    rdfs:label "Tactile Feedback Adaptation"@en ;
    rdfs:comment "An adaptation strategy that involves adjusting tactile feedback elements."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:AudioFeedbackAdaptation a owl:Class ;
    rdfs:label "Audio Feedback Adaptation"@en ;
    rdfs:comment "An adaptation strategy that involves adjusting audio feedback elements."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:VisualFeedbackAdaptation a owl:Class ;
    rdfs:label "Visual Feedback Adaptation"@en ;
    rdfs:comment "An adaptation strategy that involves adjusting visual feedback elements."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:MultimodalIntegration a owl:Class ;
    rdfs:label "Multimodal Integration"@en ;
    rdfs:comment "A situation that involves integrating multiple modalities of feedback or interaction based on the detected cognitive state of the driver."@en ;
    rdfs:subClassOf ahmi:Situation .

ahmi:hasMultimodalStrategy a owl:ObjectProperty ;
    rdfs:label "has multimodal strategy"@en ;
    rdfs:comment "Relates a multimodal integration situation to the multimodal strategy employed."@en ;
    rdfs:domain ahmi:MultimodalIntegration ;
    rdfs:range ahmi:MultimodalStrategy .

ahmi:MultimodalStrategy a owl:Class ;
    rdfs:label "Multimodal Strategy"@en ;
    rdfs:comment "A strategy for integrating multiple modalities of feedback or interaction based on the detected cognitive state of the driver."@en .

ahmi:CrossmodalCongruence a owl:Class ;
    rdfs:label "Crossmodal Congruence"@en ;
    rdfs:comment "A multimodal strategy that aims to achieve congruence and consistency across different modalities of feedback or interaction."@en ;
    rdfs:subClassOf ahmi:MultimodalStrategy .