@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A trait or characteristic that influences an individual's behavior and emotional responses."@en .

ahmi:AggressiveDrivingTendency a owl:Class ;
    rdfs:label "Aggressive Driving Tendency"@en ;
    rdfs:subClassOf ahmi:PersonalityFactor ;
    rdfs:comment "A personality trait characterized by a tendency towards aggressive or reckless driving behavior."@en .

ahmi:LowConscientiousness a owl:Class ;
    rdfs:label "Low Conscientiousness"@en ;
    rdfs:subClassOf ahmi:PersonalityFactor ;
    rdfs:comment "A personality trait characterized by a lack of self-discipline, carefulness, and organization."@en .

ahmi:hasPersonalityFactor a owl:ObjectProperty ;
    rdfs:label "has personality factor"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:PersonalityFactor .

ahmi:Driver a owl:Class ;
    rdfs:label "Driver"@en ;
    rdfs:comment "An individual who operates a vehicle."@en .

ahmi:isAssociatedWith a owl:ObjectProperty ;
    rdfs:label "is associated with"@en ;
    rdfs:domain ahmi:PersonalityFactor ;
    rdfs:range ahmi:DrivingBehavior ;
    rdfs:comment "Relates a personality factor to a driving behavior that it influences."@en .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en ;
    rdfs:comment "The manner in which an individual operates a vehicle, including actions and responses."@en .

ahmi:AggressiveDriving a owl:Class ;
    rdfs:label "Aggressive Driving"@en ;
    rdfs:subClassOf ahmi:DrivingBehavior ;
    rdfs:comment "A driving behavior characterized by aggressive actions, such as sudden acceleration, hard braking, and erratic maneuvers."@en .

ahmi:AggressiveEvent a owl:Class ;
    rdfs:label "Aggressive Event"@en ;
    rdfs:comment "An event or occurrence involving aggressive driving behavior."@en .

dul:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view, consistent with ('satisfying') a Description, on a set of entities."@en .

ahmi:AggressiveDrivingSituation a owl:Class ;
    rdfs:label "Aggressive Driving Situation"@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:comment "A situation involving aggressive driving behavior, interpreted based on a description of aggressive driving events."@en .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:domain ahmi:AggressiveDrivingSituation ;
    rdfs:range ahmi:AggressiveEvent ;
    rdfs:comment "Relates a situation to the aggressive driving event it observes or interprets."@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range dul:Description ;
    rdfs:comment "Relates a situation to the description it satisfies."@en .

ahmi:AggressiveDrivingDescription a owl:Class ;
    rdfs:label "Aggressive Driving Description"@en ;
    rdfs:subClassOf dul:Description ;
    rdfs:comment "A conceptualization or description of what constitutes aggressive driving behavior."@en .