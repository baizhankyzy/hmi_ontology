@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:EmotionalState a owl:Class ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "A mental state characterized by emotions or feelings."@en ;
    rdfs:subClassOf ahmi:MentalState .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "A mental state related to cognitive processes such as perception, attention, memory, and reasoning."@en ;
    rdfs:subClassOf ahmi:MentalState .

ahmi:MentalState a owl:Class ;
    rdfs:label "Mental State"@en ;
    rdfs:comment "A state of mind or consciousness."@en .

ahmi:hasEmotionalState a owl:ObjectProperty ;
    rdfs:label "has emotional state"@en ;
    rdfs:comment "Relates an entity to its emotional state."@en ;
    rdfs:domain ahmi:Entity ;
    rdfs:range ahmi:EmotionalState .

ahmi:hasCognitiveState a owl:ObjectProperty ;
    rdfs:label "has cognitive state"@en ;
    rdfs:comment "Relates an entity to its cognitive state."@en ;
    rdfs:domain ahmi:Entity ;
    rdfs:range ahmi:CognitiveState .

ahmi:influencedBy a owl:ObjectProperty ;
    rdfs:label "influenced by"@en ;
    rdfs:comment "Relates a mental state to another mental state that influences it."@en ;
    rdfs:domain ahmi:MentalState ;
    rdfs:range ahmi:MentalState .

ahmi:EmotionalState rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ahmi:influencedBy ;
    owl:someValuesFrom ahmi:CognitiveState
] .

ahmi:CognitiveState rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ahmi:influencedBy ;
    owl:someValuesFrom ahmi:EmotionalState
] .

ahmi:Entity a owl:Class ;
    rdfs:label "Entity"@en ;
    rdfs:comment "Any physical, social, or mental object, event, or quality."@en .

ahmi:MentalStateEvent a owl:Class ;
    rdfs:label "Mental State Event"@en ;
    rdfs:comment "An event involving a change in mental state."@en ;
    rdfs:subClassOf ahmi:Event .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process."@en .

ahmi:MentalStateSituation a owl:Class ;
    rdfs:label "Mental State Situation"@en ;
    rdfs:comment "A situation representing a view or interpretation of a mental state event."@en ;
    rdfs:subClassOf ahmi:Situation .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event, consistent with a description."@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides a context for interpreting situations and events."@en .