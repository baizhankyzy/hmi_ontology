@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:EmotionalState rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "The emotional state of the driver, such as stress or calmness."@en .

ahmi:DriverState rdfs:subClassOf dul:Situation ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A situation representing the state of the driver, including cognitive, emotional, and physiological aspects."@en .

ahmi:CognitiveState rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The cognitive state of the driver, such as high cognitive load or distraction."@en .

ahmi:HighCognitiveLoad rdfs:subClassOf ahmi:CognitiveState ;
    rdfs:label "High Cognitive Load"@en ;
    rdfs:comment "A cognitive state characterized by elevated mental demand and stress."@en .

ahmi:DistractionState rdfs:subClassOf ahmi:CognitiveState ;
    rdfs:label "Distraction State"@en ;
    rdfs:comment "A cognitive state where the driver's attention is diverted from the driving task."@en .

ahmi:EnvironmentFactor rdfs:subClassOf dul:Entity ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "External factors that influence the driving situation, such as weather or traffic conditions."@en .

ahmi:HighTrafficCongestion rdfs:subClassOf ahmi:EnvironmentFactor ;
    rdfs:label "High Traffic Congestion"@en ;
    rdfs:comment "A situation where there is a high density of vehicles on the road, causing traffic congestion."@en .

ahmi:hasEmotionalState rdfs:subPropertyOf dul:isSettingFor ;
    rdfs:label "has emotional state"@en ;
    rdfs:comment "Relates a driver state situation to the emotional state of the driver."@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:EmotionalState .

ahmi:hasEnvironmentFactor rdfs:subPropertyOf dul:isSettingFor ;
    rdfs:label "has environment factor"@en ;
    rdfs:comment "Relates a situation to the environmental factors involved."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:HighCognitiveLoadSituation rdfs:subClassOf dul:Situation ;
    rdfs:label "High Cognitive Load Situation"@en ;
    rdfs:comment "A situation where the driver is experiencing high cognitive load."@en ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ahmi:hasEmotionalState ;
        owl:someValuesFrom ahmi:EmotionalState
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ahmi:hasEnvironmentFactor ;
        owl:someValuesFrom ahmi:HighTrafficCongestion
    ] .

ahmi:HighCognitiveLoadSituation dul:satisfies ahmi:HighCognitiveLoadDescription .

ahmi:HighCognitiveLoadDescription rdfs:subClassOf dul:Description ;
    rdfs:label "High Cognitive Load Description"@en ;
    rdfs:comment "A conceptualization of a situation where the driver is experiencing high cognitive load due to environmental factors like high traffic congestion."@en .