@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:Vehicle a owl:Class ;
    rdfs:label "Vehicle"@en ;
    rdfs:comment "A vehicle equipped with adaptive HMI systems"@en .

ahmi:Driver a owl:Class ;
    rdfs:label "Driver"@en ;
    rdfs:comment "A person operating a vehicle"@en .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A trait or characteristic of a person's personality"@en .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en ;
    rdfs:comment "The manner in which a driver operates a vehicle"@en .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "An external condition or circumstance that may influence driving"@en .

ahmi:EmotionalState a owl:Class ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "The emotional condition or mood of a driver"@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A distinct emotional state, such as frustration or anger"@en .

ahmi:ContinuousEmotiveState a owl:Class ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:label "Continuous Emotive State"@en ;
    rdfs:comment "An emotional state that varies along a continuum, such as arousal levels"@en .

ahmi:DetectionMethod a owl:Class ;
    rdfs:label "Detection Method"@en ;
    rdfs:comment "A technique or approach used to monitor and detect a driver's state"@en .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A sensory channel through which feedback is provided to the driver"@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual displays or elements"@en .

ahmi:AuditoryFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Auditory Feedback"@en ;
    rdfs:comment "Feedback provided through audio or speech"@en .

ahmi:TactileFeedback a owl:Class ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Feedback provided through touch or force feedback"@en .

ahmi:AdaptationStrategy a owl:Class ;
    rdfs:label "Adaptation Strategy"@en ;
    rdfs:comment "An approach or method used by the vehicle system to adapt feedback based on the driver's state"@en .

ahmi:hasPersonalityFactor a owl:ObjectProperty ;
    rdfs:label "has personality factor"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:PersonalityFactor .

ahmi:exhibitsDrivingBehavior a owl:ObjectProperty ;
    rdfs:label "exhibits driving behavior"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:DrivingBehavior .

ahmi:influencedByEnvironmentFactor a owl:ObjectProperty ;
    rdfs:label "influenced by environment factor"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:hasEmotionalState a owl:ObjectProperty ;
    rdfs:label "has emotional state"@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:EmotionalState .

ahmi:detectsEmotionalState a owl:ObjectProperty ;
    rdfs:label "detects emotional state"@en ;
    rdfs:domain ahmi:DetectionMethod ;
    rdfs:range ahmi:EmotionalState .

ahmi:providesFeedback a owl:ObjectProperty ;
    rdfs:label "provides feedback"@en ;
    rdfs:domain ahmi:AdaptationStrategy ;
    rdfs:range ahmi:FeedbackModality .

ahmi:Frustration a owl:Class ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState ;
    rdfs:label "Frustration"@en ;
    rdfs:comment "The emotional state of feeling frustrated or annoyed"@en .

ahmi:SteeringBehaviorAnalysis a owl:Class ;
    rdfs:subClassOf ahmi:DetectionMethod ;
    rdfs:label "Steering Behavior Analysis"@en ;
    rdfs:comment "A method that analyzes the driver's steering behavior to detect emotional states"@en .

ahmi:PedalControlAnalysis a owl:Class ;
    rdfs:subClassOf ahmi:DetectionMethod ;
    rdfs:label "Pedal Control Analysis"@en ;
    rdfs:comment "A method that analyzes the driver's pedal control behavior to detect emotional states"@en .

ahmi:SpeechRecognition a owl:Class ;
    rdfs:subClassOf ahmi:DetectionMethod ;
    rdfs:label "Speech Recognition"@en ;
    rdfs:comment "A method that analyzes the driver's speech to detect emotional states"@en .

ahmi:ContextualAnalysis a owl:Class ;
    rdfs:subClassOf ahmi:DetectionMethod ;
    rdfs:label "Contextual Analysis"@en ;
    rdfs:comment "A method that analyzes the driving context and environment to detect emotional states"@en .

ahmi:DetectionMethod rdfs:subClassOf dul:Situation ;
    rdfs:label "Detection Method"@en ;
    rdfs:comment "A situation that interprets events related to detecting a driver's emotional state"@en ;
    dul:satisfies ahmi:DetectionMethodDescription .

ahmi:DetectionMethodDescription a owl:Class ;
    rdfs:subClassOf dul:Description ;
    rdfs:label "Detection Method Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations related to detecting a driver's emotional state"@en .

ahmi:AdaptationStrategy rdfs:subClassOf dul:Situation ;
    rdfs:label "Adaptation Strategy"@en ;
    rdfs:comment "A situation that interprets events related to adapting feedback based on a driver's emotional state"@en ;
    dul:satisfies ahmi:AdaptationStrategyDescription .

ahmi:AdaptationStrategyDescription a owl:Class ;
    rdfs:subClassOf dul:Description ;
    rdfs:label "Adaptation Strategy Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations related to adapting feedback based on a driver's emotional state"@en .