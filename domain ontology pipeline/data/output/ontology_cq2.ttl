@prefix : <http://example.org/drowsiness-detection#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix dct: <http://purl.org/dc/terms/> .@prefix ssn: <http://www.w3.org/ns/ssn/> .@prefix prov: <http://www.w3.org/ns/prov#> .@prefix schema: <https://schema.org/> .# Ontology:Driver rdfs:label "Driver"@en ;        rdfs:comment "A person operating a vehicle."@en ;        dct:source "User Story" ;        rdfs:subClassOf [ a owl:Restriction ;                          owl:onProperty :hasParameter ;                          owl:someValuesFrom :DriverParameter                        ] .:DriverParameter rdfs:label "Driver Parameter"@en ;                 rdfs:comment "Parameters related to the driver's state."@en ;                 dct:source "User Story" ;                 rdfs:subClassOf [ a owl:Restriction ;                                   owl:onProperty :hasPart ;                                   owl:someValuesFrom :PostureState                                 ] .:PostureState rdfs:label "Posture State"@en ;              rdfs:comment "The state related to the driver's posture."@en ;              dct:source "User Story" ;              rdfs:subClassOf [ a owl:Restriction ;                                owl:onProperty :hasPart ;                                owl:someValuesFrom :HeadPosition                              ] .:HeadPosition rdfs:label "Head Position"@en ;               rdfs:comment "The position of the driver's head."@en ;               dct:source "User Story" ;               rdfs:subClassOf [ a owl:Restriction ;                                 owl:onProperty :hasProperty ;                                 owl:someValuesFrom :NoddingPattern                               ] .:NoddingPattern rdfs:label "Nodding Pattern"@en ;                rdfs:comment "A pattern of head nodding, indicating drowsiness."@en ;                dct:source "User Story" .:DetectionMethodology rdfs:label "Detection Methodology"@en ;                      rdfs:comment "A methodology for detecting drowsiness."@en ;                      dct:source "Competency Question" ;                      rdfs:subClassOf [ a owl:Restriction ;                                        owl:onProperty :requires ;                                        owl:someValuesFrom :Sensor                                      ] .:Sensor rdfs:label "Sensor"@en ;        rdfs:comment "A device used to observe and detect phenomena."@en ;        dct:source "User Story" ;        rdfs:subClassOf [ a owl:Restriction ;                          owl:onProperty ssn:observes ;                          owl:someValuesFrom :Drowsiness                        ] .:Drowsiness rdfs:label "Drowsiness"@en ;            rdfs:comment "A state of impaired alertness and reduced cognitive performance."@en ;            dct:source "User Story" ;            rdfs:subClassOf :CognitiveState .:CognitiveState rdfs:label "Cognitive State"@en ;                rdfs:comment "A state related to cognitive functions and mental processes."@en ;                dct:source "User Story" .:VisualSensor rdfs:label "Visual Sensor"@en ;              rdfs:comment "A sensor that captures visual data."@en ;              dct:source "User Story" ;              dct:relation "Sensor-Input Pattern" ;              rdfs:subClassOf :Sensor ,                              [ a owl:Restriction ;                                owl:onProperty ssn:observes ;                                owl:someValuesFrom :EyeTrackingMetric                              ] ,                              [ a owl:Restriction ;                                owl:onProperty ssn:madeObservation ;                                owl:someValuesFrom :ReducedAttention                              ] .:EyeTrackingMetric rdfs:label "Eye Tracking Metric"@en ;                   rdfs:comment "A metric related to eye movements and gaze patterns."@en ;                   dct:source "User Story" .:ReducedAttention rdfs:label "Reduced Attention"@en ;                  rdfs:comment "A state of decreased attentiveness and focus."@en ;                  dct:source "User Story" .:PhysiologicalSensor rdfs:label "Physiological Sensor"@en ;                     rdfs:comment "A sensor that measures physiological signals."@en ;                     dct:source "User Story" ;                     dct:relation "Sensor-Input Pattern" ;                     rdfs:subClassOf :Sensor ,                                     [ a owl:Restriction ;                                       owl:onProperty :hasPart ;                                       owl:someValuesFrom :Electroencephalogram                                     ] ,                                     [ a owl:Restriction ;                                       owl:onProperty :hasPart ;                                       owl:someValuesFrom :RespirationSensor                                     ] .:Electroencephalogram rdfs:label "Electroencephalogram (EEG)"@en ;                      rdfs:comment "A technique for measuring electrical activity in the brain."@en ;                      dct:source "User Story" ;                      rdfs:subClassOf [ a owl:Restriction ;                                        owl:onProperty ssn:observes ;                                        owl:someValuesFrom :Drowsiness                                      ] .:RespirationSensor rdfs:label "Respiration Sensor"@en ;                   rdfs:comment "A sensor that measures respiratory patterns."@en ;                   dct:source "User Story" ;                   rdfs:subClassOf [ a owl:Restriction ;                                     owl:onProperty ssn:observes ;                                     owl:someValuesFrom :Fatigue                                   ] .:Fatigue rdfs:label "Fatigue"@en ;         rdfs:comment "A state of physical or mental exhaustion."@en ;         dct:source "User Story" ;         rdfs:subClassOf :CognitiveState .:VehicleSystem rdfs:label "Vehicle System"@en ;               rdfs:comment "The system responsible for managing vehicle functions and interactions."@en ;               dct:source "User Story" ;               rdfs:subClassOf [ a owl:Restriction ;                                 owl:onProperty :provides ;                                 owl:someValuesFrom :Feedback                               ] .:Feedback rdfs:label "Feedback"@en ;          rdfs:comment "Information or signals provided to the driver."@en ;          dct:source "User Story" ;          rdfs:subClassOf [ a owl:Restriction ;                            owl:onProperty prov:wasInfluencedBy ;                            owl:someValuesFrom :DriverState                          ] .:DriverState rdfs:label "Driver State"@en ;             rdfs:comment "The current state or condition of the driver."@en ;             dct:source "User Story" .:VisualFeedback rdfs:label "Visual Feedback"@en ;                rdfs:comment "Feedback provided through visual displays or elements."@en ;                dct:source "User Story" ;                dct:relation "Actuator-Output Pattern" ;                rdfs:subClassOf :Feedback ,                                [ a owl:Restriction ;                                  owl:onProperty :hasPart ;                                  owl:someValuesFrom :DisplayType                                ] ,                                [ a owl:Restriction ;                                  owl:onProperty :hasPart ;                                  owl:someValuesFrom :VisualElement                                ] .:DisplayType rdfs:label "Display Type"@en ;             rdfs:comment "The type of display used for visual feedback."@en ;             dct:source "User Story" .:VisualElement rdfs:label "Visual Element"@en ;               rdfs:comment "A visual element or component used for feedback."@en ;               dct:source "User Story" ;               rdfs:subClassOf [ a owl:Restriction ;                                 owl:onProperty prov:wasInfluencedBy ;                                 owl:someValuesFrom :ContrastAdjustment                               ] .:ContrastAdjustment rdfs:label "Contrast Adjustment"@en ;                    rdfs:comment "An adjustment to the contrast of visual elements."@en ;                    dct:source "User Story" .:TactileFeedback rdfs:label "Tactile Feedback"@en ;                 rdfs:comment "Feedback provided through tactile or haptic means."@en ;                 dct:source "User Story" ;                 dct:relation "Actuator-Output Pattern" ;                 rdfs:subClassOf :Feedback ,                                 [ a owl:Restriction ;                                   owl:onProperty :hasPart ;                                   owl:someValuesFrom :HapticAlert                                 ] ,                                 [ a owl:Restriction ;                                   owl:onProperty :hasPart ;                                   owl:someValuesFrom :ForceFeedback                                 ] .:HapticAlert rdfs:label "Haptic Alert"@en ;             rdfs:comment "An alert or notification provided through haptic feedback."@en ;             dct:source "User Story" ;             rdfs:subClassOf [ a owl:Restriction ;                               owl:onProperty :isPartOf ;                               owl:someValuesFrom :SteeringWheel                             ] .:ForceFeedback rdfs:label "Force Feedback"@en ;               rdfs:comment "Feedback provided through force or resistance in a control interface."@en ;               dct:source "User Story" ;               rdfs:subClassOf [ a owl:Restriction ;                                 owl:onProperty prov:wasInfluencedBy ;                                 owl:someValuesFrom :DriverStateAdaptationRule                               ] .:DriverStateAdaptationRule rdfs:label "Driver State Adaptation Rule"@en ;                           rdfs:comment "A rule or logic for adapting feedback based on the driver's state."@en ;                           dct:source "User Story" .:SteeringWheel rdfs:label "Steering Wheel"@en ;               rdfs:comment "The steering control interface in a vehicle."@en ;               dct:source "User Story" .:AudioFeedback rdfs:label "Audio Feedback"@en ;               rdfs:comment "Feedback provided through auditory means."@en ;               dct:source "User Story" ;               dct:relation "Actuator-Output Pattern" ;               rdfs:subClassOf :Feedback ,                               [ a owl:Restriction ;                                 owl:onProperty :hasPart ;                                 owl:someValuesFrom :AuditoryDisplay                               ] .:AuditoryDisplay rdfs:label "Auditory Display"@en ;                 rdfs:comment "A display or interface for providing auditory feedback."@en ;                 dct:source "User Story" ;                 rdfs:subClassOf [ a owl:Restriction ;                                   owl:onProperty prov:generated ;                                   owl:someValuesFrom :IncreasedVolume                                 ] .:IncreasedVolume rdfs:label "Increased Volume"@en ;                 rdfs:comment "An increase in the volume or intensity of an auditory output."@en ;                 dct:source "User Story" .:DialogDesign rdfs:label .