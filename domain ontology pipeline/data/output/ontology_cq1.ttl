eras" ;    rdfs:comment "Cameras used for driver monitoring." ;    dct:source "User Story" ;    rdfs:subClassOf [        a owl:Restriction ;        owl:onProperty ssn:observes ;        owl:someValuesFrom :EyeTrackingMetrics    ] ;    rdfs:subClassOf [        a owl:Restriction ;        owl:onProperty ssn:madeObservation ;        owl:someValuesFrom :ReducedAttention    ] .:EyeTrackingMetrics    a owl:Class ;    rdfs:label "Eye Tracking Metrics" ;    rdfs:comment "Metrics related to eye tracking." ;    dct:source "User Story" .:ReducedAttention    a owl:Class ;    rdfs:label "Reduced Attention" ;    rdfs:comment "Observation of reduced driver attention." ;    dct:source "User Story" .:PhysiologicalSensors    a owl:Class ;    rdfs:label "Physiological Sensors" ;    rdfs:comment "Sensors monitoring physiological signals." ;    dct:source "User Story" ;    rdfs:subClassOf [        a owl:Restriction ;        owl:onProperty :hasPart ;        owl:someValuesFrom :Electroencephalogram    ] .:Electroencephalogram    a owl:Class ;    rdfs:label "Electroencephalogram (EEG)" ;    rdfs:comment "A sensor measuring brain activity." ;    dct:source "User Story" ;    rdfs:subClassOf [        a owl:Restriction ;        owl:onProperty ssn:observes ;        owl:someValuesFrom :Drowsiness    ] .:Drowsiness    a owl:Class ;    rdfs:label "Drowsiness" ;    rdfs:comment "A state of sleepiness or fatigue." ;    dct:source "User Story" ;    rdfs:subClassOf :CognitiveState .:CognitiveState    a owl:Class ;    rdfs:label "Cognitive State" ;    rdfs:comment "A state related to cognitive functions." ;    dct:source "User Story" .:RespirationSensors    a owl:Class ;    rdfs:label "Respiration Sensors" ;    rdfs:comment "Sensors monitoring respiration." ;    dct:source "User Story" ;    rdfs:subClassOf [        a owl:Restriction ;        owl:onProperty ssn:observes ;        owl:someValuesFrom :Fatigue    ] .:Fatigue    a owl:Class ;    rdfs:label "Fatigue" ;    rdfs:comment "A state of physical or me