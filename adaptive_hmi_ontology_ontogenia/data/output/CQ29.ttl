@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:FrustrationEvent a owl:Class ;
    rdfs:label "Frustration Event"@en ;
    rdfs:comment "An event representing a state of frustration experienced by a driver."@en ;
    rdfs:subClassOf dul:Event .

ahmi:ContextualAnalysis a owl:Class ;
    rdfs:label "Contextual Analysis"@en ;
    rdfs:comment "The process of analyzing contextual factors that may indicate a driver's frustration."@en ;
    rdfs:subClassOf dul:Situation ;
    dul:satisfies ahmi:FrustrationEventDescription .

ahmi:FrustrationEventDescription a owl:Class ;
    rdfs:label "Frustration Event Description"@en ;
    rdfs:comment "A conceptualization that provides a context for interpreting frustration events."@en ;
    rdfs:subClassOf dul:Description .

ahmi:TrafficCondition a owl:Class ;
    rdfs:label "Traffic Condition"@en ;
    rdfs:comment "The condition of traffic on the road."@en ;
    rdfs:subClassOf dul:Entity .

ahmi:HighCongestion a owl:Class ;
    rdfs:label "High Congestion"@en ;
    rdfs:comment "A traffic condition with high levels of congestion."@en ;
    rdfs:subClassOf ahmi:TrafficCondition .

ahmi:JourneyContext a owl:Class ;
    rdfs:label "Journey Context"@en ;
    rdfs:comment "The context of a driver's journey, including factors like expected time and route."@en ;
    rdfs:subClassOf dul:Situation .

ahmi:DeviationFromExpectedTimeOnTask a owl:Class ;
    rdfs:label "Deviation from Expected Time on Task"@en ;
    rdfs:comment "A situation where the actual time spent on a task deviates significantly from the expected time."@en ;
    rdfs:subClassOf ahmi:JourneyContext .

ahmi:BrakingBehaviorAnalysis a owl:Class ;
    rdfs:label "Braking Behavior Analysis"@en ;
    rdfs:comment "The analysis of a driver's braking behavior."@en ;
    rdfs:subClassOf dul:Situation .

ahmi:SuddenStop a owl:Class ;
    rdfs:label "Sudden Stop"@en ;
    rdfs:comment "An event where the vehicle comes to a sudden stop."@en ;
    rdfs:subClassOf dul:Event .

ahmi:requiresBrakingBehaviorAnalysis a owl:ObjectProperty ;
    rdfs:label "requires braking behavior analysis"@en ;
    rdfs:comment "Relates a sudden stop event to the braking behavior analysis situation that is required to interpret it."@en ;
    rdfs:domain ahmi:SuddenStop ;
    rdfs:range ahmi:BrakingBehaviorAnalysis .

ahmi:hasContextualFactor a owl:ObjectProperty ;
    rdfs:label "has contextual factor"@en ;
    rdfs:comment "Relates a contextual analysis situation to the contextual factors that are considered in the analysis."@en ;
    rdfs:domain ahmi:ContextualAnalysis ;
    rdfs:range dul:Entity .

ahmi:hasTrafficCondition a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasContextualFactor ;
    rdfs:label "has traffic condition"@en ;
    rdfs:comment "Relates a contextual analysis situation to the traffic condition that is considered."@en ;
    rdfs:domain ahmi:ContextualAnalysis ;
    rdfs:range ahmi:TrafficCondition .

ahmi:hasJourneyContext a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasContextualFactor ;
    rdfs:label "has journey context"@en ;
    rdfs:comment "Relates a contextual analysis situation to the journey context that is considered."@en ;
    rdfs:domain ahmi:ContextualAnalysis ;
    rdfs:range ahmi:JourneyContext .

ahmi:hasBrakingBehaviorAnalysis a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasContextualFactor ;
    rdfs:label "has braking behavior analysis"@en ;
    rdfs:comment "Relates a contextual analysis situation to the braking behavior analysis that is considered."@en ;
    rdfs:domain ahmi:ContextualAnalysis ;
    rdfs:range ahmi:BrakingBehaviorAnalysis .