@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:DialogDesign a owl:Class ;
    rdfs:label "Dialog Design"@en ;
    rdfs:comment "The design of the dialog system for interacting with the user."@en .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "A visual component of the user interface."@en ;
    rdfs:subClassOf ahmi:DialogDesign .

ahmi:DisplayType a owl:Class ;
    rdfs:label "Display Type"@en ;
    rdfs:comment "The type of display used for presenting information."@en ;
    rdfs:subClassOf ahmi:VisualElement .

ahmi:NotificationDisplay a owl:Class ;
    rdfs:label "Notification Display"@en ;
    rdfs:comment "The display for showing notifications to the user."@en ;
    rdfs:subClassOf ahmi:VisualElement .

ahmi:NavigationDisplay a owl:Class ;
    rdfs:label "Navigation Display"@en ;
    rdfs:comment "The display for showing navigation information."@en ;
    rdfs:subClassOf ahmi:VisualElement .

ahmi:VisualComplexity a owl:Class ;
    rdfs:label "Visual Complexity"@en ;
    rdfs:comment "The level of complexity in the visual presentation."@en ;
    rdfs:subClassOf ahmi:DialogDesign .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "The audio feedback provided to the user."@en ;
    rdfs:subClassOf ahmi:DialogDesign .

ahmi:VoiceTone a owl:Class ;
    rdfs:label "Voice Tone"@en ;
    rdfs:comment "The tone of the voice used for audio feedback."@en ;
    rdfs:subClassOf ahmi:AudioFeedback .

ahmi:Wording a owl:Class ;
    rdfs:label "Wording"@en ;
    rdfs:comment "The choice of words used for audio feedback."@en ;
    rdfs:subClassOf ahmi:AudioFeedback .

ahmi:AudioParameters a owl:Class ;
    rdfs:label "Audio Parameters"@en ;
    rdfs:comment "The parameters for controlling audio feedback."@en ;
    rdfs:subClassOf ahmi:AudioFeedback .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The cognitive state of the driver."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The overall state of the driver."@en ;
    rdfs:equivalentClass [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:DriverStateDescription
    ] .

ahmi:DriverStateDescription a owl:Class ;
    rdfs:label "Driver State Description"@en ;
    rdfs:comment "A description that provides context for interpreting the driver's state."@en ;
    rdfs:subClassOf dul:Description .

dul:satisfies a owl:ObjectProperty .

ahmi:hasDialogDesign a owl:ObjectProperty ;
    rdfs:label "has dialog design"@en ;
    rdfs:comment "Relates a driver state to the dialog design that should be used."@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DialogDesign .

ahmi:hasVisualComplexity a owl:ObjectProperty ;
    rdfs:label "has visual complexity"@en ;
    rdfs:comment "Relates a dialog design to its level of visual complexity."@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:VisualComplexity .

ahmi:hasVisualElement a owl:ObjectProperty ;
    rdfs:label "has visual element"@en ;
    rdfs:comment "Relates a dialog design to its visual elements."@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:VisualElement .

ahmi:hasAudioFeedback a owl:ObjectProperty ;
    rdfs:label "has audio feedback"@en ;
    rdfs:comment "Relates a dialog design to its audio feedback."@en ;
    rdfs:domain ahmi:DialogDesign ;
    rdfs:range ahmi:AudioFeedback .

ahmi:HighCognitiveLoad a owl:Class ;
    rdfs:label "High Cognitive Load"@en ;
    rdfs:comment "A cognitive state characterized by high cognitive load."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:HighCognitiveLoadSituation a owl:Class ;
    rdfs:label "High Cognitive Load Situation"@en ;
    rdfs:comment "A situation where the driver is experiencing high cognitive load."@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:HighCognitiveLoadDescription
    ] .

ahmi:HighCognitiveLoadDescription a owl:Class ;
    rdfs:label "High Cognitive Load Description"@en ;
    rdfs:comment "A description that provides context for interpreting a high cognitive load situation."@en ;
    rdfs:subClassOf dul:Description .

ahmi:SimplifiedDialogDesign a owl:Class ;
    rdfs:label "Simplified Dialog Design"@en ;
    rdfs:comment "A dialog design that is simplified for high cognitive load situations."@en ;
    rdfs:subClassOf ahmi:DialogDesign ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasVisualComplexity ;
        owl:hasValue ahmi:LowVisualComplexity
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasVisualElement ;
        owl:allValuesFrom ahmi:EssentialVisualElement
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasAudioFeedback ;
        owl:someValuesFrom ahmi:CalmVoiceTone
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasAudioFeedback ;
        owl:someValuesFrom ahmi:ConciseWording
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasAudioFeedback ;
        owl:someValuesFrom ahmi:LowAudioParameters
    ] .

ahmi:LowVisualComplexity a owl:Class ;
    rdfs:label "Low Visual Complexity"@en ;
    rdfs:comment "A low level of visual complexity."@en ;
    rdfs:subClassOf ahmi:VisualComplexity .

ahmi:EssentialVisualElement a owl:Class ;
    rdfs:label "Essential Visual Element"@en ;
    rdfs:comment "A visual element that is essential and should be displayed."@en ;
    rdfs:subClassOf ahmi:VisualElement .

ahmi:CalmVoiceTone a owl:Class ;
    rdfs:label "Calm Voice Tone"@en ;
    rdfs:comment "A calm tone of voice for audio feedback."@en ;
    rdfs:subClassOf ahmi:VoiceTone .

ahmi:ConciseWording a owl:Class ;
    rdfs:label "Concise Wording"@en ;
    rdfs:comment "Concise wording for audio feedback."@en ;
    rdfs:subClassOf ahmi:Wording .

ahmi:LowAudioParameters a owl:Class ;
    rdfs:label "Low Audio Parameters"@en ;
    rdfs:comment "Low audio parameters for audio feedback."@en ;
    rdfs:subClassOf ahmi:AudioParameters .

ahmi:hasHighCognitiveLoadDialogDesign a owl:ObjectProperty ;
    rdfs:label "has high cognitive load dialog design"@en ;
    rdfs:comment "Relates a high cognitive load situation to the appropriate dialog design."@en ;
    rdfs:domain ahmi:HighCognitiveLoadSituation ;
    rdfs:range ahmi:SimplifiedDialogDesign .