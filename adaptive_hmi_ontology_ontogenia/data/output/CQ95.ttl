@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:comment "A distinct emotional state experienced by a driver."@en .

ahmi:DetectionMethod a owl:Class ;
    rdfs:label "Detection Method"@en ;
    rdfs:comment "A method or technique used to detect a driver's emotional state."@en .

ahmi:detects a owl:ObjectProperty ;
    rdfs:label "detects"@en ;
    rdfs:comment "Relates a detection method to the emotive state it can detect."@en ;
    rdfs:domain ahmi:DetectionMethod ;
    rdfs:range ahmi:DiscreteEmotiveState .

ahmi:Anger a owl:Class ;
    rdfs:label "Anger"@en ;
    rdfs:comment "The emotional state of anger."@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:Frustration a owl:Class ;
    rdfs:label "Frustration"@en ;
    rdfs:comment "The emotional state of frustration."@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:Happiness a owl:Class ;
    rdfs:label "Happiness"@en ;
    rdfs:comment "The emotional state of happiness."@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:Sadness a owl:Class ;
    rdfs:label "Sadness"@en ;
    rdfs:comment "The emotional state of sadness."@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:FacialExpressionDetection a owl:Class ;
    rdfs:label "Facial Expression Detection"@en ;
    rdfs:comment "A method that detects emotive states based on facial expressions."@en ;
    rdfs:subClassOf ahmi:DetectionMethod .

ahmi:VoiceAnalysis a owl:Class ;
    rdfs:label "Voice Analysis"@en ;
    rdfs:comment "A method that detects emotive states based on voice patterns."@en ;
    rdfs:subClassOf ahmi:DetectionMethod .

ahmi:PhysiologicalMonitoring a owl:Class ;
    rdfs:label "Physiological Monitoring"@en ;
    rdfs:comment "A method that detects emotive states based on physiological signals."@en ;
    rdfs:subClassOf ahmi:DetectionMethod .

ahmi:detectsFacialExpression a owl:ObjectProperty ;
    rdfs:label "detects facial expression"@en ;
    rdfs:comment "Relates a facial expression detection method to the emotive states it can detect."@en ;
    rdfs:subPropertyOf ahmi:detects ;
    rdfs:domain ahmi:FacialExpressionDetection ;
    rdfs:range ahmi:DiscreteEmotiveState .

ahmi:detectsVoicePattern a owl:ObjectProperty ;
    rdfs:label "detects voice pattern"@en ;
    rdfs:comment "Relates a voice analysis method to the emotive states it can detect."@en ;
    rdfs:subPropertyOf ahmi:detects ;
    rdfs:domain ahmi:VoiceAnalysis ;
    rdfs:range ahmi:DiscreteEmotiveState .

ahmi:detectsPhysiologicalSignal a owl:ObjectProperty ;
    rdfs:label "detects physiological signal"@en ;
    rdfs:comment "Relates a physiological monitoring method to the emotive states it can detect."@en ;
    rdfs:subPropertyOf ahmi:detects ;
    rdfs:domain ahmi:PhysiologicalMonitoring ;
    rdfs:range ahmi:DiscreteEmotiveState .

ahmi:EmotiveEvent a owl:Class ;
    rdfs:label "Emotive Event"@en ;
    rdfs:comment "An event involving a driver experiencing a discrete emotive state."@en .

ahmi:EmotiveSituation a owl:Class ;
    rdfs:label "Emotive Situation"@en ;
    rdfs:comment "A situation representing a view or interpretation of an emotive event."@en ;
    rdfs:subClassOf ahmi:Situation .

ahmi:EmotiveDescription a owl:Class ;
    rdfs:label "Emotive Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting emotive situations."@en ;
    rdfs:subClassOf ahmi:Description .

ahmi:hasEmotiveEvent a owl:ObjectProperty ;
    rdfs:label "has emotive event"@en ;
    rdfs:comment "Relates an emotive situation to the emotive event it represents."@en ;
    rdfs:domain ahmi:EmotiveSituation ;
    rdfs:range ahmi:EmotiveEvent .

ahmi:satisfiesEmotiveDescription a owl:ObjectProperty ;
    rdfs:label "satisfies emotive description"@en ;
    rdfs:comment "Relates an emotive situation to the emotive description it satisfies."@en ;
    rdfs:subPropertyOf ahmi:satisfies ;
    rdfs:domain ahmi:EmotiveSituation ;
    rdfs:range ahmi:EmotiveDescription .

ahmi:involvesEmotiveState a owl:ObjectProperty ;
    rdfs:label "involves emotive state"@en ;
    rdfs:comment "Relates an emotive event to the discrete emotive state involved."@en ;
    rdfs:domain ahmi:EmotiveEvent ;
    rdfs:range ahmi:DiscreteEmotiveState .