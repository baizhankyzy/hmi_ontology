@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:CrossModalCongruenceAssessment a owl:Class ;
    rdfs:label "Cross-modal congruence assessment"@en ;
    rdfs:comment "The process of evaluating the consistency and coherence of feedback across multiple modalities (e.g., visual, auditory, and tactile) in an ADAS system."@en ;
    rdfs:subClassOf ahmi:ADASEvaluation .

ahmi:ADASEvaluation a owl:Class ;
    rdfs:label "ADAS evaluation"@en ;
    rdfs:comment "The process of assessing the performance and compliance of Advanced Driver Assistance Systems (ADAS) with safety regulations and user experience factors."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual feedback"@en ;
    rdfs:comment "The visual elements and displays used to provide information or alerts to the driver."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AuditoryFeedback a owl:Class ;
    rdfs:label "Auditory feedback"@en ;
    rdfs:comment "The auditory signals or tones used to provide information or alerts to the driver."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile feedback"@en ;
    rdfs:comment "The tactile or haptic feedback mechanisms, such as force feedback through the steering wheel, used to provide information or alerts to the driver."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback modality"@en ;
    rdfs:comment "A sensory channel or mode through which information or alerts are conveyed to the driver."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver state"@en ;
    rdfs:comment "The cognitive, emotional, or behavioral condition of the driver that may influence the effectiveness of ADAS feedback and interventions."@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete emotive state"@en ;
    rdfs:comment "A specific emotional state experienced by the driver, such as confusion or frustration."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:Confusion a owl:Class ;
    rdfs:label "Confusion"@en ;
    rdfs:comment "A state of bewilderment or lack of understanding experienced by the driver."@en ;
    rdfs:subClassOf ahmi:DiscreteEmotiveState .

ahmi:SpeedViolationTendency a owl:Class ;
    rdfs:label "Speed violation tendency"@en ;
    rdfs:comment "A driver's propensity or inclination to exceed posted speed limits."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DialogDesign a owl:Class ;
    rdfs:label "Dialog design"@en ;
    rdfs:comment "The structure and flow of the interaction between the ADAS system and the driver, including the presentation of information and prompts."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:MessageStructure a owl:Class ;
    rdfs:label "Message structure"@en ;
    rdfs:comment "The organization and formatting of the messages or alerts presented to the driver by the ADAS system."@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy .

ahmi:AdaptationStrategy a owl:Class ;
    rdfs:label "Adaptation strategy"@en ;
    rdfs:comment "An approach or technique used by the ADAS system to adjust its behavior or feedback based on the driver's state or context."@en .

ahmi:hasVisualFeedback a owl:ObjectProperty ;
    rdfs:label "has visual feedback"@en ;
    rdfs:comment "Relates an ADAS evaluation to the visual feedback elements being assessed."@en ;
    rdfs:domain ahmi:CrossModalCongruenceAssessment ;
    rdfs:range ahmi:VisualFeedback .

ahmi:hasAuditoryFeedback a owl:ObjectProperty ;
    rdfs:label "has auditory feedback"@en ;
    rdfs:comment "Relates an ADAS evaluation to the auditory feedback elements being assessed."@en ;
    rdfs:domain ahmi:CrossModalCongruenceAssessment ;
    rdfs:range ahmi:AuditoryFeedback .

ahmi:hasTactileFeedback a owl:ObjectProperty ;
    rdfs:label "has tactile feedback"@en ;
    rdfs:comment "Relates an ADAS evaluation to the tactile feedback elements being assessed."@en ;
    rdfs:domain ahmi:CrossModalCongruenceAssessment ;
    rdfs:range ahmi:TactileFeedback .

ahmi:evaluatesDriverState a owl:ObjectProperty ;
    rdfs:label "evaluates driver state"@en ;
    rdfs:comment "Relates an ADAS evaluation to the driver states being considered or simulated."@en ;
    rdfs:domain ahmi:ADASEvaluation ;
    rdfs:range ahmi:DriverState .

ahmi:adaptsDialogDesign a owl:ObjectProperty ;
    rdfs:label "adapts dialog design"@en ;
    rdfs:comment "Relates an adaptation strategy to the dialog design elements being adjusted based on the driver's state or context."@en ;
    rdfs:domain ahmi:AdaptationStrategy ;
    rdfs:range ahmi:DialogDesign .

ahmi:adaptsMessageStructure a owl:ObjectProperty ;
    rdfs:label "adapts message structure"@en ;
    rdfs:comment "Relates an adaptation strategy to the message structure elements being adjusted based on the driver's state or context."@en ;
    rdfs:domain ahmi:AdaptationStrategy ;
    rdfs:range ahmi:MessageStructure .

ahmi:hasAdaptationStrategy a owl:ObjectProperty ;
    rdfs:label "has adaptation strategy"@en ;
    rdfs:comment "Relates an ADAS evaluation to the adaptation strategies being assessed or employed."@en ;
    rdfs:domain ahmi:ADASEvaluation ;
    rdfs:range ahmi:AdaptationStrategy .

ahmi:urgencyBasedTiming a owl:DatatypeProperty ;
    rdfs:label "urgency-based timing"@en ;
    rdfs:comment "The timing of alerts or feedback based on the urgency or criticality of the situation."@en ;
    rdfs:domain ahmi:CrossModalCongruenceAssessment ;
    rdfs:range xsd:duration .

ahmi:trustCalibrationEffectiveness a owl:DatatypeProperty ;
    rdfs:label "trust calibration effectiveness"@en ;
    rdfs:comment "A measure of how effectively the ADAS system conveys appropriate levels of confidence or trust to the driver."@en ;
    rdfs:domain ahmi:ADASEvaluation ;
    rdfs:range xsd:float .