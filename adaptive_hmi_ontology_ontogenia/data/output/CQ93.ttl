@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:MeasurementMetric a owl:Class ;
    rdfs:label "Measurement Metric"@en ;
    rdfs:comment "A metric used to quantify or evaluate a detection methodology."@en .

ahmi:DetectionMethodology a owl:Class ;
    rdfs:label "Detection Methodology"@en ;
    rdfs:comment "A method or approach for detecting or identifying a particular condition or event."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
        owl:onProperty ahmi:usesMeasurementMetric ;
        owl:someValuesFrom ahmi:MeasurementMetric
    ] .

ahmi:usesMeasurementMetric a owl:ObjectProperty ;
    rdfs:label "uses measurement metric"@en ;
    rdfs:comment "Relates a detection methodology to the measurement metrics it employs."@en ;
    rdfs:domain ahmi:DetectionMethodology ;
    rdfs:range ahmi:MeasurementMetric .

ahmi:Accuracy a owl:Class ;
    rdfs:label "Accuracy"@en ;
    rdfs:comment "A measurement metric that quantifies the correctness or precision of a detection methodology."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:Precision a owl:Class ;
    rdfs:label "Precision"@en ;
    rdfs:comment "A measurement metric that quantifies the proportion of true positive results among all positive results."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:Recall a owl:Class ;
    rdfs:label "Recall"@en ;
    rdfs:comment "A measurement metric that quantifies the proportion of true positive results among all actual positive cases."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:F1Score a owl:Class ;
    rdfs:label "F1 Score"@en ;
    rdfs:comment "A measurement metric that combines precision and recall into a single score, representing the harmonic mean of the two."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:ResponseTime a owl:Class ;
    rdfs:label "Response Time"@en ;
    rdfs:comment "A measurement metric that quantifies the time taken by a detection methodology to respond or provide a result."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:ComputationalComplexity a owl:Class ;
    rdfs:label "Computational Complexity"@en ;
    rdfs:comment "A measurement metric that quantifies the computational resources required by a detection methodology, such as time or space complexity."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:SensorAccuracy a owl:Class ;
    rdfs:label "Sensor Accuracy"@en ;
    rdfs:comment "A measurement metric that quantifies the accuracy or precision of the sensors used in a detection methodology."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .

ahmi:SensorResolution a owl:Class ;
    rdfs:label "Sensor Resolution"@en ;
    rdfs:comment "A measurement metric that quantifies the level of detail or granularity provided by the sensors used in a detection methodology."@en ;
    rdfs:subClassOf ahmi:MeasurementMetric .