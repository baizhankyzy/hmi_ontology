@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual modalities in a vehicle HMI system."@en .

ahmi:HeadUpDisplay a owl:Class ;
    rdfs:label "Head-Up Display"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "A visual feedback system that projects information onto the windshield or a transparent display in the driver's line of sight."@en .

ahmi:InstrumentCluster a owl:Class ;
    rdfs:label "Instrument Cluster"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "A visual feedback system that displays vehicle information and status indicators on a dashboard panel."@en .

ahmi:CenterConsoleDisplay a owl:Class ;
    rdfs:label "Center Console Display"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "A visual feedback system that provides information and controls through a display integrated into the center console."@en .

ahmi:AmbientLighting a owl:Class ;
    rdfs:label "Ambient Lighting"@en ;
    rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:comment "A visual feedback system that uses colored lighting in the vehicle interior to convey information or alerts."@en .

ahmi:providesVisualFeedback a owl:ObjectProperty ;
    rdfs:label "provides visual feedback"@en ;
    rdfs:comment "Relates a vehicle HMI system to the visual feedback modalities it employs."@en ;
    rdfs:domain ahmi:VehicleHMISystem ;
    rdfs:range ahmi:VisualFeedback .

ahmi:VehicleHMISystem a owl:Class ;
    rdfs:label "Vehicle HMI System"@en ;
    rdfs:comment "The human-machine interface system in a vehicle that provides feedback and controls."@en .

ahmi:VisualFeedbackSituation a owl:Class ;
    rdfs:label "Visual Feedback Situation"@en ;
    rdfs:comment "A situation representing a specific instance or view of visual feedback provided by the vehicle HMI system."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:providesVisualFeedback ;
        owl:someValuesFrom ahmi:VisualFeedback
    ] .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view, consistent with a Description, on a set of entities."@en .

ahmi:hasVisualFeedbackType a owl:ObjectProperty ;
    rdfs:label "has visual feedback type"@en ;
    rdfs:comment "Relates a Visual Feedback Situation to the specific type of visual feedback employed."@en ;
    rdfs:domain ahmi:VisualFeedbackSituation ;
    rdfs:range ahmi:VisualFeedback .

ahmi:isVisualFeedbackFor a owl:ObjectProperty ;
    rdfs:label "is visual feedback for"@en ;
    rdfs:comment "Relates a Visual Feedback Situation to the Vehicle HMI System it provides feedback for."@en ;
    rdfs:domain ahmi:VisualFeedbackSituation ;
    rdfs:range ahmi:VehicleHMISystem .