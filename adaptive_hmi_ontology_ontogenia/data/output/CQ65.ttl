@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:DaytimeCondition a owl:Class ;
    rdfs:label "Daytime Condition"@en ;
    rdfs:comment "Environmental conditions related to the time of day."@en .

ahmi:DayTime a owl:Class ;
    rdfs:label "Day Time"@en ;
    rdfs:subClassOf ahmi:DaytimeCondition .

ahmi:NightTime a owl:Class ;
    rdfs:label "Night Time"@en ;
    rdfs:subClassOf ahmi:DaytimeCondition ;
    owl:disjointWith ahmi:DayTime .

ahmi:DisplayType a owl:Class ;
    rdfs:label "Display Type"@en ;
    rdfs:comment "A type of display used in the vehicle's HMI."@en .

ahmi:DisplayBrightness a owl:Class ;
    rdfs:label "Display Brightness"@en ;
    rdfs:subClassOf ahmi:DisplayType .

ahmi:DisplayContrast a owl:Class ;
    rdfs:label "Display Contrast"@en ;
    rdfs:subClassOf ahmi:DisplayType .

ahmi:affectsBrightnessParameter a owl:ObjectProperty ;
    rdfs:label "affects brightness parameter"@en ;
    rdfs:comment "Relates a daytime condition to the brightness parameter it affects."@en ;
    rdfs:domain ahmi:DaytimeCondition ;
    rdfs:range ahmi:DisplayBrightness .

ahmi:BrightnessParameter a owl:Class ;
    rdfs:label "Brightness Parameter"@en ;
    rdfs:comment "A parameter that constrains the brightness of a display."@en .

ahmi:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:comment "Relates a display type to its parameters."@en ;
    rdfs:domain ahmi:DisplayType ;
    rdfs:range ahmi:BrightnessParameter .

ahmi:DaytimeConditionSituation a owl:Class ;
    rdfs:label "Daytime Condition Situation"@en ;
    rdfs:comment "A situation representing a specific daytime condition."@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:observes ;
        owl:someValuesFrom ahmi:DaytimeCondition
    ] .

ahmi:observes a owl:ObjectProperty ;
    rdfs:label "observes"@en ;
    rdfs:comment "Relates a situation to the event or condition it observes."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "An occurrence or process."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of an event or condition."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations."@en .