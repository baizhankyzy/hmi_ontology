@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "Factors related to the external environment that may influence driver state and vehicle behavior."@en .

ahmi:WeatherCondition a owl:Class ;
    rdfs:label "Weather Condition"@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:TrafficCondition a owl:Class ;
    rdfs:label "Traffic Condition"@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:RoadType a owl:Class ;
    rdfs:label "Road Type"@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The cognitive, emotional, and physiological state of the driver."@en .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:EmotionalState a owl:Class ;
    rdfs:label "Emotional State"@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:DistractionState a owl:Class ;
    rdfs:label "Distraction State"@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:HighCognitiveLoad a owl:Class ;
    rdfs:label "High Cognitive Load"@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:HighStress a owl:Class ;
    rdfs:label "High Stress"@en ;
    rdfs:subClassOf ahmi:EmotionalState .

ahmi:DriverStateDetection a owl:Class ;
    rdfs:label "Driver State Detection"@en ;
    rdfs:comment "The process of detecting the driver's cognitive, emotional, and physiological state."@en .

ahmi:SensorData a owl:Class ;
    rdfs:label "Sensor Data"@en ;
    rdfs:comment "Data from various sensors used to detect the driver's state."@en .

ahmi:PhysiologicalSensorData a owl:Class ;
    rdfs:label "Physiological Sensor Data"@en ;
    rdfs:subClassOf ahmi:SensorData .

ahmi:BehavioralSensorData a owl:Class ;
    rdfs:label "Behavioral Sensor Data"@en ;
    rdfs:subClassOf ahmi:SensorData .

ahmi:EnvironmentSensorData a owl:Class ;
    rdfs:label "Environment Sensor Data"@en ;
    rdfs:subClassOf ahmi:SensorData .

ahmi:ContextualAnalysis a owl:Class ;
    rdfs:label "Contextual Analysis"@en ;
    rdfs:comment "The process of analyzing the context, including environmental factors and sensor data, to determine the driver's state."@en .

ahmi:HMIAdaptation a owl:Class ;
    rdfs:label "HMI Adaptation"@en ;
    rdfs:comment "The process of adapting the Human-Machine Interface (HMI) based on the detected driver state."@en .

ahmi:VisualFeedbackAdaptation a owl:Class ;
    rdfs:label "Visual Feedback Adaptation"@en ;
    rdfs:subClassOf ahmi:HMIAdaptation .

ahmi:AudioFeedbackAdaptation a owl:Class ;
    rdfs:label "Audio Feedback Adaptation"@en ;
    rdfs:subClassOf ahmi:HMIAdaptation .

ahmi:ADASAdaptation a owl:Class ;
    rdfs:label "ADAS Adaptation"@en ;
    rdfs:subClassOf ahmi:HMIAdaptation .

ahmi:hasEnvironmentFactor a owl:ObjectProperty ;
    rdfs:label "has environment factor"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DriverState .

ahmi:detectedBy a owl:ObjectProperty ;
    rdfs:label "detected by"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DriverStateDetection .

ahmi:usesSensorData a owl:ObjectProperty ;
    rdfs:label "uses sensor data"@en ;
    rdfs:domain ahmi:DriverStateDetection ;
    rdfs:range ahmi:SensorData .

ahmi:performsContextualAnalysis a owl:ObjectProperty ;
    rdfs:label "performs contextual analysis"@en ;
    rdfs:domain ahmi:DriverStateDetection ;
    rdfs:range ahmi:ContextualAnalysis .

ahmi:triggersAdaptation a owl:ObjectProperty ;
    rdfs:label "triggers adaptation"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:HMIAdaptation .

ahmi:hasVisualFeedbackAdaptation a owl:ObjectProperty ;
    rdfs:label "has visual feedback adaptation"@en ;
    rdfs:domain ahmi:HMIAdaptation ;
    rdfs:range ahmi:VisualFeedbackAdaptation .

ahmi:hasAudioFeedbackAdaptation a owl:ObjectProperty ;
    rdfs:label "has audio feedback adaptation"@en ;
    rdfs:domain ahmi:HMIAdaptation ;
    rdfs:range ahmi:AudioFeedbackAdaptation .

ahmi:hasADASAdaptation a owl:ObjectProperty ;
    rdfs:label "has ADAS adaptation"@en ;
    rdfs:domain ahmi:HMIAdaptation ;
    rdfs:range ahmi:ADASAdaptation .

ahmi:DriverStateDetectionAccuracy a owl:Class ;
    rdfs:label "Driver State Detection Accuracy"@en ;
    rdfs:comment "The accuracy of detecting the driver's cognitive, emotional, and physiological state."@en .

ahmi:influencedBy a owl:ObjectProperty ;
    rdfs:label "influenced by"@en ;
    rdfs:domain ahmi:DriverStateDetectionAccuracy ;
    rdfs:range ahmi:EnvironmentFactor .