@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:AutonomousToManualTransition a owl:Class ;
    rdfs:label "Autonomous to Manual Transition"@en ;
    rdfs:comment "The process of transitioning from autonomous driving mode to manual driving mode."@en ;
    rdfs:subClassOf dul:Event .

ahmi:VisualFeedbackElement a owl:Class ;
    rdfs:label "Visual Feedback Element"@en ;
    rdfs:comment "A visual element used to provide feedback during the autonomous to manual transition."@en .

ahmi:AmbientLighting a owl:Class ;
    rdfs:label "Ambient Lighting"@en ;
    rdfs:comment "The ambient lighting in the vehicle cabin."@en ;
    rdfs:subClassOf ahmi:VisualFeedbackElement .

ahmi:DrivingInterface a owl:Class ;
    rdfs:label "Driving Interface"@en ;
    rdfs:comment "The visual interface displaying driving-related information."@en ;
    rdfs:subClassOf ahmi:VisualFeedbackElement .

ahmi:HighlightedVisualElement a owl:Class ;
    rdfs:label "Highlighted Visual Element"@en ;
    rdfs:comment "Visual elements that are highlighted or emphasized."@en ;
    rdfs:subClassOf ahmi:VisualFeedbackElement .

ahmi:CountdownTimer a owl:Class ;
    rdfs:label "Countdown Timer"@en ;
    rdfs:comment "A visual element displaying a countdown timer."@en ;
    rdfs:subClassOf ahmi:VisualFeedbackElement .

ahmi:LaneDetectionVisualization a owl:Class ;
    rdfs:label "Lane Detection Visualization"@en ;
    rdfs:comment "A visual representation of the detected lanes on the road."@en ;
    rdfs:subClassOf ahmi:VisualFeedbackElement .

ahmi:TransitionSituation a owl:Class ;
    rdfs:label "Transition Situation"@en ;
    rdfs:comment "A situation representing the view or interpretation of the autonomous to manual transition event."@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:TransitionDescription
    ] .

ahmi:TransitionDescription a owl:Class ;
    rdfs:label "Transition Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting the autonomous to manual transition situation."@en ;
    rdfs:subClassOf dul:Description .

ahmi:hasVisualFeedbackElement a owl:ObjectProperty ;
    rdfs:label "has Visual Feedback Element"@en ;
    rdfs:comment "Relates a transition situation to the visual feedback elements used during the transition."@en ;
    rdfs:domain ahmi:TransitionSituation ;
    rdfs:range ahmi:VisualFeedbackElement .

ahmi:isImportantFor a owl:ObjectProperty ;
    rdfs:label "is important for"@en ;
    rdfs:comment "Relates a visual feedback element to the transition situation for which it is important."@en ;
    rdfs:domain ahmi:VisualFeedbackElement ;
    rdfs:range ahmi:TransitionSituation ;
    owl:inverseOf ahmi:hasVisualFeedbackElement .