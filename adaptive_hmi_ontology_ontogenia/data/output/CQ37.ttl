@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:AdaptiveHMI a owl:Class ;
    rdfs:label "Adaptive HMI"@en ;
    rdfs:comment "A human-machine interface that adapts its feedback and behavior based on driver state, vehicle context, and environmental factors."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The cognitive, physical, and emotional state of the driver."@en .

ahmi:CognitiveState a owl:Class ;
    rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The cognitive abilities and mental workload of the driver."@en .

ahmi:hasReactionTime a owl:DatatypeProperty ;
    rdfs:label "has reaction time"@en ;
    rdfs:domain ahmi:CognitiveState ;
    rdfs:range xsd:duration .

ahmi:JourneyContext a owl:Class ;
    rdfs:label "Journey Context"@en ;
    rdfs:comment "The environmental and situational context of the journey."@en .

ahmi:hasFamiliarity a owl:DatatypeProperty ;
    rdfs:label "has familiarity"@en ;
    rdfs:domain ahmi:JourneyContext ;
    rdfs:range xsd:boolean .

ahmi:EnvironmentalCondition a owl:Class ;
    rdfs:label "Environmental Condition"@en ;
    rdfs:comment "The external conditions affecting the journey, such as weather and road type."@en .

ahmi:hasWeatherCondition a owl:DatatypeProperty ;
    rdfs:label "has weather condition"@en ;
    rdfs:domain ahmi:EnvironmentalCondition ;
    rdfs:range rdfs:Literal .

ahmi:hasRoadType a owl:DatatypeProperty ;
    rdfs:label "has road type"@en ;
    rdfs:domain ahmi:EnvironmentalCondition ;
    rdfs:range rdfs:Literal .

ahmi:DriverBehavior a owl:Class ;
    rdfs:label "Driver Behavior"@en ;
    rdfs:comment "The observable behavior and actions of the driver."@en .

ahmi:EyeTracking a owl:Class ;
    rdfs:subClassOf ahmi:DriverBehavior ;
    rdfs:label "Eye Tracking"@en ;
    rdfs:comment "The eye movements and fixations of the driver."@en .

ahmi:hasEyeFixationTime a owl:DatatypeProperty ;
    rdfs:label "has eye fixation time"@en ;
    rdfs:domain ahmi:EyeTracking ;
    rdfs:range xsd:duration .

ahmi:SteeringBehavior a owl:Class ;
    rdfs:subClassOf ahmi:DriverBehavior ;
    rdfs:label "Steering Behavior"@en ;
    rdfs:comment "The steering actions and patterns of the driver."@en .

ahmi:hasMicroAdjustmentsFrequency a owl:DatatypeProperty ;
    rdfs:label "has micro adjustments frequency"@en ;
    rdfs:domain ahmi:SteeringBehavior ;
    rdfs:range xsd:float .

ahmi:hasSteeringEntropy a owl:DatatypeProperty ;
    rdfs:label "has steering entropy"@en ;
    rdfs:domain ahmi:SteeringBehavior ;
    rdfs:range xsd:float .

ahmi:ContextualAnalysis a owl:Class ;
    rdfs:label "Contextual Analysis"@en ;
    rdfs:comment "The analysis and interpretation of the overall context, including driver state, environmental conditions, and journey context."@en .

ahmi:HMIFeedback a owl:Class ;
    rdfs:label "HMI Feedback"@en ;
    rdfs:comment "The feedback and adaptation provided by the human-machine interface."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:subClassOf ahmi:HMIFeedback ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "The visual feedback and display elements of the HMI."@en .

ahmi:hasVisualElementSize a owl:DatatypeProperty ;
    rdfs:label "has visual element size"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range xsd:float .

ahmi:hasDisplayContrast a owl:DatatypeProperty ;
    rdfs:label "has display contrast"@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range xsd:float .

ahmi:AudioFeedback a owl:Class ;
    rdfs:subClassOf ahmi:HMIFeedback ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "The audio feedback and voice prompts of the HMI."@en .

ahmi:hasPromptTiming a owl:DatatypeProperty ;
    rdfs:label "has prompt timing"@en ;
    rdfs:domain ahmi:AudioFeedback ;
    rdfs:range xsd:duration .

ahmi:hasDialogClarity a owl:DatatypeProperty ;
    rdfs:label "has dialog clarity"@en ;
    rdfs:domain ahmi:AudioFeedback ;
    rdfs:range xsd:float .

ahmi:WordingFeedback a owl:Class ;
    rdfs:subClassOf ahmi:HMIFeedback ;
    rdfs:label "Wording Feedback"@en ;
    rdfs:comment "The wording and message structure of the HMI feedback."@en .

ahmi:hasMessageComplexity a owl:DatatypeProperty ;
    rdfs:label "has message complexity"@en ;
    rdfs:domain ahmi:WordingFeedback ;
    rdfs:range xsd:float .

ahmi:HapticFeedback a owl:Class ;
    rdfs:subClassOf ahmi:HMIFeedback ;
    rdfs:label "Haptic Feedback"@en ;
    rdfs:comment "The haptic feedback and alerts provided by the HMI."@en .

ahmi:hasHapticAlertIntensity a owl:DatatypeProperty ;
    rdfs:label "has haptic alert intensity"@en ;
    rdfs:domain ahmi:HapticFeedback ;
    rdfs:range xsd:float .

ahmi:VehicleSystem a owl:Class ;
    rdfs:label "Vehicle System"@en ;
    rdfs:comment "The vehicle systems and driver assistance features."@en .

ahmi:LaneKeepingAssist a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:label "Lane Keeping Assist"@en ;
    rdfs:comment "The lane keeping assistance system of the vehicle."@en .

ahmi:hasSensitivity a owl:DatatypeProperty ;
    rdfs:label "has sensitivity"@en ;
    rdfs:domain ahmi:LaneKeepingAssist ;
    rdfs:range xsd:float .

ahmi:LaneDetection a owl:Class ;
    rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:label "Lane Detection"@en ;
    rdfs:comment "The lane detection system of the vehicle."@en .

ahmi:hasLaneDriftThreshold a owl:DatatypeProperty ;
    rdfs:label "has lane drift threshold"@en ;
    rdfs:domain ahmi:LaneDetection ;
    rdfs:range xsd:float .

ahmi:influencesAdaptation a owl:ObjectProperty ;
    rdfs:label "influences adaptation"@en ;
    rdfs:comment "Relates a driver state, environmental condition, or journey context to the adaptation of the HMI feedback or vehicle systems."@en ;
    rdfs:domain owl:unionOf (ahmi:DriverState ahmi:EnvironmentalCondition ahmi:JourneyContext) ;
    rdfs:range owl:unionOf (ahmi:HMIFeedback ahmi:VehicleSystem) .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of a set of events and entities based on a conceptual description."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A physical, social, or mental process or occurrence."@en .

ahmi:isViewOf a owl:ObjectProperty ;
    rdfs:label "is view of"@en ;
    rdfs:comment "Relates a situation to the event it provides a view or interpretation of."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Event .

ahmi:JourneyEvent a owl:Class ;
    rdfs:subClassOf ahmi:Event ;
    rdfs:label "Journey Event"@en ;
    rdfs:comment "An event that occurs during the journey."@en .

ahmi:DriverStateEvent a owl:Class ;
    rdfs:subClassOf ahmi:JourneyEvent ;
    rdfs:label "Driver State Event"@en ;
    rdfs:comment "An event related to the driver's state during the journey."@en .

ahmi:EnvironmentalConditionEvent a owl:Class ;
    rdfs:subClassOf ahmi:JourneyEvent ;
    rdfs:label "Environmental Condition Event"@en ;
    rdfs:comment "An event related to the environmental conditions during the journey."@en .

ahmi:JourneyContextEvent a owl:Class ;
    rdfs:subClassOf ahmi:JourneyEvent ;
    rdfs:label "Journey Context Event"@en ;
    rdfs:comment "An event related to the overall context of the journey."@en .

ahmi:DriverStateView a owl:Class ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:label "Driver State View"@en ;
    rdfs:comment "A view or interpretation of a driver state event."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:isViewOf ;
        owl:someValuesFrom ahmi:DriverStateEvent
    ] .

ahmi:EnvironmentalConditionView a owl:Class ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:label "Environmental Condition View"@en ;
    rdfs:comment "A view or interpretation of an environmental condition event."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:isViewOf ;
        owl:someValuesFrom ahmi:EnvironmentalConditionEvent
    ] .

ahmi:JourneyContextView a owl:Class ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:label "Journey