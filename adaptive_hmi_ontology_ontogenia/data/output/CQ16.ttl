@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:DistractionState rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Distraction State"@en .

ahmi:DriverState rdfs:subClassOf dul:Situation ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "A situation describing the cognitive, emotional, and behavioral state of a driver."@en .

ahmi:CognitiveState rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Cognitive State"@en .

ahmi:EmotionalState rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Emotional State"@en .

ahmi:HighCognitiveLoad rdfs:subClassOf ahmi:CognitiveState ;
    rdfs:label "High Cognitive Load"@en .

ahmi:HighStress rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:label "High Stress"@en .

ahmi:DriverStateDetection rdfs:subClassOf dul:Description ;
    rdfs:label "Driver State Detection"@en ;
    rdfs:comment "A conceptualization for interpreting driver states based on sensor data."@en .

ahmi:DriverStateAdaptation rdfs:subClassOf dul:Description ;
    rdfs:label "Driver State Adaptation"@en ;
    rdfs:comment "A conceptualization for adapting vehicle systems and HMI based on detected driver states."@en .

ahmi:detectedDriverState rdfs:subPropertyOf dul:satisfies ;
    rdfs:label "detected driver state"@en ;
    rdfs:domain ahmi:DriverStateDetection ;
    rdfs:range ahmi:DriverState .

ahmi:adaptedFor rdfs:subPropertyOf dul:satisfies ;
    rdfs:label "adapted for"@en ;
    rdfs:domain ahmi:DriverStateAdaptation ;
    rdfs:range ahmi:DriverState .

ahmi:hasAdaptation rdfs:subPropertyOf dul:isParameterFor ;
    rdfs:label "has adaptation"@en ;
    rdfs:domain ahmi:DriverStateAdaptation ;
    rdfs:range ahmi:Adaptation .

ahmi:Adaptation rdfs:subClassOf dul:Parameter ;
    rdfs:label "Adaptation"@en ;
    rdfs:comment "A parameter that constrains how vehicle systems and HMI should adapt to a driver state."@en .

ahmi:VisualAdaptation rdfs:subClassOf ahmi:Adaptation ;
    rdfs:label "Visual Adaptation"@en .

ahmi:AudioAdaptation rdfs:subClassOf ahmi:Adaptation ;
    rdfs:label "Audio Adaptation"@en .

ahmi:VehicleSystemAdaptation rdfs:subClassOf ahmi:Adaptation ;
    rdfs:label "Vehicle System Adaptation"@en .

ahmi:modifiesVisualFeedback rdfs:subPropertyOf ahmi:hasAdaptation ;
    rdfs:label "modifies visual feedback"@en ;
    rdfs:domain ahmi:VisualAdaptation ;
    rdfs:range ahmi:VisualFeedback .

ahmi:VisualFeedback rdfs:subClassOf dul:Concept ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "A concept representing visual elements and displays in the HMI."@en .

ahmi:modifiesAudioFeedback rdfs:subPropertyOf ahmi:hasAdaptation ;
    rdfs:label "modifies audio feedback"@en ;
    rdfs:domain ahmi:AudioAdaptation ;
    rdfs:range ahmi:AudioFeedback .

ahmi:AudioFeedback rdfs:subClassOf dul:Concept ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "A concept representing audio elements and outputs in the HMI."@en .

ahmi:adaptsVehicleSystem rdfs:subPropertyOf ahmi:hasAdaptation ;
    rdfs:label "adapts vehicle system"@en ;
    rdfs:domain ahmi:VehicleSystemAdaptation ;
    rdfs:range ahmi:VehicleSystem .

ahmi:VehicleSystem rdfs:subClassOf dul:Concept ;
    rdfs:label "Vehicle System"@en ;
    rdfs:comment "A concept representing a vehicle system that can be adapted based on driver state."@en .

ahmi:LaneKeepingAssist rdfs:subClassOf ahmi:VehicleSystem ;
    rdfs:label "Lane Keeping Assist"@en .