@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "A state related to the cognitive processes of a driver."@en .

ahmi:InattentionState a owl:Class ;
    rdfs:label "Inattention State"@en ;
    rdfs:comment "A state where the driver's attention is reduced or impaired."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:Drowsiness a owl:Class ;
    rdfs:label "Drowsiness"@en ;
    rdfs:comment "A state of feeling sleepy or fatigued."@en ;
    rdfs:subClassOf ahmi:InattentionState .

ahmi:Fatigue a owl:Class ;
    rdfs:label "Fatigue"@en ;
    rdfs:comment "A state of physical or mental exhaustion."@en ;
    rdfs:subClassOf ahmi:InattentionState .

ahmi:DriverParameter a owl:Class ;
    rdfs:label "Driver Parameter"@en ;
    rdfs:comment "A parameter related to the driver's state or behavior."@en .

ahmi:hasDriverParameter a owl:ObjectProperty ;
    rdfs:label "has driver parameter"@en ;
    rdfs:comment "Relates a situation to the driver parameters observed in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DriverParameter .

ahmi:HeadPosition a owl:Class ;
    rdfs:label "Head Position"@en ;
    rdfs:comment "The position or orientation of the driver's head."@en ;
    rdfs:subClassOf ahmi:DriverParameter .

ahmi:EyeTracking a owl:Class ;
    rdfs:label "Eye Tracking"@en ;
    rdfs:comment "The tracking of the driver's eye movements and gaze patterns."@en ;
    rdfs:subClassOf ahmi:DriverParameter .

ahmi:BlinkDuration a owl:Class ;
    rdfs:label "Blink Duration"@en ;
    rdfs:comment "The duration of the driver's blink or eye closure."@en ;
    rdfs:subClassOf ahmi:DriverParameter .

ahmi:DetectionMethod a owl:Class ;
    rdfs:label "Detection Method"@en ;
    rdfs:comment "A method used by the vehicle system to detect driver parameters."@en .

ahmi:VisualSensor a owl:Class ;
    rdfs:label "Visual Sensor"@en ;
    rdfs:comment "A sensor that uses visual data to detect driver parameters."@en ;
    rdfs:subClassOf ahmi:DetectionMethod .

ahmi:PhysiologicalSensor a owl:Class ;
    rdfs:label "Physiological Sensor"@en ;
    rdfs:comment "A sensor that measures physiological signals to detect driver parameters."@en ;
    rdfs:subClassOf ahmi:DetectionMethod .

ahmi:BehavioralAnalysis a owl:Class ;
    rdfs:label "Behavioral Analysis"@en ;
    rdfs:comment "An analysis of the driver's behavior to detect patterns or anomalies."@en ;
    rdfs:subClassOf ahmi:DetectionMethod .

ahmi:usesDetectionMethod a owl:ObjectProperty ;
    rdfs:label "uses detection method"@en ;
    rdfs:comment "Relates a situation to the detection methods used to observe driver parameters in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DetectionMethod .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "A modality or channel used by the vehicle system to provide feedback to the driver."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "Feedback provided through visual displays or elements."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:comment "Feedback provided through tactile or haptic sensations."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "Feedback provided through auditory displays or sounds."@en ;
    rdfs:subClassOf ahmi:FeedbackModality .

ahmi:usesFeedbackModality a owl:ObjectProperty ;
    rdfs:label "uses feedback modality"@en ;
    rdfs:comment "Relates a situation to the feedback modalities used by the vehicle system in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:FeedbackModality .

ahmi:AdaptivityParameter a owl:Class ;
    rdfs:label "Adaptivity Parameter"@en ;
    rdfs:comment "A parameter that governs how the vehicle system adapts its feedback based on the detected driver state."@en .

ahmi:hasAdaptivityParameter a owl:ObjectProperty ;
    rdfs:label "has adaptivity parameter"@en ;
    rdfs:comment "Relates a situation to the adaptivity parameters used by the vehicle system in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:AdaptivityParameter .

ahmi:DrivingBehavior a owl:Class ;
    rdfs:label "Driving Behavior"@en ;
    rdfs:comment "The behavior or patterns exhibited by the driver while driving."@en ;
    rdfs:subClassOf ahmi:DriverParameter .

ahmi:deviatesFromNormalBehavior a owl:ObjectProperty ;
    rdfs:label "deviates from normal behavior"@en ;
    rdfs:comment "Indicates that the observed driving behavior deviates from the driver's normal patterns."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DrivingBehavior .

ahmi:InattentionSituation a owl:Class ;
    rdfs:label "Inattention Situation"@en ;
    rdfs:comment "A situation where the driver is in an inattentive state, as detected by the vehicle system."@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasDriverParameter ;
        owl:someValuesFrom ahmi:InattentionState
    ] .

ahmi:InattentionDescription a owl:Class ;
    rdfs:label "Inattention Description"@en ;
    rdfs:comment "A conceptualization or description of inattentive driver states and their detection."@en ;
    rdfs:subClassOf dul:Description .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range dul:Description .