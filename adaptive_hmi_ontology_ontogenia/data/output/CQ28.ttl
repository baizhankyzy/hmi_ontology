@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:EmotionalState a owl:Class ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "A state of mind characterized by emotional experiences and expressions."@en .

ahmi:ContinuousEmotiveState a owl:Class ;
    rdfs:label "Continuous Emotive State"@en ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:comment "An emotional state that varies along a continuous dimension, such as arousal levels."@en .

ahmi:DiscreteEmotiveState a owl:Class ;
    rdfs:label "Discrete Emotive State"@en ;
    rdfs:subClassOf ahmi:EmotionalState ;
    rdfs:comment "An emotional state that can be categorized into distinct categories, such as frustration or anger."@en .

ahmi:DrivingBehaviour a owl:Class ;
    rdfs:label "Driving Behaviour"@en ;
    rdfs:comment "The manner in which a person operates a vehicle, including actions and responses."@en .

ahmi:PersonalityFactor a owl:Class ;
    rdfs:label "Personality Factor"@en ;
    rdfs:comment "A trait or characteristic that influences an individual's behavior and emotional responses."@en .

ahmi:BigFivePersonalityTraits a owl:Class ;
    rdfs:label "Big Five Personality Traits"@en ;
    rdfs:subClassOf ahmi:PersonalityFactor ;
    rdfs:comment "A widely accepted model of personality that includes openness, conscientiousness, extraversion, agreeableness, and neuroticism."@en .

ahmi:AggressiveDrivingTendency a owl:Class ;
    rdfs:label "Aggressive Driving Tendency"@en ;
    rdfs:subClassOf ahmi:PersonalityFactor ;
    rdfs:comment "A personality trait that predisposes an individual to engage in aggressive or reckless driving behaviors."@en .

ahmi:LowConscientiousness a owl:Class ;
    rdfs:label "Low Conscientiousness"@en ;
    rdfs:subClassOf ahmi:BigFivePersonalityTraits ;
    rdfs:comment "A personality trait characterized by a lack of self-discipline, organization, and attention to detail."@en .

ahmi:DriverProfile a owl:Class ;
    rdfs:label "Driver Profile"@en ;
    rdfs:comment "A representation of a driver's characteristics, including personality factors and typical driving behaviors."@en .

ahmi:DetectionMethodology a owl:Class ;
    rdfs:label "Detection Methodology"@en ;
    rdfs:comment "A technique or approach used to monitor and detect a driver's state or behavior."@en .

ahmi:SteeringBehaviorAnalysis a owl:Class ;
    rdfs:label "Steering Behavior Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology ;
    rdfs:comment "A method that analyzes the driver's steering behavior, such as acceleration and smoothness."@en .

ahmi:PedalControlAnalysis a owl:Class ;
    rdfs:label "Pedal Control Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology ;
    rdfs:comment "A method that analyzes the driver's pedal control, such as braking behavior."@en .

ahmi:PressureSensorAnalysis a owl:Class ;
    rdfs:label "Pressure Sensor Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology ;
    rdfs:comment "A method that analyzes pressure sensor data, such as steering wheel grip pressure."@en .

ahmi:SpeechRecognitionAnalysis a owl:Class ;
    rdfs:label "Speech Recognition Analysis"@en ;
    rdfs:subClassOf ahmi:DetectionMethodology ;
    rdfs:comment "A method that analyzes the driver's speech and verbal expressions."@en .

ahmi:ContextualAnalysis a owl:Class ;
    rdfs:label "Contextual Analysis"@en ;
    rdfs:comment "An analysis of the environmental and situational factors that may influence a driver's state or behavior."@en .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "An external factor or condition in the environment that may impact a driver's state or behavior."@en .

ahmi:TrafficCondition a owl:Class ;
    rdfs:label "Traffic Condition"@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor ;
    rdfs:comment "The level of congestion or flow of traffic on the road."@en .

ahmi:HighCongestion a owl:Class ;
    rdfs:label "High Congestion"@en ;
    rdfs:subClassOf ahmi:TrafficCondition ;
    rdfs:comment "A traffic condition characterized by heavy congestion and slow-moving or stopped vehicles."@en .

ahmi:RoadType a owl:Class ;
    rdfs:label "Road Type"@en ;
    rdfs:comment "The classification of a road based on its characteristics and location."@en .

ahmi:CityRoad a owl:Class ;
    rdfs:label "City Road"@en ;
    rdfs:subClassOf ahmi:RoadType ;
    rdfs:comment "A road located within an urban or city environment."@en .

ahmi:JourneyContext a owl:Class ;
    rdfs:label "Journey Context"@en ;
    rdfs:comment "The circumstances and conditions surrounding a particular journey or trip."@en .

ahmi:TimeOnTaskMeasurement a owl:Class ;
    rdfs:label "Time-on-Task Measurement"@en ;
    rdfs:comment "A measurement of the duration or time spent on a particular task or activity."@en .

ahmi:FeedbackModality a owl:Class ;
    rdfs:label "Feedback Modality"@en ;
    rdfs:comment "The sensory channel or mode through which feedback is provided to the driver."@en .

ahmi:VisualFeedback a owl:Class ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:comment "Feedback provided through visual displays or elements."@en .

ahmi:AudioFeedback a owl:Class ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:comment "Feedback provided through auditory channels or speakers."@en .

ahmi:TactileFeedback a owl:Class ;
    rdfs:label "Tactile Feedback"@en ;
    rdfs:subClassOf ahmi:FeedbackModality ;
    rdfs:comment "Feedback provided through tactile or haptic sensations."@en .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "A visual component or element used in the feedback or display."@en .

ahmi:AuditoryDisplay a owl:Class ;
    rdfs:label "Auditory Display"@en ;
    rdfs:comment "An auditory output or sound used in the feedback or display."@en .

ahmi:ForceFeedback a owl:Class ;
    rdfs:label "Force Feedback"@en ;
    rdfs:subClassOf ahmi:TactileFeedback ;
    rdfs:comment "Tactile feedback that provides resistance or force sensations."@en .

ahmi:AdaptationStrategy a owl:Class ;
    rdfs:label "Adaptation Strategy"@en ;
    rdfs:comment "A strategy or approach used by the vehicle system to adapt the feedback or behavior based on the driver's state or context."@en .

ahmi:RouteAlternativePresentation a owl:Class ;
    rdfs:label "Route Alternative Presentation"@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy ;
    rdfs:comment "A strategy that presents alternative route options to the driver."@en .

ahmi:VisualElementAdaptation a owl:Class ;
    rdfs:label "Visual Element Adaptation"@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy ;
    rdfs:comment "A strategy that adapts the visual elements or displays to influence the driver's state."@en .

ahmi:AudioFeedbackAdaptation a owl:Class ;
    rdfs:label "Audio Feedback Adaptation"@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy ;
    rdfs:comment "A strategy that adapts the audio feedback or auditory displays to influence the driver's state."@en .

ahmi:EnvironmentalControlAdaptation a owl:Class ;
    rdfs:label "Environmental Control Adaptation"@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy ;
    rdfs:comment "A strategy that adapts the vehicle's environmental systems, such as cabin temperature, to influence the driver's state."@en .

ahmi:ForceFeedbackAdaptation a owl:Class ;
    rdfs:label "Force Feedback Adaptation"@en ;
    rdfs:subClassOf ahmi:AdaptationStrategy ;
    rdfs:comment "A strategy that adapts the force feedback or resistance provided to the driver to influence their driving behavior."@en .

ahmi:hasPersonalityFactor a owl:ObjectProperty ;
    rdfs:label "has personality factor"@en ;
    rdfs:domain ahmi:DriverProfile ;
    rdfs:range ahmi:PersonalityFactor ;
    rdfs:comment "Relates a driver profile to the personality factors of the driver."@en .

ahmi:hasTypicalDrivingBehaviour a owl:ObjectProperty ;
    rdfs:label "has typical driving behaviour"@en ;
    rdfs:domain ahmi:DriverProfile ;
    rdfs:range ahmi:DrivingBehaviour ;
    rdfs:comment "Relates a driver profile to the typical driving behaviors exhibited by the driver."@en .

ahmi:employsDetectionMethodology a owl:ObjectProperty ;
    rdfs:label "employs detection methodology"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:DetectionMethodology ;
    rdfs:comment "Relates a situation to the detection methodologies employed to monitor the driver's state or behavior."@en .

ahmi:confirmsEnvironmentFactor a owl:ObjectProperty ;
    rdfs:label "confirms environment factor"@en ;
    rdfs:domain ahmi:ContextualAnalysis ;
    rdfs:range ahmi:EnvironmentFactor ;
    rdfs:comment "Relates a contextual analysis to the environmental factors it confirms or identifies."@en .

ahmi:triggersEmotionalState a owl:ObjectProperty ;
    rdfs:label "triggers emotional state"@en ;
    rdfs:domain ahmi:EnvironmentFactor ;
    rdfs:range ahmi:EmotionalState ;
    rdfs:comment "Relates an environmental factor to the emotional state it may trigger or influence in the driver."@en .

ahmi:hasEmotionalState a owl:ObjectProperty ;
    rdfs:label "has emotional state"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:EmotionalState ;
    rdfs:comment "Relates a situation to the emotional state of the driver observed or interpreted in that situation."@en .

ahmi:hasFeedbackModality a owl:ObjectProperty ;
    rdfs:label "has feedback modality"@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi