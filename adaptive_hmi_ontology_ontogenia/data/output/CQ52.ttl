@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:NASATLXMeasure a owl:Class ;
    rdfs:label "NASA-TLX Measure"@en ;
    rdfs:comment "A measure for assessing driver workload, comprising multiple components."@en ;
    rdfs:subClassOf ahmi:DriverWorkloadAssessment .

ahmi:MentalWorkloadComponent a owl:Class ;
    rdfs:label "Mental Workload Component"@en ;
    rdfs:subClassOf ahmi:NASATLXMeasure .

ahmi:FrustrationComponent a owl:Class ;
    rdfs:label "Frustration Component"@en ;
    rdfs:subClassOf ahmi:NASATLXMeasure .

ahmi:EffortComponent a owl:Class ;
    rdfs:label "Effort Component"@en ;
    rdfs:subClassOf ahmi:NASATLXMeasure .

ahmi:hasComponent a owl:ObjectProperty ;
    rdfs:label "has component"@en ;
    rdfs:comment "Relates a measure to its components."@en ;
    rdfs:domain ahmi:DriverWorkloadAssessment ;
    rdfs:range ahmi:NASATLXMeasure .

ahmi:DriverWorkloadAssessment a owl:Class ;
    rdfs:label "Driver Workload Assessment"@en ;
    rdfs:comment "An assessment of a driver's workload."@en .

ahmi:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A view or interpretation of events, consistent with a description."@en ;
    rdfs:subClassOf ahmi:SocialObject ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:Description
    ] .

ahmi:SocialObject a owl:Class ;
    rdfs:label "Social Object"@en ;
    rdfs:comment "An object created in a social context."@en .

ahmi:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting situations and events."@en ;
    rdfs:subClassOf ahmi:SocialObject .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a situation to the description it satisfies."@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description .

ahmi:isPartOf a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "is part of"@en ;
    rdfs:comment "A relation between any entities."@en ;
    rdfs:domain ahmi:Entity ;
    rdfs:range ahmi:Entity ;
    rdfs:subPropertyOf ahmi:associatedWith ;
    owl:inverseOf ahmi:hasPart .

ahmi:Entity a owl:Class ;
    rdfs:label "Entity"@en ;
    rdfs:comment "Any physical, social, or mental object, event, or quality."@en .

ahmi:associatedWith a owl:ObjectProperty ;
    rdfs:label "associated with"@en ;
    rdfs:comment "A general, symmetric relation between entities."@en ;
    rdfs:domain ahmi:Entity ;
    rdfs:range ahmi:Entity .

ahmi:isParameterFor a owl:ObjectProperty ;
    rdfs:label "is parameter for"@en ;
    rdfs:comment "A concept can have a parameter that constrains the attributes that a classified entity can have in a certain situation."@en ;
    rdfs:domain ahmi:Parameter ;
    rdfs:range ahmi:Concept ;
    owl:inverseOf ahmi:hasParameter ;
    rdfs:subPropertyOf ahmi:isRelatedToConcept .

ahmi:Parameter a owl:Class ;
    rdfs:label "Parameter"@en ;
    rdfs:comment "A concept that constrains attributes of entities in a situation."@en .

ahmi:Concept a owl:Class ;
    rdfs:label "Concept"@en ;
    rdfs:comment "A concept is a social object which represents a category of entities."@en ;
    rdfs:subClassOf ahmi:SocialObject .

ahmi:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:comment "Relates a concept to parameters that constrain it."@en ;
    rdfs:domain ahmi:Concept ;
    rdfs:range ahmi:Parameter .

ahmi:isRelatedToConcept a owl:ObjectProperty ;
    rdfs:label "is related to concept"@en ;
    rdfs:comment "A general relation between an entity and a concept."@en ;
    rdfs:range ahmi:Concept .