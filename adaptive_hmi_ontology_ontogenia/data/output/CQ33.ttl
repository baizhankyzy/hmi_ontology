@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The cognitive and physical state of a driver" .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:subClassOf ahmi:DriverState ;
    rdfs:comment "The cognitive state of a driver, including factors like reaction time and attention" .

ahmi:AgeRelatedCognitiveState a owl:Class ;
    rdfs:label "Age-Related Cognitive State"@en ;
    rdfs:subClassOf ahmi:CognitiveState ;
    rdfs:comment "A cognitive state influenced by age-related factors" .

ahmi:SlowerReactionTime a owl:Class ;
    rdfs:label "Slower Reaction Time"@en ;
    rdfs:subClassOf ahmi:AgeRelatedCognitiveState ;
    rdfs:comment "A cognitive state characterized by slower reaction times" .

ahmi:VisualElement a owl:Class ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "A visual component of a user interface" .

ahmi:DisplayType a owl:Class ;
    rdfs:label "Display Type"@en ;
    rdfs:comment "A type of visual display or interface" .

ahmi:SimplifiedDisplayType a owl:Class ;
    rdfs:label "Simplified Display Type"@en ;
    rdfs:subClassOf ahmi:DisplayType ;
    rdfs:comment "A display type with simplified visual elements" .

ahmi:LargeVisualElement a owl:Class ;
    rdfs:label "Large Visual Element"@en ;
    rdfs:subClassOf ahmi:VisualElement ;
    rdfs:comment "A visual element with a larger size" .

ahmi:hasPreferredDisplayType a owl:ObjectProperty ;
    rdfs:label "has preferred display type"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:DisplayType ;
    rdfs:comment "Relates a driver state to the preferred display type for that state" .

ahmi:hasPreferredVisualElement a owl:ObjectProperty ;
    rdfs:label "has preferred visual element"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:VisualElement ;
    rdfs:comment "Relates a driver state to the preferred visual element for that state" .

ahmi:requiresVisualElementConfiguration a owl:ObjectProperty ;
    rdfs:label "requires visual element configuration"@en ;
    rdfs:domain ahmi:DriverState ;
    rdfs:range ahmi:VisualElementConfiguration ;
    rdfs:comment "Relates a driver state to the required visual element configuration" .

ahmi:VisualElementConfiguration a owl:Class ;
    rdfs:label "Visual Element Configuration"@en ;
    rdfs:comment "A configuration of visual elements for a user interface" .

ahmi:hasVisualElementSize a owl:ObjectProperty ;
    rdfs:label "has visual element size"@en ;
    rdfs:domain ahmi:VisualElementConfiguration ;
    rdfs:range ahmi:VisualElementSize ;
    rdfs:comment "Relates a visual element configuration to the size of its visual elements" .

ahmi:VisualElementSize a owl:Class ;
    rdfs:label "Visual Element Size"@en ;
    rdfs:comment "The size of a visual element" .

ahmi:Large a owl:NamedIndividual ;
    rdf:type ahmi:VisualElementSize ;
    rdfs:label "Large"@en .

ahmi:AgeRelatedVisualElementConfiguration a owl:Class ;
    rdfs:label "Age-Related Visual Element Configuration"@en ;
    rdfs:subClassOf ahmi:VisualElementConfiguration ;
    rdfs:comment "A visual element configuration adapted for age-related driver states" .

ahmi:AgeRelatedVisualElementConfiguration rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ahmi:hasVisualElementSize ;
    owl:hasValue ahmi:Large
] .

ahmi:DriverAgeDescription a owl:Class ;
    rdfs:label "Driver Age Description"@en ;
    rdfs:subClassOf ahmi:Description ;
    rdfs:comment "A description or conceptualization related to the age of a driver" .

ahmi:DriverAgeSituation a owl:Class ;
    rdfs:label "Driver Age Situation"@en ;
    rdfs:subClassOf ahmi:Situation ;
    rdfs:comment "A situation or view related to the age of a driver" .

ahmi:DriverAgeSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ahmi:satisfies ;
    owl:someValuesFrom ahmi:DriverAgeDescription
] .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:domain ahmi:Situation ;
    rdfs:range ahmi:Description ;
    rdfs:comment "Relates a situation to the description it satisfies" .

ahmi:hasDriverAge a owl:DatatypeProperty ;
    rdfs:label "has driver age"@en ;
    rdfs:domain ahmi:DriverAgeSituation ;
    rdfs:range xsd:integer ;
    rdfs:comment "Relates a driver age situation to the age of the driver" .