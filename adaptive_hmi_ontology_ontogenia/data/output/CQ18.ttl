@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:WeatherCondition a owl:Class ;
    rdfs:label "Weather Condition"@en ;
    rdfs:comment "Environmental conditions related to weather, such as rain, snow, or sunshine."@en .

ahmi:VehicleAdaptation a owl:Class ;
    rdfs:label "Vehicle Adaptation"@en ;
    rdfs:comment "Modifications made by the vehicle system to adapt to the driver's state or environmental conditions."@en .

ahmi:VisualFeedbackAdaptation a owl:Class ;
    rdfs:subClassOf ahmi:VehicleAdaptation ;
    rdfs:label "Visual Feedback Adaptation"@en ;
    rdfs:comment "Adaptations made to the visual feedback provided by the vehicle's HMI."@en .

ahmi:AudioFeedbackAdaptation a owl:Class ;
    rdfs:subClassOf ahmi:VehicleAdaptation ;
    rdfs:label "Audio Feedback Adaptation"@en ;
    rdfs:comment "Adaptations made to the audio feedback provided by the vehicle's HMI."@en .

ahmi:DriverAssistanceAdaptation a owl:Class ;
    rdfs:subClassOf ahmi:VehicleAdaptation ;
    rdfs:label "Driver Assistance Adaptation"@en ;
    rdfs:comment "Adaptations made to the driver assistance systems, such as lane keeping assist or adaptive cruise control."@en .

ahmi:hasWeatherCondition a owl:ObjectProperty ;
    rdfs:label "has weather condition"@en ;
    rdfs:comment "Relates a situation to the weather conditions present in that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:WeatherCondition .

ahmi:hasVehicleAdaptation a owl:ObjectProperty ;
    rdfs:label "has vehicle adaptation"@en ;
    rdfs:comment "Relates a situation to the vehicle adaptations made in response to that situation."@en ;
    rdfs:domain dul:Situation ;
    rdfs:range ahmi:VehicleAdaptation .

ahmi:hasVisualFeedbackAdaptation a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasVehicleAdaptation ;
    rdfs:label "has visual feedback adaptation"@en ;
    rdfs:comment "Relates a situation to the visual feedback adaptations made by the vehicle's HMI."@en ;
    rdfs:range ahmi:VisualFeedbackAdaptation .

ahmi:hasAudioFeedbackAdaptation a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasVehicleAdaptation ;
    rdfs:label "has audio feedback adaptation"@en ;
    rdfs:comment "Relates a situation to the audio feedback adaptations made by the vehicle's HMI."@en ;
    rdfs:range ahmi:AudioFeedbackAdaptation .

ahmi:hasDriverAssistanceAdaptation a owl:ObjectProperty ;
    rdfs:subPropertyOf ahmi:hasVehicleAdaptation ;
    rdfs:label "has driver assistance adaptation"@en ;
    rdfs:comment "Relates a situation to the adaptations made to the driver assistance systems."@en ;
    rdfs:range ahmi:DriverAssistanceAdaptation .

ahmi:Rain a owl:NamedIndividual , ahmi:WeatherCondition ;
    rdfs:label "Rain"@en ;
    rdfs:comment "Weather condition characterized by precipitation in the form of liquid water droplets."@en .

ahmi:SimplifyVisualElements a owl:NamedIndividual , ahmi:VisualFeedbackAdaptation ;
    rdfs:label "Simplify Visual Elements"@en ;
    rdfs:comment "Adaptation that simplifies the visual elements displayed on the vehicle's HMI."@en .

ahmi:ReduceNonEssentialVisualElements a owl:NamedIndividual , ahmi:VisualFeedbackAdaptation ;
    rdfs:label "Reduce Non-Essential Visual Elements"@en ;
    rdfs:comment "Adaptation that reduces or hides non-essential visual elements on the vehicle's HMI."@en .

ahmi:IncreaseVisualContrast a owl:NamedIndividual , ahmi:VisualFeedbackAdaptation ;
    rdfs:label "Increase Visual Contrast"@en ;
    rdfs:comment "Adaptation that increases the contrast of visual elements on the vehicle's HMI."@en .

ahmi:PauseIncomingNotifications a owl:NamedIndividual , ahmi:VisualFeedbackAdaptation ;
    rdfs:label "Pause Incoming Notifications"@en ;
    rdfs:comment "Adaptation that pauses or stops incoming notifications on the vehicle's HMI."@en .

ahmi:LowerBackgroundAudio a owl:NamedIndividual , ahmi:AudioFeedbackAdaptation ;
    rdfs:label "Lower Background Audio"@en ;
    rdfs:comment "Adaptation that lowers the volume or mutes background audio in the vehicle."@en .

ahmi:ClearConciseVoiceOutput a owl:NamedIndividual , ahmi:AudioFeedbackAdaptation ;
    rdfs:label "Clear and Concise Voice Output"@en ;
    rdfs:comment "Adaptation that provides clear and concise voice output from the vehicle's audio system."@en .

ahmi:CalmerVoiceTone a owl:NamedIndividual , ahmi:AudioFeedbackAdaptation ;
    rdfs:label "Calmer Voice Tone"@en ;
    rdfs:comment "Adaptation that uses a calmer voice tone for audio output from the vehicle's system."@en .

ahmi:IncreaseSteeringAssist a owl:NamedIndividual , ahmi:DriverAssistanceAdaptation ;
    rdfs:label "Increase Steering Assist"@en ;
    rdfs:comment "Adaptation that increases the level of steering assistance provided by the vehicle's driver assistance system."@en .