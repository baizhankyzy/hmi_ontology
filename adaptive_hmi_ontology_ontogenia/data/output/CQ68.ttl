@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:SpeedViolationTendencyMeasurement a owl:Class ;
    rdfs:label "Speed Violation Tendency Measurement"@en ;
    rdfs:comment "A measurement of the driver's tendency to exceed safe speeds based on current environmental conditions."@en .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "A factor related to the driving environment that can influence the driver's behavior and the vehicle's systems."@en .

ahmi:RoadCondition a owl:Class ;
    rdfs:subClassOf ahmi:EnvironmentFactor ;
    rdfs:label "Road Condition"@en ;
    rdfs:comment "The condition of the road surface, such as even or uneven."@en .

ahmi:DaytimeCondition a owl:Class ;
    rdfs:subClassOf ahmi:EnvironmentFactor ;
    rdfs:label "Daytime Condition"@en ;
    rdfs:comment "The lighting conditions based on the time of day, such as daytime or nighttime."@en .

ahmi:AirQuality a owl:Class ;
    rdfs:subClassOf ahmi:EnvironmentFactor ;
    rdfs:label "Air Quality"@en ;
    rdfs:comment "The quality of the air, such as clean, polluted, or with pollen."@en .

ahmi:WeatherCondition a owl:Class ;
    rdfs:subClassOf ahmi:EnvironmentFactor ;
    rdfs:label "Weather Condition"@en ;
    rdfs:comment "The current weather conditions, such as rain or clear."@en .

ahmi:hasThresholdFor a owl:ObjectProperty ;
    rdfs:label "has threshold for"@en ;
    rdfs:comment "Relates a speed violation tendency measurement to the environment factors that influence its threshold."@en ;
    rdfs:domain ahmi:SpeedViolationTendencyMeasurement ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:EnvironmentSituation a owl:Class ;
    rdfs:label "Environment Situation"@en ;
    rdfs:comment "A specific combination of environment factors that influence the driver's behavior and the vehicle's systems."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:hasEnvironmentFactor ;
        owl:someValuesFrom ahmi:EnvironmentFactor
    ] .

ahmi:hasEnvironmentFactor a owl:ObjectProperty ;
    rdfs:label "has environment factor"@en ;
    rdfs:comment "Relates an environment situation to the specific environment factors involved."@en ;
    rdfs:domain ahmi:EnvironmentSituation ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:SpeedViolationTendencyThreshold a owl:Class ;
    rdfs:label "Speed Violation Tendency Threshold"@en ;
    rdfs:comment "A threshold value for the speed violation tendency measurement, which can be adjusted based on the current environment situation."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:isThresholdFor ;
        owl:someValuesFrom ahmi:EnvironmentSituation
    ] .

ahmi:isThresholdFor a owl:ObjectProperty ;
    rdfs:label "is threshold for"@en ;
    rdfs:comment "Relates a speed violation tendency threshold to the environment situation it is defined for."@en ;
    rdfs:domain ahmi:SpeedViolationTendencyThreshold ;
    rdfs:range ahmi:EnvironmentSituation .