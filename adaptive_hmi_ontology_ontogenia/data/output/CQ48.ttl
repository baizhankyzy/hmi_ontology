@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .
@prefix dul: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .

ahmi:AdaptiveHMIEvent a owl:Class ;
    rdfs:label "Adaptive HMI Event"@en ;
    rdfs:comment "An event related to the adaptation of a human-machine interface (HMI) based on the driver's cognitive state."@en ;
    rdfs:subClassOf dul:Event .

ahmi:AdaptiveHMISituation a owl:Class ;
    rdfs:label "Adaptive HMI Situation"@en ;
    rdfs:comment "A view or interpretation of an Adaptive HMI Event, providing context for adaptation based on the driver's cognitive state."@en ;
    rdfs:subClassOf dul:Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dul:satisfies ;
        owl:someValuesFrom ahmi:AdaptiveHMIDescription
    ] .

ahmi:AdaptiveHMIDescription a owl:Class ;
    rdfs:label "Adaptive HMI Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting Adaptive HMI Situations and Events, based on the driver's cognitive state."@en ;
    rdfs:subClassOf dul:Description .

ahmi:DriverCognitiveState a owl:Class ;
    rdfs:label "Driver Cognitive State"@en ;
    rdfs:comment "The cognitive state of the driver, which influences the adaptation of the HMI."@en ;
    rdfs:subClassOf dul:Concept .

ahmi:InteractionTiming a owl:Class ;
    rdfs:label "Interaction Timing"@en ;
    rdfs:comment "The timing parameters for the interaction between the driver and the HMI, which can be adjusted based on the driver's cognitive state."@en ;
    rdfs:subClassOf dul:Parameter .

ahmi:hasDriverCognitiveState a owl:ObjectProperty ;
    rdfs:label "has driver cognitive state"@en ;
    rdfs:comment "Relates an Adaptive HMI Situation to the Driver Cognitive State that influences the adaptation."@en ;
    rdfs:domain ahmi:AdaptiveHMISituation ;
    rdfs:range ahmi:DriverCognitiveState .

ahmi:adjustsInteractionTiming a owl:ObjectProperty ;
    rdfs:label "adjusts interaction timing"@en ;
    rdfs:comment "Relates an Adaptive HMI Situation to the Interaction Timing parameters that should be adjusted based on the driver's cognitive state."@en ;
    rdfs:domain ahmi:AdaptiveHMISituation ;
    rdfs:range ahmi:InteractionTiming .

ahmi:InteractionTiming rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ahmi:isParameterFor ;
    owl:someValuesFrom ahmi:AdaptiveHMIDescription
] .

ahmi:isParameterFor a owl:ObjectProperty ;
    rdfs:label "is parameter for"@en ;
    rdfs:comment "Relates a parameter to the description it constrains."@en ;
    rdfs:domain ahmi:InteractionTiming ;
    rdfs:range ahmi:AdaptiveHMIDescription ;
    rdfs:subPropertyOf dul:isParameterFor .

ahmi:DriverCognitiveState rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty dul:isRelatedToConcept ;
    owl:someValuesFrom ahmi:AdaptiveHMIDescription
] .

ahmi:DriverAttentionLevel a owl:Class ;
    rdfs:label "Driver Attention Level"@en ;
    rdfs:comment "The level of attention of the driver, which is a component of the Driver Cognitive State."@en ;
    rdfs:subClassOf ahmi:DriverCognitiveState .

ahmi:DriverVigilance a owl:Class ;
    rdfs:label "Driver Vigilance"@en ;
    rdfs:comment "The vigilance level of the driver, which is a component of the Driver Cognitive State."@en ;
    rdfs:subClassOf ahmi:DriverCognitiveState .

ahmi:DriverDistraction a owl:Class ;
    rdfs:label "Driver Distraction"@en ;
    rdfs:comment "The level of distraction of the driver, which is a component of the Driver Cognitive State."@en ;
    rdfs:subClassOf ahmi:DriverCognitiveState .

ahmi:DriverPosture a owl:Class ;
    rdfs:label "Driver Posture"@en ;
    rdfs:comment "The posture of the driver, which is a component of the Driver Cognitive State."@en ;
    rdfs:subClassOf ahmi:DriverCognitiveState .

ahmi:hasAttentionLevel a owl:ObjectProperty ;
    rdfs:label "has attention level"@en ;
    rdfs:comment "Relates a Driver Cognitive State to the Driver Attention Level."@en ;
    rdfs:domain ahmi:DriverCognitiveState ;
    rdfs:range ahmi:DriverAttentionLevel .

ahmi:hasVigilance a owl:ObjectProperty ;
    rdfs:label "has vigilance"@en ;
    rdfs:comment "Relates a Driver Cognitive State to the Driver Vigilance."@en ;
    rdfs:domain ahmi:DriverCognitiveState ;
    rdfs:range ahmi:DriverVigilance .

ahmi:hasDistraction a owl:ObjectProperty ;
    rdfs:label "has distraction"@en ;
    rdfs:comment "Relates a Driver Cognitive State to the Driver Distraction."@en ;
    rdfs:domain ahmi:DriverCognitiveState ;
    rdfs:range ahmi:DriverDistraction .

ahmi:hasPosture a owl:ObjectProperty ;
    rdfs:label "has posture"@en ;
    rdfs:comment "Relates a Driver Cognitive State to the Driver Posture."@en ;
    rdfs:domain ahmi:DriverCognitiveState ;
    rdfs:range ahmi:DriverPosture .