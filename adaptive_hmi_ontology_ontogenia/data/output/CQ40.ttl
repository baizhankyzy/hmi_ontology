@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:Driver a owl:Class ;
    rdfs:label "Driver"@en ;
    rdfs:comment "A person operating a vehicle."@en .

ahmi:ElderlyDriver rdfs:subClassOf ahmi:Driver ;
    rdfs:label "Elderly Driver"@en ;
    rdfs:comment "A driver over a certain age threshold, typically with age-related factors affecting driving abilities."@en .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The physical, cognitive, and emotional condition of a driver."@en .

ahmi:CognitiveState rdfs:subClassOf ahmi:DriverState ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The mental state of a driver, including factors like reaction time and attention."@en .

ahmi:Vehicle a owl:Class ;
    rdfs:label "Vehicle"@en ;
    rdfs:comment "A means of transportation."@en .

ahmi:AdaptiveVehicleSystem rdfs:subClassOf ahmi:Vehicle ;
    rdfs:label "Adaptive Vehicle System"@en ;
    rdfs:comment "A vehicle system capable of adapting its behavior and interfaces based on driver state and context."@en .

ahmi:HMI rdfs:subClassOf ahmi:AdaptiveVehicleSystem ;
    rdfs:label "Human-Machine Interface"@en ;
    rdfs:comment "The interface between a vehicle system and the driver, including visual, audio, and haptic feedback."@en .

ahmi:VisualFeedback rdfs:subClassOf ahmi:HMI ;
    rdfs:label "Visual Feedback"@en ;
    rdfs:comment "The visual component of a vehicle's human-machine interface."@en .

ahmi:DisplayType rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:label "Display Type"@en ;
    rdfs:comment "The type of visual display used for presenting information to the driver."@en .

ahmi:VisualElement rdfs:subClassOf ahmi:VisualFeedback ;
    rdfs:label "Visual Element"@en ;
    rdfs:comment "The individual visual components or elements displayed on a vehicle interface."@en .

ahmi:AudioFeedback rdfs:subClassOf ahmi:HMI ;
    rdfs:label "Audio Feedback"@en ;
    rdfs:comment "The audio component of a vehicle's human-machine interface."@en .

ahmi:AudioParameter rdfs:subClassOf ahmi:AudioFeedback ;
    rdfs:label "Audio Parameter"@en ;
    rdfs:comment "The parameters that control the audio feedback, such as volume, timing, and clarity."@en .

ahmi:DialogDesign rdfs:subClassOf ahmi:AudioFeedback ;
    rdfs:label "Dialog Design"@en ;
    rdfs:comment "The design of the spoken dialog or prompts provided to the driver."@en .

ahmi:HapticFeedback rdfs:subClassOf ahmi:HMI ;
    rdfs:label "Haptic Feedback"@en ;
    rdfs:comment "The tactile or force feedback component of a vehicle's human-machine interface."@en .

ahmi:HapticAlert rdfs:subClassOf ahmi:HapticFeedback ;
    rdfs:label "Haptic Alert"@en ;
    rdfs:comment "A tactile alert or notification provided to the driver through haptic feedback."@en .

ahmi:DriverFacingVisualSensor a owl:Class ;
    rdfs:label "Driver-facing Visual Sensor"@en ;
    rdfs:comment "A sensor that captures visual data related to the driver's behavior and state."@en .

ahmi:EyeTrackingSensor rdfs:subClassOf ahmi:DriverFacingVisualSensor ;
    rdfs:label "Eye Tracking Sensor"@en ;
    rdfs:comment "A sensor that tracks the driver's eye movements and gaze direction."@en .

ahmi:EyeTrackingData a owl:Class ;
    rdfs:label "Eye Tracking Data"@en ;
    rdfs:comment "The data collected by eye tracking sensors, including eye fixations, saccades, and gaze patterns."@en .

ahmi:EnvironmentalCondition a owl:Class ;
    rdfs:label "Environmental Condition"@en ;
    rdfs:comment "The external conditions or context in which the vehicle is operating."@en .

ahmi:WeatherCondition rdfs:subClassOf ahmi:EnvironmentalCondition ;
    rdfs:label "Weather Condition"@en ;
    rdfs:comment "The weather conditions affecting the driving environment."@en .

ahmi:RoadType rdfs:subClassOf ahmi:EnvironmentalCondition ;
    rdfs:label "Road Type"@en ;
    rdfs:comment "The type of road or driving environment, such as urban, suburban, or highway."@en .

ahmi:Visibility rdfs:subClassOf ahmi:EnvironmentalCondition ;
    rdfs:label "Visibility"@en ;
    rdfs:comment "The level of visibility or clarity in the driving environment."@en .

ahmi:EnvironmentalSensor a owl:Class ;
    rdfs:label "Environmental Sensor"@en ;
    rdfs:comment "A sensor that detects and monitors environmental conditions."@en .

ahmi:VisibilitySensor rdfs:subClassOf ahmi:EnvironmentalSensor ;
    rdfs:label "Visibility Sensor"@en ;
    rdfs:comment "A sensor that measures the level of visibility in the driving environment."@en .

ahmi:VehicleBehavior a owl:Class ;
    rdfs:label "Vehicle Behavior"@en ;
    rdfs:comment "The behavior or actions of the vehicle in response to driver state and environmental conditions."@en .

ahmi:SteeringBehavior rdfs:subClassOf ahmi:VehicleBehavior ;
    rdfs:label "Steering Behavior"@en ;
    rdfs:comment "The behavior related to the steering of the vehicle."@en .

ahmi:MicroAdjustment rdfs:subClassOf ahmi:SteeringBehavior ;
    rdfs:label "Micro Adjustment"@en ;
    rdfs:comment "Small adjustments made to the steering wheel by the driver."@en .

ahmi:SteeringEntropy rdfs:subClassOf ahmi:SteeringBehavior ;
    rdfs:label "Steering Entropy"@en ;
    rdfs:comment "A measure of the variability or randomness in the driver's steering behavior."@en .

ahmi:LaneKeepingAssist rdfs:subClassOf ahmi:VehicleBehavior ;
    rdfs:label "Lane Keeping Assist"@en ;
    rdfs:comment "A driver assistance system that helps the vehicle stay within the lane markings."@en .

ahmi:LaneDetectionSystem rdfs:subClassOf ahmi:VehicleBehavior ;
    rdfs:label "Lane Detection System"@en ;
    rdfs:comment "A system that detects the lane markings and boundaries on the road."@en .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a driver to their current state."@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:DriverState .

ahmi:hasPreference a owl:ObjectProperty ;
    rdfs:label "has preference"@en ;
    rdfs:comment "Relates a driver to their preferred interface or feedback types."@en ;
    rdfs:domain ahmi:Driver ;
    rdfs:range ahmi:HMI .

ahmi:providesVisualFeedback a owl:ObjectProperty ;
    rdfs:label "provides visual feedback"@en ;
    rdfs:comment "Relates a vehicle system to the visual feedback it provides."@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:VisualFeedback .

ahmi:providesAudioFeedback a owl:ObjectProperty ;
    rdfs:label "provides audio feedback"@en ;
    rdfs:comment "Relates a vehicle system to the audio feedback it provides."@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:AudioFeedback .

ahmi:providesHapticFeedback a owl:ObjectProperty ;
    rdfs:label "provides haptic feedback"@en ;
    rdfs:comment "Relates a vehicle system to the haptic feedback it provides."@en ;
    rdfs:domain ahmi:HMI ;
    rdfs:range ahmi:HapticFeedback .

ahmi:hasVisualElement a owl:ObjectProperty ;
    rdfs:label "has visual element"@en ;
    rdfs:comment "Relates a visual feedback component to the individual visual elements it displays."@en ;
    rdfs:domain ahmi:VisualFeedback ;
    rdfs:range ahmi:VisualElement .

ahmi:hasAudioParameter a owl:ObjectProperty ;
    rdfs:label "has audio parameter"@en ;
    rdfs:comment "Relates an audio feedback component to the parameters that control its behavior."@en ;
    rdfs:domain ahmi:AudioFeedback ;
    rdfs:range ahmi:AudioParameter .

ahmi:hasDialogDesign a owl:ObjectProperty ;
    rdfs:label "has dialog design"@en ;
    rdfs:comment "Relates an audio feedback component to the design of its spoken dialog or prompts."@en ;
    rdfs:domain ahmi:AudioFeedback ;
    rdfs:range ahmi:DialogDesign .

ahmi:capturesEyeTrackingData a owl:ObjectProperty ;
    rdfs:label "captures eye tracking data"@en ;
    rdfs:comment "Relates an eye tracking sensor to the eye tracking data it captures."@en ;
    rdfs:domain ahmi:EyeTrackingSensor ;
    rdfs:range ahmi:EyeTrackingData .

ahmi:detectsEnvironmentalCondition a owl:ObjectProperty ;
    rdfs:label "detects environmental condition"@en ;
    rdfs:comment "Relates an environmental sensor to the environmental conditions it detects."@en ;
    rdfs:domain ahmi:EnvironmentalSensor ;
    rdfs:range ahmi:EnvironmentalCondition .

ahmi:affectsVisibility a owl:ObjectProperty ;
    rdfs:label "affects visibility"@en ;
    rdfs:comment "Relates an environmental condition to its impact on visibility."@en ;
    rdfs:domain ahmi:EnvironmentalCondition ;
    rdfs:range ahmi:Visibility .

ahmi:monitorsSteeringBehavior a owl:ObjectProperty ;
    rdfs:label "monitors steering behavior"@en ;
    rdfs:comment "Relates a vehicle system to the steering behavior it monitors."@en ;
    rdfs:domain ahmi:AdaptiveVehicleSystem ;
    rdfs:range ahmi:SteeringBehavior .

ahmi:providesLaneKeepingAssist a owl:ObjectProperty ;
    rdfs:label "provides lane keeping assist"@en ;
    rdfs:comment "Relates a vehicle system to the lane keeping assistance it provides."@en ;
    rdfs:domain ahmi:AdaptiveVehicleSystem ;
    rdfs:range ahmi:LaneKeepingAssist .

