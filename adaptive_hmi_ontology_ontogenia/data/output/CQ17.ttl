@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ahmi: <http://example.org/ahmi#> .

ahmi:DriverState a owl:Class ;
    rdfs:label "Driver State"@en ;
    rdfs:comment "The cognitive, emotional, and physiological state of the driver."@en .

ahmi:CognitiveState a owl:Class ;
    rdfs:label "Cognitive State"@en ;
    rdfs:comment "The cognitive state of the driver, including cognitive load, attention, and distraction."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:EmotionalState a owl:Class ;
    rdfs:label "Emotional State"@en ;
    rdfs:comment "The emotional state of the driver, such as stress or calmness."@en ;
    rdfs:subClassOf ahmi:DriverState .

ahmi:EnvironmentFactor a owl:Class ;
    rdfs:label "Environment Factor"@en ;
    rdfs:comment "External factors that influence the driving context, such as weather, traffic, and road conditions."@en .

ahmi:TrafficCondition a owl:Class ;
    rdfs:label "Traffic Condition"@en ;
    rdfs:comment "The traffic conditions, including congestion and vehicle behavior."@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:WeatherCondition a owl:Class ;
    rdfs:label "Weather Condition"@en ;
    rdfs:comment "The weather conditions, such as rain, snow, or clear skies."@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:RoadCondition a owl:Class ;
    rdfs:label "Road Condition"@en ;
    rdfs:comment "The conditions of the road, including type, complexity, and potential distractions."@en ;
    rdfs:subClassOf ahmi:EnvironmentFactor .

ahmi:DriverSituation a owl:Class ;
    rdfs:label "Driver Situation"@en ;
    rdfs:comment "A view or interpretation of the driver's state and the surrounding environment."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ahmi:satisfies ;
        owl:someValuesFrom ahmi:DriverDescription
    ] .

ahmi:DriverDescription a owl:Class ;
    rdfs:label "Driver Description"@en ;
    rdfs:comment "A conceptualization that provides context for interpreting driver situations."@en .

ahmi:satisfies a owl:ObjectProperty ;
    rdfs:label "satisfies"@en ;
    rdfs:comment "Relates a driver situation to the description it satisfies."@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:DriverDescription .

ahmi:hasDriverState a owl:ObjectProperty ;
    rdfs:label "has driver state"@en ;
    rdfs:comment "Relates a driver situation to the driver's cognitive, emotional, and physiological state."@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:DriverState .

ahmi:hasEnvironmentFactor a owl:ObjectProperty ;
    rdfs:label "has environment factor"@en ;
    rdfs:comment "Relates a driver situation to the external factors influencing the driving context."@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:EnvironmentFactor .

ahmi:impactsDriverState a owl:ObjectProperty ;
    rdfs:label "impacts driver state"@en ;
    rdfs:comment "Relates an environment factor to the driver state it influences."@en ;
    rdfs:domain ahmi:EnvironmentFactor ;
    rdfs:range ahmi:DriverState .

ahmi:hasTrafficCondition a owl:ObjectProperty ;
    rdfs:label "has traffic condition"@en ;
    rdfs:comment "Relates a driver situation to the traffic conditions."@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:TrafficCondition .

ahmi:hasWeatherCondition a owl:ObjectProperty ;
    rdfs:label "has weather condition"@en ;
    rdfs:comment "Relates a driver situation to the weather conditions."@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:WeatherCondition .

ahmi:hasRoadCondition a owl:ObjectProperty ;
    rdfs:label "has road condition"@en ;
    rdfs:comment "Relates a driver situation to the road conditions."@en ;
    rdfs:domain ahmi:DriverSituation ;
    rdfs:range ahmi:RoadCondition .

ahmi:CognitiveLoad a owl:Class ;
    rdfs:label "Cognitive Load"@en ;
    rdfs:comment "The level of cognitive load experienced by the driver."@en ;
    rdfs:subClassOf ahmi:CognitiveState .

ahmi:highCognitiveLoad a owl:Class ;
    rdfs:label "High Cognitive Load"@en ;
    rdfs:comment "A state of high cognitive load experienced by the driver."@en ;
    rdfs:subClassOf ahmi:CognitiveLoad .

ahmi:Stress a owl:Class ;
    rdfs:label "Stress"@en ;
    rdfs:comment "The level of stress experienced by the driver."@en ;
    rdfs:subClassOf ahmi:EmotionalState .

ahmi:highStress a owl:Class ;
    rdfs:label "High Stress"@en ;
    rdfs:comment "A state of high stress experienced by the driver."@en ;
    rdfs:subClassOf ahmi:Stress .

ahmi:highTrafficDensity a owl:Class ;
    rdfs:label "High Traffic Density"@en ;
    rdfs:comment "A traffic condition with high density or congestion."@en ;
    rdfs:subClassOf ahmi:TrafficCondition .

ahmi:highRoadComplexity a owl:Class ;
    rdfs:label "High Road Complexity"@en ;
    rdfs:comment "A road condition with high complexity, such as multiple lanes, construction, or potential distractions."@en ;
    rdfs:subClassOf ahmi:RoadCondition .

ahmi:moderateRain a owl:Class ;
    rdfs:label "Moderate Rain"@en ;
    rdfs:comment "A weather condition with moderate rainfall."@en ;
    rdfs:subClassOf ahmi:WeatherCondition .