@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns1: <http://ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix ns2: <http://example.org/ahmi#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/adaptive-hmi> a owl:Ontology ;
    rdfs:label "Adaptive Human-Machine Interface Ontology"@en ;
    rdfs:comment "An ontology for describing adaptive human-machine interfaces with a focus on driver state detection and multimodal feedback"@en .

ns2: a owl:Ontology .

ns2:Accuracy a owl:Class ;
    rdfs:label "Accuracy"@en,
        "Sensor Accuracy"@en ;
    rdfs:comment "A measurement metric that quantifies the accuracy or precision of the sensors used in a detection methodology."@en,
        "A measurement metric that quantifies the correctness or precision of a detection methodology."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:AdaptiveFeedback a owl:Class ;
    rdfs:label "Adaptive Feedback"@en ;
    rdfs:comment "The ability to adjust and adapt the feedback provided to the driver based on their monitored state and readiness during the transition."@en ;
    rdfs:subClassOf ns2:TrustCalibrationMechanism .

ns2:AdjustedPosture a owl:Class ;
    rdfs:label "Adjusted Posture"@en,
        "Adjusted Posture Data"@en ;
    rdfs:comment "A driver state with an adjusted body posture to maintain stability."@en,
        "Seat occupancy sensor data indicating an adjusted body posture to maintain stability."@en ;
    rdfs:subClassOf ns2:Driver,
        ns2:SensorData ;
    owl:disjointWith ns2:NormalPosture .

ns2:Age a owl:Class ;
    rdfs:label "Age"@en ;
    rdfs:comment "The age of the driver."@en ;
    rdfs:subClassOf ns2:Driver .

ns2:Aggressiveness a owl:Class ;
    rdfs:label "Aggressiveness"@en ;
    rdfs:comment "The tendency to act in an aggressive or hostile manner."@en ;
    rdfs:subClassOf ns2:BigFivePersonalityTrait .

ns2:AirFilteringResponseSituation a ns2:Situation,
        owl:NamedIndividual ;
    rdfs:label "Air Filtering Response Situation"@en ;
    ns2:hasClimateControlResponse ns2:AirFilteringResponse ;
    ns2:satisfies ns2:Description .

ns2:AmbientLighting a owl:Class ;
    rdfs:label "Ambient Lighting"@en,
        "Lighting"@en ;
    rdfs:comment "A visual feedback system that uses colored lighting in the vehicle interior to convey information or alerts."@en,
        "The ambient lighting in the vehicle cabin."@en,
        "The level and quality of illumination, including natural and artificial light sources."@en ;
    rdfs:subClassOf ns2:EnvironmentFactor,
        ns2:VisualFeedback .

ns2:Anger a owl:Class ;
    rdfs:label "Anger"@en ;
    rdfs:comment "The emotional state of anger."@en ;
    rdfs:subClassOf ns2:DiscreteEmotiveState .

ns2:AttentiveBodyPosition a owl:Class ;
    rdfs:label "Attentive Body Position"@en ;
    rdfs:comment "A body position indicating attentiveness and readiness for driving."@en ;
    rdfs:subClassOf ns2:BodyPosture .

ns2:AudibleFeedback a owl:Class ;
    rdfs:label "Audible Feedback"@en ;
    rdfs:comment "Audio cues and notifications provided to the driver."@en,
        "Auditory notifications and sounds provided to the driver."@en,
        "Feedback conveyed through audible sounds or tones."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns2:providesFor ;
            owl:someValuesFrom ns2:LaneKeepingAssist ],
        ns2:FeedbackModality .

ns2:AutonomousMode a owl:Class ;
    rdfs:label "Autonomous Mode"@en ;
    rdfs:comment "The mode of operation where the vehicle is driving autonomously."@en ;
    rdfs:subClassOf ns2:Situation .

ns2:AutonomousToManualTransition a owl:Class ;
    rdfs:label "Autonomous to Manual Transition"@en ;
    rdfs:comment "The process of transitioning from autonomous driving mode to manual driving mode."@en ;
    rdfs:subClassOf ns2:Event .

ns2:BehavioralAnalysis a owl:Class ;
    rdfs:label "Behavioral Analysis"@en ;
    rdfs:comment "An analysis of the driver's behavior to detect patterns or anomalies."@en,
        "An analysis of the driver's behavior, such as steering wheel grip or eye movements, to infer their cognitive and emotional state."@en,
        "The process of analyzing and interpreting patterns of behavior to infer cognitive or emotional states."@en ;
    rdfs:subClassOf ns2:DetectionMethod,
        ns2:DriverMonitoring .

ns2:BehavioralState a owl:Class ;
    rdfs:label "Behavioral State"@en ;
    rdfs:comment "The behavioral aspects of the driver's state, such as eye tracking, steering behavior, and lane keeping."@en ;
    rdfs:subClassOf ns2:Driver .

ns2:BehavioralTendency rdfs:label "Behavioral Tendency"@en ;
    rdfs:comment "A tendency or pattern in the driver's behavior, such as a tendency for speed violations."@en ;
    rdfs:subClassOf ns2:Driver .

ns2:BiometricMonitoring a owl:Class ;
    rdfs:label "Biometric Monitoring"@en ;
    rdfs:comment "Detection methodology that uses biometric data, such as heart rate, respiration, and brain activity, to assess the driver's state."@en ;
    rdfs:subClassOf ns2:DetectionMethodology .

ns2:BlinkDetection a owl:Class ;
    rdfs:label "Blink Detection"@en ;
    rdfs:comment "A method for detecting blink patterns"@en ;
    rdfs:subClassOf ns2:DetectionMethod .

ns2:BrainActivity a owl:Class ;
    rdfs:label "Brain Activity"@en ;
    rdfs:comment "A measurement of the driver's brain activity, such as through electroencephalography (EEG)."@en ;
    rdfs:subClassOf ns2:PhysiologicalMeasurement .

ns2:BrakingBehavior a owl:Class ;
    rdfs:label "Braking Behavior"@en ;
    rdfs:comment "The behavior exhibited by a driver when applying the brakes of a vehicle."@en ;
    rdfs:subClassOf ns2:ContextualFactor .

ns2:BrightnessAdjustment a owl:Class ;
    rdfs:label "Brightness Adjustment"@en ;
    rdfs:comment "The adaptation of display brightness to improve visibility and reduce eye strain."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:CalmerVoiceTone a ns2:AudioFeedback,
        owl:NamedIndividual ;
    rdfs:label "Calmer Voice Tone"@en ;
    rdfs:comment "Adaptation that uses a calmer voice tone for audio output from the vehicle's system."@en .

ns2:CameraDetection a owl:Class ;
    rdfs:label "Camera Detection"@en ;
    rdfs:comment "A detection methodology that uses cameras to monitor the driver's behavior and attention."@en,
        "Using cameras to detect visual cues about the driver or environment."@en ;
    rdfs:subClassOf ns2:DetectionMethod,
        ns2:DetectionMethodology .

ns2:CenterConsoleDisplay a owl:Class ;
    rdfs:label "Center Console Display"@en ;
    rdfs:comment "A visual feedback system that provides information and controls through a display integrated into the center console."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:CleanAirSituation a ns2:Situation,
        owl:NamedIndividual ;
    rdfs:label "Clean Air Situation"@en ;
    ns2:hasAirQuality ns2:Clean ;
    ns2:satisfies ns2:Description .

ns2:ClearConciseVoiceOutput a ns2:AudioFeedback,
        owl:NamedIndividual ;
    rdfs:label "Clear and Concise Voice Output"@en ;
    rdfs:comment "Adaptation that provides clear and concise voice output from the vehicle's audio system."@en .

ns2:ComputationalComplexity a owl:Class ;
    rdfs:label "Computational Complexity"@en ;
    rdfs:comment "A measurement metric that quantifies the computational resources required by a detection methodology, such as time or space complexity."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:ContrastAdjustment a owl:Class ;
    rdfs:label "Contrast Adjustment"@en ;
    rdfs:comment "The adaptation of display contrast to improve visibility and reduce eye strain."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:CountdownTimer a owl:Class ;
    rdfs:label "Countdown Timer"@en ;
    rdfs:comment "A visual element displaying a countdown timer."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:DeviationFromExpectedTimeOnTask a owl:Class ;
    rdfs:label "Deviation from Expected Time on Task"@en ;
    rdfs:comment "A situation where the actual time spent on a task deviates significantly from the expected or planned duration."@en,
        "A situation where the actual time spent on a task deviates significantly from the expected time."@en ;
    rdfs:subClassOf ns2:JourneyContext .

ns2:DeviationFromNormalBehaviour a owl:Class ;
    rdfs:label "Deviation from Normal Behaviour"@en ;
    rdfs:comment "A significant departure from the driver's typical driving patterns."@en ;
    rdfs:subClassOf ns2:DrivingBehaviour .

ns2:DrivingInterface a owl:Class ;
    rdfs:label "Driving Interface"@en ;
    rdfs:comment "The visual interface displaying driving-related information."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:Drowsiness a owl:Class ;
    rdfs:label "Drowsiness"@en,
        "Drowsiness Detection Description"@en,
        "Drowsiness Detection Situation"@en ;
    rdfs:comment "A conceptualization for interpreting situations as drowsiness detection"@en,
        "A situation where drowsiness is detected in the driver"@en,
        "A state of feeling sleepy or fatigued"@en,
        "A state of feeling sleepy or fatigued."@en,
        "A state of impaired alertness and reduced ability to perform tasks due to lack of sleep or fatigue."@en ;
    rdfs:subClassOf ns2:Description,
        ns2:InattentionState,
        ns2:Situation .

ns2:EffortComponent a owl:Class ;
    rdfs:label "Effort Component"@en ;
    rdfs:subClassOf ns2:NASATLXMeasure .

ns2:Electroencephalogram a owl:Class ;
    rdfs:label "Electroencephalogram"@en ;
    rdfs:comment "A physiological detection method that measures brain activity using electroencephalography (EEG)."@en ;
    rdfs:subClassOf ns2:PhysiologicalDetection .

ns2:Extraversion a owl:Class ;
    rdfs:label "Extraversion"@en ;
    rdfs:comment "The tendency to be outgoing, sociable, and assertive."@en ;
    rdfs:subClassOf ns2:BigFivePersonalityTrait .

ns2:F1Score a owl:Class ;
    rdfs:label "F1 Score"@en ;
    rdfs:comment "A measurement metric that combines precision and recall into a single score, representing the harmonic mean of the two."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:Fog a owl:Class ;
    rdfs:label "Fog"@en ;
    rdfs:comment "Weather condition with reduced visibility due to suspended water droplets."@en ;
    rdfs:subClassOf ns2:WeatherCondition .

ns2:ForceThresholdParameter a owl:Class ;
    rdfs:label "Force Threshold Parameter"@en ;
    rdfs:comment "A parameter representing the threshold force value for a feedback mechanism." ;
    rdfs:subClassOf ns2:ForceParameter .

ns2:Frequency a owl:Class,
        owl:DatatypeProperty ;
    rdfs:label "Frequency"@en,
        "Micro-adjustments Frequency Value"@en,
        "frequency"@en,
        "has frequency"@en ;
    rdfs:comment "The frequency or commonality of a non-driving activity occurring in autonomous mode."@en,
        "The frequency or rate of a haptic alert."@en,
        "The frequency or repetition rate of a tactile alert."@en,
        "The measured frequency of micro-adjustments made by the driver."@en ;
    rdfs:domain ns2:HapticAlert,
        ns2:MicroadjustmentsFrequency,
        ns2:NonDrivingActivity,
        ns2:TactileCharacteristic ;
    rdfs:range xsd:float ;
    rdfs:subClassOf ns2:HapticAlert .

ns2:Happiness a owl:Class ;
    rdfs:label "Happiness"@en ;
    rdfs:comment "The emotional state of happiness."@en ;
    rdfs:subClassOf ns2:DiscreteEmotiveState .

ns2:HeadUpDisplay a owl:Class ;
    rdfs:label "Head-Up Display"@en ;
    rdfs:comment "A visual feedback system that projects information onto the windshield or a transparent display in the driver's line of sight."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:HeartRateMonitoring a owl:Class ;
    rdfs:label "Heart Rate Monitoring"@en ;
    rdfs:comment "A measurement of the driver's heart rate, such as through electrocardiography (ECG)."@en,
        "A physiological detection method that measures heart rate using electrocardiography (ECG)."@en ;
    rdfs:subClassOf ns2:PhysiologicalDetection,
        ns2:PhysiologicalMeasurement .

ns2:HeartRateSensor a owl:Class ;
    rdfs:label "Heart Rate Sensor"@en ;
    rdfs:comment "A sensor that measures the driver's heart rate."@en ;
    rdfs:subClassOf ns2:PhysiologicalSensor .

ns2:HighCongestion a owl:Class ;
    rdfs:label "High Congestion"@en ;
    rdfs:comment "A traffic condition characterized by a high volume of vehicles and slow or stopped movement."@en,
        "A traffic condition with high levels of congestion."@en ;
    rdfs:subClassOf ns2:TrafficCondition .

ns2:HighContrastDisplay a owl:Class ;
    rdfs:label "High Contrast Display"@en ;
    rdfs:comment "A display type with enhanced contrast between visual elements and background."@en ;
    rdfs:subClassOf ns2:DisplayType .

ns2:HighWorkloadState a owl:Class ;
    rdfs:label "High Workload State"@en ;
    rdfs:comment "A cognitive state characterized by high mental workload or cognitive strain."@en ;
    rdfs:subClassOf ns2:CognitiveState .

ns2:Highway a owl:Class ;
    rdfs:label "Highway"@en ;
    rdfs:subClassOf ns2:RoadType .

ns2:Impairment a owl:Class ;
    rdfs:label "Impairment"@en ;
    rdfs:comment "A condition that reduces the driver's ability to operate the vehicle safely."@en,
        "A driver state where the driver's abilities are diminished due to factors such as alcohol, drugs, or medical conditions."@en ;
    rdfs:subClassOf ns2:Driver .

ns2:IncreaseComprehensibility a owl:Class ;
    rdfs:label "Increase Comprehensibility"@en ;
    rdfs:comment "An adaptation strategy that increases the comprehensibility of messages and feedback."@en ;
    rdfs:subClassOf ns2:Wording .

ns2:IncreaseSteeringAssist a ns2:Driver,
        owl:NamedIndividual ;
    rdfs:label "Increase Steering Assist"@en ;
    rdfs:comment "Adaptation that increases the level of steering assistance provided by the vehicle's driver assistance system."@en .

ns2:IncreaseVisualContrast a ns2:VisualFeedback,
        owl:NamedIndividual ;
    rdfs:label "Increase Visual Contrast"@en ;
    rdfs:comment "Adaptation that increases the contrast of visual elements on the vehicle's HMI."@en .

ns2:InstrumentCluster a owl:Class ;
    rdfs:label "Instrument Cluster"@en ;
    rdfs:comment "A visual feedback system that displays vehicle information and status indicators on a dashboard panel."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:LaneDepartureSituation rdfs:label "Lane Departure Situation"@en ;
    rdfs:comment "A situation representing the interpretation of a lane departure event."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns2:observes ;
            owl:someValuesFrom ns2:Event ],
        ns2:Situation .

ns2:LaneDetectionVisualization a owl:Class ;
    rdfs:label "Lane Detection Visualization"@en ;
    rdfs:comment "A visual representation of the detected lanes on the road."@en ;
    rdfs:subClassOf ns2:VisualFeedback .

ns2:LaneDriftAlert a owl:Class ;
    rdfs:label "Lane Drift Alert"@en ;
    rdfs:comment "A haptic alert that notifies the driver when drifting out of the lane."@en ;
    rdfs:subClassOf ns2:SteeringWheelAlert .

ns2:LaneKeepingAssistAdaptation rdfs:label "Lane Keeping Assist Adaptation"@en ;
    rdfs:comment "An adaptation of the lane keeping assist system, providing varying levels of steering support based on the driver's state."@en ;
    rdfs:subClassOf ns2:ADASAdaptation .

ns2:Location a owl:Class ;
    rdfs:label "Location"@en ;
    rdfs:comment "The location or position of a haptic alert."@en ;
    rdfs:subClassOf ns2:HapticAlert .

ns2:LowConscientiousness a owl:Class ;
    rdfs:label "Conscientiousness"@en,
        "Low Conscientiousness"@en ;
    rdfs:comment "A personality trait characterized by a lack of self-discipline, carefulness, and organization."@en,
        "The tendency to be organized, responsible, and self-disciplined."@en ;
    rdfs:subClassOf ns2:BigFivePersonalityTrait,
        ns2:PersonalityFactor .

ns2:LowWorkloadState a owl:Class ;
    rdfs:label "Low Workload State"@en ;
    rdfs:comment "A cognitive state characterized by low mental workload or cognitive strain."@en ;
    rdfs:subClassOf ns2:CognitiveState .

ns2:LowerBackgroundAudio a ns2:AudioFeedback,
        owl:NamedIndividual ;
    rdfs:label "Lower Background Audio"@en ;
    rdfs:comment "Adaptation that lowers the volume or mutes background audio in the vehicle."@en .

ns2:MentalWorkloadComponent a owl:Class ;
    rdfs:label "Mental Workload Component"@en ;
    rdfs:subClassOf ns2:NASATLXMeasure .

ns2:MultimodalIntegrationFramework a owl:Class ;
    rdfs:label "Multimodal Integration Framework"@en ;
    rdfs:comment "A framework for integrating multiple modalities in human-machine interactions."@en .

ns2:NavigationDisplay a owl:Class ;
    rdfs:label "Navigation Display"@en ;
    rdfs:comment "The display for showing navigation information."@en ;
    rdfs:subClassOf ns2:VisualElement .

ns2:NavigationPrompt a owl:Class ;
    rdfs:label "Navigation Prompt"@en ;
    rdfs:subClassOf ns2:AuditoryDisplay .

ns2:Neuroticism a owl:Class ;
    rdfs:label "Neuroticism"@en ;
    rdfs:comment "The tendency to experience negative emotions, such as anxiety and depression."@en ;
    rdfs:subClassOf ns2:BigFivePersonalityTrait .

ns2:NightTime a owl:Class ;
    rdfs:label "Night Time"@en ;
    rdfs:subClassOf ns2:DaytimeCondition ;
    owl:disjointWith ns2:DayTime .

ns2:Nighttime a owl:Class ;
    rdfs:label "Nighttime"@en ;
    rdfs:comment "The period of the day when the sun is below the horizon."@en ;
    rdfs:subClassOf ns2:DaytimeCondition .

ns2:NoddingPattern a owl:Class ;
    rdfs:label "Nodding Pattern"@en,
        "Pattern"@en ;
    rdfs:comment "A pattern of head nodding or drooping"@en,
        "A pattern of repetitive head movements indicating drowsiness or fatigue."@en,
        "The pattern or rhythm of a haptic alert."@en ;
    rdfs:subClassOf ns2:HapticAlert,
        ns2:HeadPosition .

ns2:NonDrivingRelatedActivity a owl:Class ;
    rdfs:label "Non-Driving Related Activity"@en ;
    rdfs:comment "An activity not related to the driving task, such as reading emails."@en ;
    rdfs:subClassOf ns2:DistractionState .

ns2:NotificationDisplay a owl:Class ;
    rdfs:label "Notification Display"@en ;
    rdfs:comment "The display for showing notifications to the user."@en ;
    rdfs:subClassOf ns2:VisualElement .

ns2:Openness a owl:Class ;
    rdfs:label "Openness"@en ;
    rdfs:comment "The tendency to be open to new experiences, ideas, and perspectives."@en ;
    rdfs:subClassOf ns2:BigFivePersonalityTrait .

ns2:PauseIncomingNotifications a ns2:VisualFeedback,
        owl:NamedIndividual ;
    rdfs:label "Pause Incoming Notifications"@en ;
    rdfs:comment "Adaptation that pauses or stops incoming notifications on the vehicle's HMI."@en .

ns2:PedalControlAnalysis a owl:Class ;
    rdfs:label "Pedal Control Analysis"@en ;
    rdfs:comment "A method that analyzes the driver's pedal control behavior to detect emotional states"@en ;
    rdfs:subClassOf ns2:DetectionMethod .

ns2:PhysicalState a owl:Class ;
    rdfs:label "Physical State"@en ;
    rdfs:comment "The physical aspects of the driver's state, such as age, vision, and motor abilities."@en ;
    rdfs:subClassOf ns2:Driver .

ns2:PhysiologicalSensing a owl:Class ;
    rdfs:label "Physiological Sensing"@en ;
    rdfs:comment "A detection methodology that uses physiological sensors to monitor the driver's physical state."@en ;
    rdfs:subClassOf ns2:DetectionMethodology .

ns2:PollenSituation a ns2:Situation,
        owl:NamedIndividual ;
    rdfs:label "Pollen Situation"@en ;
    ns2:hasAirQuality ns2:Pollen ;
    ns2:satisfies ns2:Description .

ns2:PollutionSituation a ns2:Situation,
        owl:NamedIndividual ;
    rdfs:label "Pollution Situation"@en ;
    ns2:hasAirQuality ns2:Pollution ;
    ns2:satisfies ns2:Description .

ns2:PostureAdaptationStrategy a owl:Class ;
    rdfs:label "PostureAdaptationStrategy"@en ;
    rdfs:comment "A strategy for adapting the HMI based on the driver's posture"@en ;
    rdfs:subClassOf ns2:AdaptationStrategy .

ns2:PostureDetectionMethod a owl:Class ;
    rdfs:label "PostureDetectionMethod"@en ;
    rdfs:comment "A method for detecting the posture of a driver"@en ;
    rdfs:subClassOf ns2:DetectionMethod .

ns2:Precision a owl:Class ;
    rdfs:label "Precision"@en ;
    rdfs:comment "A measurement metric that quantifies the proportion of true positive results among all positive results."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:Rain a ns2:WeatherCondition,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Rain"@en ;
    rdfs:comment "A weather condition involving precipitation in the form of liquid water droplets."@en,
        "Weather condition characterized by precipitation in the form of liquid water droplets."@en ;
    rdfs:subClassOf ns2:WeatherCondition .

ns2:ReadingEmails a owl:Class ;
    rdfs:label "Reading Emails"@en ;
    rdfs:comment "The activity of reading emails while in the vehicle."@en ;
    rdfs:subClassOf ns2:NonDrivingActivity .

ns2:Recall a owl:Class ;
    rdfs:label "Recall"@en ;
    rdfs:comment "A measurement metric that quantifies the proportion of true positive results among all actual positive cases."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:ReduceNonEssentialVisualElements a ns2:VisualFeedback,
        owl:NamedIndividual ;
    rdfs:label "Reduce Non-Essential Visual Elements"@en ;
    rdfs:comment "Adaptation that reduces or hides non-essential visual elements on the vehicle's HMI."@en .

ns2:RelaxedBodyPosition a owl:Class ;
    rdfs:label "Relaxed Body Position"@en ;
    rdfs:comment "A relaxed body position indicating low vigilance."@en ;
    rdfs:subClassOf ns2:BodyPosture .

ns2:ResponseTime a owl:Class ;
    rdfs:label "Response Time"@en ;
    rdfs:comment "A measurement metric that quantifies the time taken by a detection methodology to respond or provide a result."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:RoadComplexity rdfs:label "Road Complexity"@en ;
    rdfs:comment "A factor describing the complexity of the road environment, such as the presence of construction, pedestrians, or unpredictable traffic."@en ;
    rdfs:subClassOf ns2:EnvironmentFactor .

ns2:RoadFacingCamera a owl:Class ;
    rdfs:label "Road-facing Camera"@en ;
    rdfs:comment "A camera system mounted on a vehicle to capture visual information about the road and environment."@en ;
    rdfs:subClassOf ns2:Vehicle .

ns2:Sadness a owl:Class ;
    rdfs:label "Sadness"@en ;
    rdfs:comment "The emotional state of sadness."@en ;
    rdfs:subClassOf ns2:DiscreteEmotiveState .

ns2:SeatOccupancySensor a owl:Class ;
    rdfs:label "Seat Occupancy Sensor"@en ;
    rdfs:comment "A sensor that detects the occupancy and posture of the driver in the vehicle seat."@en .

ns2:SensorDetection a owl:Class ;
    rdfs:label "Sensor Detection"@en ;
    rdfs:comment "Using sensors to detect physiological or environmental data."@en ;
    rdfs:subClassOf ns2:DetectionMethod .

ns2:SensorResolution a owl:Class ;
    rdfs:label "Sensor Resolution"@en ;
    rdfs:comment "A measurement metric that quantifies the level of detail or granularity provided by the sensors used in a detection methodology."@en ;
    rdfs:subClassOf ns2:MeasurementMetric .

ns2:SimplifiedDisplay a owl:Class ;
    rdfs:label "Simplified Display"@en ;
    rdfs:comment "A display type with fewer visual elements and simpler layout."@en,
        "A display with reduced complexity."@en ;
    rdfs:subClassOf ns2:DisplayType .

ns2:SimplifyVisualElements a ns2:VisualFeedback,
        owl:NamedIndividual ;
    rdfs:label "Simplify Visual Elements"@en ;
    rdfs:comment "Adaptation that simplifies the visual elements displayed on the vehicle's HMI."@en .

ns2:SlowerReactionTime a owl:Class ;
    rdfs:label "Slower Reaction Time"@en ;
    rdfs:comment "A cognitive state characterized by slower reaction times",
        "A cognitive state characterized by slower reaction times."@en,
        "A condition where a driver has a slower response time."@en ;
    rdfs:subClassOf ns2:AgeRelatedFactor,
        ns2:CognitiveState .

ns2:SpeechRecognition a owl:Class ;
    rdfs:label "Speech Recognition"@en ;
    rdfs:comment "A method that analyzes the driver's speech to detect emotional states"@en ;
    rdfs:subClassOf ns2:DetectionMethod .

ns2:SteeringWheelSensor a owl:Class ;
    rdfs:label "Steering Wheel Sensor"@en ;
    rdfs:comment "A sensor that monitors the driver's grip or interaction with the steering wheel."@en ;
    rdfs:subClassOf ns2:BehavioralSensor .

ns2:TactileAlert a owl:Class ;
    rdfs:label "Tactile Alert"@en ;
    rdfs:comment "A haptic feedback mechanism used to alert the driver."@en ;
    rdfs:subClassOf ns2:FeedbackModality .

ns2:TrafficDensity rdfs:label "Traffic Density"@en ;
    rdfs:comment "A factor representing the level of congestion and traffic density on the road."@en ;
    rdfs:subClassOf ns2:EnvironmentFactor .

ns2:TransitionAlert a owl:Class ;
    rdfs:label "Transition Alert"@en ;
