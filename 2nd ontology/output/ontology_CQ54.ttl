@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:DriverReadiness a owl:Class ;
    rdfs:label "Driver Readiness" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasGazePattern ;
        owl:someValuesFrom :GazePattern
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasPupilSize ;
        owl:someValuesFrom :PupilSize
    ] .

:GazePattern a owl:Class ;
    rdfs:label "Gaze Pattern" .

:PupilSize a owl:Class ;
    rdfs:label "Pupil Size" .

:hasGazePattern a owl:ObjectProperty ;
    rdfs:label "has Gaze Pattern" ;
    rdfs:domain :DriverReadiness ;
    rdfs:range :GazePattern .

:hasPupilSize a owl:ObjectProperty ;
    rdfs:label "has Pupil Size" ;
    rdfs:domain :DriverReadiness ;
    rdfs:range :PupilSize .