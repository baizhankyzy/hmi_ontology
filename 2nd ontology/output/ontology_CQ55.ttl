@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:SeatVibrator a owl:Class ;
    rdfs:label "SeatVibrator" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :generatesTouchCues ;
        owl:someValuesFrom :TouchCues
    ] .

:TouchCues a owl:Class ;
    rdfs:label "TouchCues" ;
    rdfs:subClassOf :Cue .

:Cue a owl:Class ;
    rdfs:label "Cue" .

:generatesTouchCues a owl:ObjectProperty ;
    rdfs:label "generatesTouchCues" ;
    rdfs:domain :SeatVibrator ;
    rdfs:range :TouchCues .

:TransitionAlerting a owl:Class ;
    rdfs:label "TransitionAlerting" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :involvesCue ;
        owl:someValuesFrom :Cue
    ] .

:involvesCue a owl:ObjectProperty ;
    rdfs:label "involvesCue" ;
    rdfs:domain :TransitionAlerting ;
    rdfs:range :Cue .