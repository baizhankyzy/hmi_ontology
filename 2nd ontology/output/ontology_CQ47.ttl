@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:NavigationDisplay a owl:Class ;
    rdfs:label "NavigationDisplay" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasComplexity ;
        owl:someValuesFrom :DisplayComplexity
    ] .

:DisplayComplexity a owl:Class ;
    rdfs:label "DisplayComplexity" .

:ReactionTime a owl:Class ;
    rdfs:label "ReactionTime" .

:hasComplexity a owl:ObjectProperty ;
    rdfs:label "hasComplexity" ;
    rdfs:domain :NavigationDisplay ;
    rdfs:range :DisplayComplexity .

:adaptsBasedOn a owl:ObjectProperty ;
    rdfs:label "adaptsBasedOn" ;
    rdfs:domain :DisplayComplexity ;
    rdfs:range :ReactionTime .