@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:CrossModalChecker a owl:Class ;
    rdfs:label "CrossModalChecker" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :identifiesMatchingPatterns ;
        owl:someValuesFrom :MatchingPattern
    ] .

:MatchingPattern a owl:Class ;
    rdfs:label "MatchingPattern" .

:identifiesMatchingPatterns a owl:ObjectProperty ;
    rdfs:label "identifiesMatchingPatterns" ;
    rdfs:domain :CrossModalChecker ;
    rdfs:range :MatchingPattern .

:DataComparison a owl:Class ;
    rdfs:label "DataComparison" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasInput ;
        owl:someValuesFrom :Data
    ] .

:Data a owl:Class ;
    rdfs:label "Data" .

:hasInput a owl:ObjectProperty ;
    rdfs:label "hasInput" ;
    rdfs:domain :DataComparison ;
    rdfs:range :Data .