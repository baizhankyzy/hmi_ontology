@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:VoiceAssistant a owl:Class ;
    rdfs:label "VoiceAssistant" .

:AlertMessage a owl:Class ;
    rdfs:label "AlertMessage" .

:DrowsinessState a owl:Class ;
    rdfs:label "DrowsinessState" ;
    rdfs:subClassOf :InattentionState .

:InattentionState a owl:Class ;
    rdfs:label "InattentionState" ;
    rdfs:subClassOf :CognitiveState .

:CognitiveState a owl:Class ;
    rdfs:label "CognitiveState" .

:generatesAlertFor a owl:ObjectProperty ;
    rdfs:label "generatesAlertFor" ;
    rdfs:domain :VoiceAssistant ;
    rdfs:range :AlertMessage ;
    rdfs:subPropertyOf [
        a owl:Restriction ;
        owl:onProperty :forDriverState ;
        owl:someValuesFrom :DrowsinessState
    ] .

:forDriverState a owl:ObjectProperty ;
    rdfs:label "forDriverState" ;
    rdfs:domain :AlertMessage ;
    rdfs:range :CognitiveState .