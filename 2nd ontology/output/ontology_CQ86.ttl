@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:DriverAssistanceFeature a owl:Class ;
    rdfs:label "Driver Assistance Feature" .

:MatchingChecker a owl:Class ;
    rdfs:label "Matching Checker" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :assessesCoordinationFor ;
        owl:someValuesFrom :DriverAssistanceFeature
    ] .

:assessesCoordinationFor a owl:ObjectProperty ;
    rdfs:label "assesses coordination for" ;
    rdfs:domain :MatchingChecker ;
    rdfs:range :DriverAssistanceFeature .