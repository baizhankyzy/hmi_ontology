@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:TimingController a owl:Class ;
    rdfs:label "TimingController" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :adjustsHandoverBasedOn ;
        owl:someValuesFrom :CognitiveState
    ] .

:CognitiveState a owl:Class ;
    rdfs:label "CognitiveState" .

:adjustsHandoverBasedOn a owl:ObjectProperty ;
    rdfs:label "adjustsHandoverBasedOn" ;
    rdfs:domain :TimingController ;
    rdfs:range :CognitiveState .

:ReadinessScore a owl:Class ;
    rdfs:label "ReadinessScore" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :isParameterFor ;
        owl:someValuesFrom :TimingController
    ] .

:isParameterFor a owl:ObjectProperty ;
    rdfs:label "isParameterFor" ;
    rdfs:domain :ReadinessScore ;
    rdfs:range :TimingController .