@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:DistractionState a owl:Class ;
    rdfs:label "Distraction State" .

:ConductanceAnalysis a owl:Class ;
    rdfs:label "Conductance Analysis" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :providesFor ;
        owl:someValuesFrom :DistractionState
    ] .

:VariabilityAnalysis a owl:Class ;
    rdfs:label "Variability Analysis" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :providesFor ;
        owl:someValuesFrom :DistractionState
    ] .

:providesFor a owl:ObjectProperty ;
    rdfs:label "provides for" ;
    rdfs:domain [ rdf:type owl:Class ; owl:unionOf ( :ConductanceAnalysis :VariabilityAnalysis ) ] ;
    rdfs:range :DistractionState .