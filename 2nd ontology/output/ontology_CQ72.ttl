@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:VirtualSimulator a owl:Class ;
    rdfs:label "VirtualSimulator" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :provides ;
        owl:someValuesFrom :DriverStudyCapability
    ] .

:DriverStudyCapability a owl:Class ;
    rdfs:label "DriverStudyCapability" .

:provides a owl:ObjectProperty ;
    rdfs:label "provides" ;
    rdfs:domain :VirtualSimulator ;
    rdfs:range :DriverStudyCapability .

:PersonalityTest a owl:Class ;
    rdfs:subClassOf :DriverStudyCapability .

