@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:LaneController a owl:Class ;
    rdfs:label "LaneController" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :activatesThrough ;
        owl:someValuesFrom :SteeringActuator
    ] .

:SteeringWheel a owl:Class ;
    rdfs:label "SteeringWheel" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :generatesForceFeedback ;
        owl:someValuesFrom :ForceFeedback
    ] .

:SteeringActuator a owl:Class ;
    rdfs:label "SteeringActuator" .

:ForceFeedback a owl:Class ;
    rdfs:label "ForceFeedback" .

:activatesThrough a owl:ObjectProperty ;
    rdfs:label "activatesThrough" ;
    rdfs:domain :LaneController ;
    rdfs:range :SteeringActuator .

:generatesForceFeedback a owl:ObjectProperty ;
    rdfs:label "generatesForceFeedback" ;
    rdfs:domain :SteeringWheel ;
    rdfs:range :ForceFeedback .