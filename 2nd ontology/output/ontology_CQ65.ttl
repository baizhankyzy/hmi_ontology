@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:WaveAnalysis a owl:Class ;
    rdfs:label "WaveAnalysis" .

:CognitiveState a owl:Class ;
    rdfs:label "CognitiveState" .

:BrainMonitor a owl:Class ;
    rdfs:label "BrainMonitor" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :performsWaveAnalysis ;
        owl:someValuesFrom :WaveAnalysis
    ] .

:performsWaveAnalysis a owl:ObjectProperty ;
    rdfs:label "performsWaveAnalysis" ;
    rdfs:domain :BrainMonitor ;
    rdfs:range :WaveAnalysis .

:indicatesCognitiveState a owl:ObjectProperty ;
    rdfs:label "indicatesCognitiveState" ;
    rdfs:domain :WaveAnalysis ;
    rdfs:range :CognitiveState .