@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:SweatAnalysis a owl:Class ;
    rdfs:label "SweatAnalysis" .

:FrustrationLevel a owl:Class ;
    rdfs:label "FrustrationLevel" .

:SweatAnalysisPattern a owl:Class ;
    rdfs:label "SweatAnalysisPattern" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :correlatesWith ;
        owl:someValuesFrom :FrustrationLevel
    ] .

:correlatesWith a owl:ObjectProperty ;
    rdfs:label "correlatesWith" ;
    rdfs:domain :SweatAnalysisPattern ;
    rdfs:range :FrustrationLevel .