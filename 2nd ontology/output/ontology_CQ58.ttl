@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:LaneVisualizer a owl:Class ;
    rdfs:label "LaneVisualizer" .

:HandoverSituation a owl:Class ;
    rdfs:label "HandoverSituation" ;
    rdfs:subClassOf :Situation ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasVisualization ;
        owl:someValuesFrom :LaneVisualizer
    ] .

:hasVisualization a owl:ObjectProperty ;
    rdfs:label "hasVisualization" ;
    rdfs:domain :HandoverSituation ;
    rdfs:range :Visualization .

:Visualization a owl:Class ;
    rdfs:label "Visualization" .

:Situation a owl:Class .