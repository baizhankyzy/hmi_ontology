@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:SoundAnalysis a owl:Class ;
    rdfs:label "Sound Analysis" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :detectsAngerExpression ;
        owl:someValuesFrom :AngerExpression
    ] .

:HighCongestion a owl:Class ;
    rdfs:label "High Congestion" .

:AngerExpression a owl:Class ;
    rdfs:label "Anger Expression" .

:detectsAngerExpression a owl:ObjectProperty ;
    rdfs:label "detects anger expression" ;
    rdfs:domain :SoundAnalysis ;
    rdfs:range :AngerExpression .