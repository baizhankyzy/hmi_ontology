@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:RoadSensor a owl:Class ;
    rdfs:label "RoadSensor" .

:PostureAnalysis a owl:Class ;
    rdfs:label "PostureAnalysis" .

:SurfaceCondition a owl:Class ;
    rdfs:label "SurfaceCondition" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :detectedBy ;
        owl:someValuesFrom :RoadSensor
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :affects ;
        owl:someValuesFrom :PostureAnalysis
    ] .

:detectedBy a owl:ObjectProperty ;
    rdfs:label "detectedBy" ;
    rdfs:domain :SurfaceCondition ;
    rdfs:range :RoadSensor .

:affects a owl:ObjectProperty ;
    rdfs:label "affects" ;
    rdfs:domain :SurfaceCondition ;
    rdfs:range :PostureAnalysis .