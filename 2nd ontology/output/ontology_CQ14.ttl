@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:HighCongestion a owl:Class ;
    rdfs:label "HighCongestion" .

:StressLevel a owl:Class ;
    rdfs:label "StressLevel" .

:HeartMonitor a owl:Class ;
    rdfs:label "HeartMonitor" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :detectsStressLevelDuring ;
        owl:someValuesFrom :StressLevel
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :detectsStressLevelDuring ;
        owl:allValuesFrom :HighCongestion
    ] .

:SkinMonitor a owl:Class ;
    rdfs:label "SkinMonitor" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :detectsStressLevelDuring ;
        owl:someValuesFrom :StressLevel
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :detectsStressLevelDuring ;
        owl:allValuesFrom :HighCongestion
    ] .

:detectsStressLevelDuring a owl:ObjectProperty ;
    rdfs:label "detectsStressLevelDuring" ;
    rdfs:domain [ rdf:type owl:Class ; owl:unionOf ( :HeartMonitor :SkinMonitor ) ] ;
    rdfs:range :StressLevel .