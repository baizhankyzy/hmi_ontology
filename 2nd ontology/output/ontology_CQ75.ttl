@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:AirMonitor a owl:Class ;
    rdfs:label "AirMonitor" .

:PollenSensor a owl:Class ;
    rdfs:label "PollenSensor" .

:AirFilter a owl:Class ;
    rdfs:label "AirFilter" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasResponse ;
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:unionOf ( :AirMonitorReading :PollenSensorReading )
        ]
    ] .

:AirMonitorReading a owl:Class ;
    rdfs:label "AirMonitorReading" .

:PollenSensorReading a owl:Class ;
    rdfs:label "PollenSensorReading" .

:hasResponse a owl:ObjectProperty ;
    rdfs:label "hasResponse" ;
    rdfs:domain :AirFilter ;
    rdfs:range [
        rdf:type owl:Class ;
        owl:unionOf ( :AirMonitorReading :PollenSensorReading )
    ] .