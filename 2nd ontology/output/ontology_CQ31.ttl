<http://www.example.org/test#> a owl:Ontology .

:Driver a owl:Class ;
    rdfs:label "Driver" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasDriverCharacteristics ;
        owl:someValuesFrom :DriverCharacteristics
    ] .

:DriverCharacteristics a owl:Class ;
    rdfs:label "Driver Characteristics" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasPersonalityTraits ;
        owl:someValuesFrom :PersonalityTraits
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasPersonalityScore ;
        owl:someValuesFrom :PersonalityScore
    ] .

:PersonalityTraits a owl:Class ;
    rdfs:label "Personality Traits" ;
    rdfs:subClassOf [
        a owl:Restriction ;
