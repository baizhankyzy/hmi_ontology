@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:PersonalityScore a owl:Class ;
    rdfs:label "PersonalityScore" .

:VehicleSystemAdaptationEffectiveness a owl:Class ;
    rdfs:label "VehicleSystemAdaptationEffectiveness" .

:relatesTo a owl:ObjectProperty ;
    rdfs:label "relatesTo" ;
    rdfs:domain :PersonalityScore ;
    rdfs:range :VehicleSystemAdaptationEffectiveness .

:PersonalityProfile a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasPersonalityScore ;
        owl:someValuesFrom :PersonalityScore
    ] .

:hasPersonalityScore a owl:ObjectProperty ;
    rdfs:label "hasPersonalityScore" ;
    rdfs:domain :PersonalityProfile ;
    rdfs:range :PersonalityScore .

:VehicleSystem a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasAdaptationEffectiveness ;
        owl:someValuesFrom :VehicleSystemAdaptationEffectiveness
    ] .

:hasAdaptationEffectiveness a owl:ObjectProperty ;
    rdfs:label "hasAdaptationEffectiveness" ;
    rdfs:domain :VehicleSystem ;
    rdfs:range :VehicleSystemAdaptationEffectiveness .