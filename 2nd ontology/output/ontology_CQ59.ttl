@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:SeatAdjuster a owl:Class ;
    rdfs:label "SeatAdjuster" .

:PostureType a owl:Class ;
    rdfs:label "PostureType" .

:AttentivePosition a owl:Class ;
    rdfs:label "AttentivePosition" ;
    rdfs:subClassOf :PostureType .

:RelaxedPosition a owl:Class ;
    rdfs:label "RelaxedPosition" ;
    rdfs:subClassOf :PostureType .

:adjustsToPromote a owl:ObjectProperty ;
    rdfs:label "adjustsToPromote" ;
    rdfs:domain :SeatAdjuster ;
    rdfs:range :PostureType .

:hasPostureType a owl:ObjectProperty ;
    rdfs:label "hasPostureType" ;
    rdfs:domain :SeatAdjuster ;
    rdfs:range :PostureType .

:SeatAdjuster rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :adjustsToPromote ;
    owl:someValuesFrom :AttentivePosition
] .

:SeatAdjuster rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasPostureType ;
    owl:someValuesFrom :RelaxedPosition
] .