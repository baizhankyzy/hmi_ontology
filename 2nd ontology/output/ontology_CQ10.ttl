@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://www.example.org/test#> a owl:Ontology .

:VehicleSystem a owl:Class ;
    rdfs:label "VehicleSystem" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :adaptsToDetected ;
        owl:someValuesFrom :InattentionState
    ] .

:InattentionState a owl:Class ;
    rdfs:label "InattentionState" ;
    rdfs:subClassOf :CognitiveState .

:CognitiveState a owl:Class ;
    rdfs:label "CognitiveState" .

:DrowsinessState a owl:Class ;
    rdfs:label "DrowsinessState" ;
    rdfs:subClassOf :InattentionState .

:FatigueState a owl:Class ;
    rdfs:label "FatigueState" ;
    rdfs:subClassOf :InattentionState .

:EnvironmentalCondition a owl:Class ;
    rdfs:label "EnvironmentalCondition" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :influences ;
        owl:someValuesFrom :VehicleSystem
    ] .

:influences a owl:ObjectProperty ;
    rdfs:label "influences" ;
    rdfs:domain :EnvironmentalCondition ;
    rdfs:range :VehicleSystem .

:adaptsToDetected a owl:ObjectProperty ;
    rdfs:label "adaptsToDetected" ;
    rdfs:domain :VehicleSystem ;
    rdfs:range :InattentionState .